{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT D.DEPT_ID,\n                 D.DEPT_CODE || '/' || D.DEPT_NAME AS CODENAME,\n                 D.DEPT_NAME,\n                 P.POST_ID,\n                 P.POST_CODE,\n                 P.POST_NAME,\n                 P.ORG_ID,\n                 P.ENABLE_FLAG,\n                 P.ATTRIBUTE1,\n                 P.ATTRIBUTE4,\n                 p.ATTRIBUTE3,\n                 (select count(*) from xip_pub_emp_asg a WHERE A.POST_ID = P.POST_ID) EMP_NUM\n            FROM XIP_PUB_DEPTS D, XIP_PUB_POSTS P\n           WHERE D.DEPT_ID = P.DEPT_ID\n             AND P.ORG_ID = {?orgComobox?}\n             AND (P.POST_CODE LIKE '%{#POST_NAME#}%' OR P.POST_NAME LIKE '%{#POST_NAME#}%') \n             \n           order by P.CREATION_DATE DESC","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}