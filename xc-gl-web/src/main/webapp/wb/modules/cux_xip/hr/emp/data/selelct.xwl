{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT\n       pub.EMP_ID,\n       pub.EMP_CODE,\n       pub.EMP_NAME,\n       pub.EMP_SEX,\n       pub.ID_NO,\n       pub.PHONE_NO,\n       pub.ADDRESS,\n       pub.EMAIL,\n       pub.BIRTHDAY,\n       pub.ATTRIBUTE1,\n       pub.ATTRIBUTE2,\n       pub.ATTRIBUTE3,\n       pub.ATTRIBUTE4,\n       pub.ATTRIBUTE5,\n       pub.ATTRIBUTE6,\n       pub.ATTRIBUTE7,\n       pub.ATTRIBUTE8,\n       pub.ATTRIBUTE9,\n       pub.ATTRIBUTE10,\n       pub.CREATION_DATE,\n       pub.CREATED_BY,\n       pub.LAST_UPDATE_DATE,\n       pub.LAST_UPDATED_BY,\n       pub.LAST_UPDATE_LOGIN,\n\n       ext.BANK_NO,\n       ext.BANK_NAME\n\n\n  FROM XIP_PUB_EMPS pub \nLEFT JOIN xc_ex_ext_emps ext ON pub.EMP_ID = ext.EMP_ID\n  WHERE (pub.EMP_CODE LIKE '%{#ygbm#}%' or pub.EMP_NAME like '%{#ygbm#}%')\norder by pub.emp_code desc \n","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}