{"inframe":false,"title":"账簿组织查询","hidden":true,"roles":{"default":1},"children":[{"configs":{"itemId":"module"},"children":[{"configs":{"sql":"SELECT T.LEDGER_ID,\n       T.LEDGER_CODE, \n       T.LEDGER_NAME,\n       T.START_PERIOD_CODE,\n       T.ORG_ID,\n       (select o.ORG_NAME from xip_pub_orgs o where o.ORG_ID = T.ORG_ID) ORG_NAME,\n       (select o.ORG_CODE from xip_pub_orgs o where o.ORG_ID = T.ORG_ID) ORG_CODE,\n       T.EX_HRCY_ID, \n       T.BG_IS_CHK,\n       T.ACC_HRCY_ID,\n       T.CURRENCY_CODE\n  FROM XC_GL_LEDGERS T\n WHERE T.IS_VALID = 'Y'  \n   AND  (T.LEDGER_NAME LIKE {?ledgerName?} OR T.LEDGER_CODE LIKE {?ledgerName?})\n   AND EXISTS (SELECT 1\n          FROM XC_GL_LD_SEC A\n         WHERE T.LEDGER_ID = A.LEDGER_ID\n           AND A.ROLE_ID = {?XIP.roleId?}\n           AND A.USER_ID = {?XIP.userId?})\nORDER BY T.LEDGER_CODE\n","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}