{"inframe":false,"title":"","hidden":true,"roles":{"default":1},"children":[{"configs":{"itemId":"module"},"children":[{"configs":{"sql":"SELECT\n\tt.*, a.ORG_NAME,\n(select p.TAB_NAME from xc_rep_tab p where p.TAB_ID = t.TAB_ID) AS TAB_NAME,\n\t(\n\t\tSELECT\n\t\t\tp.LEDGER_NAME\n\t\tFROM\n\t\t\txc_gl_ledgers p\n\t\tWHERE\n\t\t\tp.LEDGER_ID = t.LEDGER_ID\n\t) AS LEDGER_NAME,\n\t(\n\t\tSELECT\n\t\t\tc.CURRENCY_NAME\n\t\tFROM\n\t\t\txc_gl_currencies c\n\t\tWHERE\n\t\t\tc.CURRENCY_CODE = t.CNY_CODE\n\t) AS CNY_NAME,\n\tCASE t.APP_STATUS\nWHEN 'A' THEN\n\t'草稿'\nELSE\n\t'锁定'\nEND AS APP_STATUS_NAME\nFROM\n\txc_rep_sheets t\nLEFT JOIN xip_pub_orgs a ON t.ORG_ID = a.ORG_ID\nWHERE\n    t.PERIOD_CODE >= {?sPeriod?}\nAND \n    t.PERIOD_CODE <= {?ePeriod?}\nAND \n    t.LEDGER_ID IN ({#ledgerIds#})\n    {#WHERESQL#}","itemId":"selectReprotData"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}