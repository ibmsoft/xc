{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"true","itemId":"module"},"children":[{"configs":{"itemId":"select","oracle":"SELECT\n    a.*, b.DEPT_ID,\n    (SELECT dept_name FROM xip_pub_depts c WHERE c.dept_id=b.DEPT_ID) DEPT_NAME\nFROM\n    xc_pm_projects a\nLEFT JOIN xc_pm_prj_dept b ON a.PROJECT_ID = b.PROJECT_ID AND a.LEDGER_ID = b.LEDGER_ID\nWHERE \n    a.LEDGER_ID ={?QUERY_LEDGER_ID?}\nAND decode(b.DEPT_ID,'%%') LIKE '%{#QUERY_DEPT_ID#}%'\nAND (SELECT COUNT(*) FROM xc_pm_projects d WHERE d.UP_PROJECT_ID=a.PROJECT_ID)=0\nAND (PROJECT_CODE like '%{#QUERY_CODE_NAME#}%' OR PROJECT_NAME like '%{#QUERY_CODE_NAME#}%')\nAND NOT exists (SELECT 1 FROM XC_PM_PROJECTS xp where a.PROJECT_ID = xp.UP_PROJECT_ID)\nAND SYSDATE BETWEEN a.START_DATE AND a.END_DATE\nORDER BY PROJECT_CODE","mysql":"SELECT\n    a.*, b.DEPT_ID,\n    (SELECT dept_name FROM xip_pub_depts c WHERE c.dept_id=b.DEPT_ID) DEPT_NAME\nFROM\n    xc_pm_projects a\nLEFT JOIN xc_pm_prj_dept b ON a.PROJECT_ID = b.PROJECT_ID AND a.LEDGER_ID = b.LEDGER_ID\nWHERE \n    a.LEDGER_ID ={?QUERY_LEDGER_ID?}\nAND IFNULL(b.DEPT_ID,'%%') LIKE '%{#QUERY_DEPT_ID#}%'\nAND (SELECT COUNT(*) FROM xc_pm_projects d WHERE d.UP_PROJECT_ID=a.PROJECT_ID)=0\nAND (PROJECT_CODE like '%{#QUERY_CODE_NAME#}%' OR PROJECT_NAME like '%{#QUERY_CODE_NAME#}%')\nAND NOT exists (SELECT 1 FROM XC_PM_PROJECTS xp where a.PROJECT_ID = xp.UP_PROJECT_ID)\nAND CURDATE() BETWEEN a.START_DATE AND a.END_DATE\nORDER BY PROJECT_CODE"},"children":[],"expanded":false,"type":"sqlswitcher"},{"configs":{"autoPage":"false","sql":"{#select#}","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}