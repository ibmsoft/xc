{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT\n\ta.MATERIAL_ID,\n\ta.MATERIAL_CODE,\n\ta.MATERIAL_NAME,\n\ta.MATERIAL_PRO,\n\ta.MATERIAL_SET_ID,\n\ta.MATERIAL_TYPE_ID,\n\ta.FINACE_TYPE_ID,\n\ta.SPECIFICATION,\n\ta.DIM_ID,\n\ta.IS_PURCHASE,\n\ta.IS_PURCHASE_EXCESS,\n\tb.PURCHASE_PRICE,\n\tc.MATERIAL_TYPE_NAME,\n\td.DIM_NAME,\n\te.VAL_CODE AS 'MATERIAL_PRO_CODE',\n\te.VAL_NAME AS 'MATERIAL_PRO_NAME'\nFROM\n\txc_st_material a\nLEFT JOIN xc_st_material_parms b ON a.MATERIAL_ID = b.MATERIAL_ID\nLEFT JOIN xc_st_material_type c ON a.MATERIAL_TYPE_ID = c.MATERIAL_TYPE_ID\nLEFT JOIN xc_gl_dimensions d ON a.DIM_ID = d.DIM_ID\nLEFT JOIN xip_pub_val_set_dtl e ON a.MATERIAL_PRO = e.VAL_CODE\nWHERE\n\tb.LEDGER_ID = {?ledgerId?}\nAND b.IS_PURCHASE = 'Y'\nAND e.VAL_SET_CODE = 'XC_ST_MATERIAL_PROPERTY'\nAND ( a.MATERIAL_NAME LIKE '%{#name#}%' OR a.MATERIAL_CODE LIKE '%{#name#}%' )","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}