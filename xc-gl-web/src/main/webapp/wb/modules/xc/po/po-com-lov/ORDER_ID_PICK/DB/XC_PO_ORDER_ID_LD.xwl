{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT\n\ta.*, b.AP_CONTRACT_CODE,\n\tb.AP_CONTRACT_NAME,\n\tc.DEPT_NAME,\n\td.VENDOR_NAME,\n\te.PROJECT_NAME\nFROM\n\txc_po_order_h a\nLEFT JOIN xc_ap_contract b ON a.CONTRACT_ID = b.AP_CONTRACT_ID\nLEFT JOIN xip_pub_depts c ON a.DEPT_ID = c.DEPT_ID\nLEFT JOIN xc_ap_vendors d ON a.VENDOR_ID = d.VENDOR_ID\nLEFT JOIN xc_pm_projects e ON a.PROJECT_ID = e.PROJECT_ID\nWHERE\n\ta.LEDGER_ID = {?QUERY_LEDGER_ID?}\nAND ( a.ORDER_H_CODE LIKE '%{#QUERY_CODE_NAME#}%' OR a.ORDER_H_NAME LIKE '%{#QUERY_CODE_NAME#}%' OR d.VENDOR_NAME LIKE '%{#QUERY_CODE_NAME#}%' )\nAND a.ORDER_H_TYPE = 'ORDER_WZ'\nAND a.SYS_AUDIT_STATUS = 'E'\nORDER BY a.ORDER_H_CODE DESC","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":"seek_icon"}