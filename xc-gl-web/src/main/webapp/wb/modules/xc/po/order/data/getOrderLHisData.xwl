{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT\n\ta.*,\n\tb.MATERIAL_CODE,\n\tb.MATERIAL_NAME,\n( SELECT m.PUR_AP_H_CODE FROM xc_po_pur_ap_h m WHERE m.PUR_AP_H_ID = c.PUR_AP_H_ID ) AS 'PUR_AP_H_CODE',\n\td.DIM_NAME,\n\tc.PUR_AP_L_ID,\n\te.DEPT_NAME,\n\tf.PROJECT_NAME,\n    (\n      EXISTS(SELECT 1 FROM xc_po_arrival_l arrival_l WHERE arrival_l.ORDER_L_ID = a.ORDER_L_ID)\n      ||  \n      EXISTS(SELECT 1 FROM xc_po_return_l return_l WHERE return_l.ORDER_L_ID = a.ORDER_L_ID)\n     ) AS 'IS_USED'\nFROM\n\txc_po_order_l_his a\nLEFT JOIN xc_st_material b ON a.MATERIAL_ID = b.MATERIAL_ID\nLEFT JOIN xc_po_pur_ap_l c ON c.PUR_AP_L_ID = a.PUR_AP_L_ID\nLEFT JOIN xc_gl_dimensions d ON a.DIM_ID = d.DIM_ID\nLEFT JOIN xip_pub_depts e ON a.DEPT_ID = e.DEPT_ID\nLEFT JOIN xc_pm_projects f ON a.PROJECT_ID = f.PROJECT_ID\nWHERE\n\ta.ORDER_H_HIS_ID = {?ORDER_H_HIS_ID?}","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}