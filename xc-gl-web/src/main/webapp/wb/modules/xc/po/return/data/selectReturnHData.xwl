{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"select \na.*,\nc.ORDER_H_CODE,\nd.AP_CONTRACT_CODE,\n(select EMP_NAME from xip_pub_emps where EMP_ID in (select EMP_ID from xip_pub_users where USER_ID = a.PURCHASE_PERSON)) as 'PURCHASE_PERSON_NAME',\ne.WAREHOUSE_NAME,\nf.VENDOR_NAME,\nh.DEPT_NAME,\ni.PROJECT_NAME\nfrom xc_po_return_h a\nleft join xc_po_order_h c\non \na.ORDER_H_ID = c.ORDER_H_ID\nleft join xc_ap_contract d\non\na.CONTRACT_ID = d.AP_CONTRACT_ID\nleft join xc_st_warehouse e\non\na.WAREHOUSE_ID = e.WAREHOUSE_ID\nleft join xc_ap_vendors f\non\na.VENDOR_ID = f.VENDOR_ID\nleft join xip_pub_depts h\non\na.DEPT_ID = h.DEPT_ID\nleft join xc_pm_projects i\non\na.PROJECT_ID = i.PROJECT_ID\nwhere\na.ORG_ID = {?ORG_ID?}\nand\na.VENDOR_ID like '%{#VENDOR_ID#}%'\nand\nc.ORDER_H_CODE like '%{#ORDER_H_CODE#}%'\nand\na.AUDIT_STATUS  like '%{#AUDIT_STATUS#}%'\nand\na.SYS_AUDIT_STATUS  like '%{#SYS_AUDIT_STATUS#}%'\nand\nif({?startDate?} is null,1=1,a.DOCUMENT_DATE>={?startDate?})\nand\nif({?endDate?} is null,1=1,a.DOCUMENT_DATE<={?endDate?})\nand\n(ifnull(d.AP_CONTRACT_CODE,'') like '%{#AP_CONTRACT_CODE#}%' or ifnull(d.AP_CONTRACT_NAME,'') like '%{#AP_CONTRACT_CODE#}%')\nand\na.WAREHOUSE_ID like '%{#WAREHOUSE_ID#}%'\nand\na.DEPT_ID like '%{#DEPT_ID#}%'\n\nORDER BY a.RETURN_H_CODE DESC\n","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}