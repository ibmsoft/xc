{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT\n\ta.*, b.AP_CONTRACT_CODE,\n\tb.AP_CONTRACT_NAME,\n\tc.VENDOR_CODE,\n\tc.VENDOR_NAME,\n\tf.ORG_NAME,\n( SELECT DEPT_NAME FROM xip_pub_depts m WHERE a.PURCHASE_DEPT_ID = m.DEPT_ID ) AS 'PURCHASE_DEPT_NAME',\n( SELECT DEPT_NAME FROM xip_pub_depts m WHERE a.DEPT_ID = m.DEPT_ID ) AS 'DEPT_NAME',\n( SELECT EMP_NAME FROM xip_pub_emps WHERE EMP_ID IN ( SELECT EMP_ID FROM xip_pub_users WHERE USER_ID = a.PURCHASE_PERSON )) AS 'PURCHASE_PERSON_NAME',\n\tg.VAL_NAME,\n\th.PROJECT_NAME,\n( SELECT VAL_NAME FROM xip_pub_val_set_dtl s WHERE s.VAL_CODE = a.ORDER_H_TYPE AND s.VAL_SET_CODE = 'XC_PO_ORDER_TYPE' ) AS 'ORDER_H_TYPE_NAME'\nFROM\n\txc_po_order_h a\nLEFT JOIN xc_ap_contract b ON a.CONTRACT_ID = b.AP_CONTRACT_ID\nLEFT JOIN xc_ap_vendors c ON a.VENDOR_ID = c.VENDOR_ID\nLEFT JOIN xip_pub_orgs f ON a.ORG_ID = f.ORG_ID\nLEFT JOIN xip_pub_val_set_dtl g ON a.SYS_AUDIT_STATUS = g.VAL_CODE\nLEFT JOIN xc_pm_projects h ON a.PROJECT_ID = h.PROJECT_ID\nWHERE\n\tg.VAL_SET_CODE = 'XC_AP_CONTRACT_SYS_AUDIT_STATUS'\nAND a.ORG_ID = {?ORG_ID?}\nAND a.VENDOR_ID LIKE '%{#VENDOR_ID#}%'\nAND ( a.ORDER_H_CODE LIKE '%{#ORDER_H_CODE#}%' OR a.ORDER_H_NAME LIKE '%{#ORDER_H_CODE#}%' )\nAND a.AUDIT_STATUS LIKE '%{#AUDIT_STATUS#}%'\nAND a.SYS_AUDIT_STATUS LIKE '%{#SYS_AUDIT_STATUS#}%'\nAND (a.PURCHASE_DEPT_ID LIKE '%{#PURCHASE_DEPT_ID#}%' OR a.PURCHASE_DEPT_ID is null)\nAND (a.DEPT_ID LIKE '%{#DEPT_ID#}%' OR a.DEPT_ID is null)\nAND (a.PROJECT_ID LIKE '%{#PROJECT_ID#}%' OR a.PROJECT_ID is null)\nAND a.IS_CLOSE LIKE '%{#IS_CLOSE#}%'\nAND a.ORDER_H_TYPE LIKE '%{#ORDER_H_TYPE#}%'\nAND\nIF ( '{#startDate#}' = '', 1 = 1, a.DOCUMENT_DATE >= '{#startDate#}' )\nAND\nIF ( '{#endDate#}' = '', 1 = 1, a.DOCUMENT_DATE <= '{#endDate#}' )\nAND ( ifnull(b.AP_CONTRACT_CODE, '') LIKE '%{#CONTRACT_CODE#}%' OR ifnull(b.AP_CONTRACT_NAME, '') LIKE '%{#CONTRACT_CODE#}%' )\nORDER BY\n\ta.ORDER_H_CODE DESC","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}