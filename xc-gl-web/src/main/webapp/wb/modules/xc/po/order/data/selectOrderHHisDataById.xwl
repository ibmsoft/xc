{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT\n\ta.*, \n\tdate_format(a.DOCUMENT_DATE, '%y-%m-%d') AS 'documentDate',\n\tb.AP_CONTRACT_CODE,\n\tb.AP_CONTRACT_NAME,\n\tc.VENDOR_CODE,\n\tc.VENDOR_NAME,\n\td.DEPT_NAME,\n\tf.ORG_NAME,\n( SELECT DEPT_NAME FROM xip_pub_depts m WHERE a.PURCHASE_DEPT_ID = m.DEPT_ID ) AS 'PURCHASE_DEPT_NAME',\n( SELECT EMP_NAME FROM xip_pub_emps WHERE EMP_ID IN ( SELECT EMP_ID FROM xip_pub_users WHERE USER_ID = a.CREATED_BY )) AS 'PURCHASE_PERSON_NAME',\n\te.PROJECT_NAME,\n\tg.VAL_NAME,\n( SELECT VAL_NAME FROM xip_pub_val_set_dtl s WHERE s.VAL_CODE = a.ORDER_H_TYPE AND s.VAL_SET_CODE = 'XC_PO_ORDER_TYPE' ) AS 'ORDER_H_TYPE_NAME'\nFROM\n\txc_po_order_h_his a\nLEFT JOIN xc_ap_contract b ON a.CONTRACT_ID = b.AP_CONTRACT_ID\nAND ( b.AP_CONTRACT_CODE LIKE '%{#CONTRACT_CODE#}%' OR b.AP_CONTRACT_NAME LIKE '%{#CONTRACT_CODE#}%' )\nLEFT JOIN xc_ap_vendors c ON a.VENDOR_ID = c.VENDOR_ID\nLEFT JOIN xip_pub_depts d ON a.DEPT_ID = d.DEPT_ID\nLEFT JOIN xc_pm_projects e ON a.PROJECT_ID = e.PROJECT_ID\nLEFT JOIN xip_pub_orgs f ON a.ORG_ID = f.ORG_ID\nLEFT JOIN xip_pub_val_set_dtl g ON a.SYS_AUDIT_STATUS = g.VAL_CODE\nAND g.VAL_SET_CODE = 'XC_AP_CONTRACT_SYS_AUDIT_STATUS'\nWHERE\n\ta.ORDER_H_ID = {?orderHId?}\nORDER BY\n\ta.version DESC\n","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}