{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT\n\ta.*, b.ORDER_H_CODE,\n\tc.AP_CONTRACT_NAME,\n\tc.AP_CONTRACT_CODE,\n\td.VENDOR_NAME,\n( SELECT EMP_NAME FROM xip_pub_emps WHERE EMP_ID IN ( SELECT EMP_ID FROM xip_pub_users WHERE USER_ID = a.PURCHASE_PERSON )) AS 'PURCHASE_PERSON_NAME',\n\te.WAREHOUSE_NAME,\n\tdate_format(a.DOCUMENT_DATE, '%y-%m-%d') AS 'doucmentDate',\n\tf.ORG_NAME,\n\tg.DEPT_NAME,\n\th.PROJECT_NAME\nFROM\n\txc_po_arrival_h a\nLEFT JOIN xc_po_order_h b ON a.ORDER_H_ID = b.ORDER_H_ID\nLEFT JOIN xc_ap_contract c ON a.CONTRACT_ID = c.AP_CONTRACT_ID\nLEFT JOIN xc_ap_vendors d ON a.VENDOR_ID = d.VENDOR_ID\nLEFT JOIN xc_st_warehouse e ON a.WAREHOUSE_ID = e.WAREHOUSE_ID\nLEFT JOIN xip_pub_orgs f ON a.ORG_ID = f.ORG_ID\nLEFT JOIN xip_pub_depts g ON a.DEPT_ID = g.DEPT_ID\nLEFT JOIN xc_pm_projects h ON a.PROJECT_ID = h.PROJECT_ID\nWHERE\n\ta.ARRIVAL_H_ID = {?ARRIVAL_H_ID?}","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}