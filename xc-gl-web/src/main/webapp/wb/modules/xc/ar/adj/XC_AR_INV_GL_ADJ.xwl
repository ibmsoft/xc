C_AR_INV_GL_ADJ_CHECK',\n  params: {\n    AR_INV_GL_H_ID: app.AR_INV_GL_H_ID_XWL.AR_INV_GL_H_ID.getValue()\n  },\n  success: function(resp) {\n    var o = Wb.decode(resp.responseText);\n    if (Wb.isEmpty(o.NO_PAY_AMT)) {\n      o.NO_PAY_AMT = 0;\n    }\n    if (Number(o.NO_PAY_AMT) + Number(app.ADJ_AMT.getValue()) < 0) {\n      Wb.toast('{#saveMsg2#}');\n\n    } else {\n      Wb.request({\n      \turl:'{#contextPath#}'+'/arInvGlAdjAction.do?method=saveInvAdj',\n        params:{'params':Wb.getValue(app.form1)},\n        success:function(resp){\n          var rec = Wb.decode(resp.responseText);\n          if(rec.flag==0){\n             saveVoucher(paramJson);\n          }\n        }\n      });\n    }\n  }\n});\n\nfunction saveVoucher(paramJson) {\n  if (app.V_STATUS_NAME.getValue() === '{#draft#}' || app.V_STATUS_NAME.getValue() === '' || app.V_STATUS_NAME.getValue() === null) {\n    Wb.request({\n      url: '{#contextPath#}' + '/arInvGlAdjAction.do?method=createVoucher', //后台java代码生成凭证\n      method: 'POST',\n      async: false, //异步传参\n      params: {\n        'params': Wb.encode(paramJson)\n      },\n      success: function(resp) {\n        var record = Wb.decode(resp.responseText);\n        var headId = record.headId;\n        var msg = record.msg;\n        if (record.flag == '0') {\n          Wb.toast('{#saveMsg3#}');\n          app.V_HEAD_ID.setValue(headId);\n          app.V_STATUS_NAME.setValue('{#draft#}');\n        }\n        if (record.flag == '1') {\n          Wb.toast(msg);\n        }\n      }\n    });\n  } else {\n    Wb.toast('{#saveMsg4#}');\n  }\n  app.LEDGER_ID_XWL.LEDGER_NAME.setReadOnly(true);\n}"},"children":[],"expanded":true,"type":"button"},{"configs":{"text":"{#verify#}","itemId":"verfyBtn"},"events":{"click":"var headId = app.V_HEAD_ID.getValue();\nif(headId === '' || headId === null){\n    Wb.toast('{#verifyMsg1#}');\n    return;\n}\nif(app.V_STATUS_NAME.getValue() !== '{#draft#}'){\n    Wb.toast('{#verifyMsg2#}');\n    return;\n}\nelse{\n 