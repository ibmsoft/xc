{"inframe":false,"title":"项目查询","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"oracle":"SELECT\n\tp.PROJECT_ID,\n/* \tp.PROJECT_NAME, */\n\tCONCAT((SELECT d.DEPT_NAME FROM xip_pub_depts d where d.DEPT_ID = ppd.DEPT_ID),'/',p.PROJECT_NAME) PROJECT_NAME,\n\tp.PROJECT_CODE,\n\tppd.DEPT_ID\nFROM\n\txc_pm_projects p\nLEFT JOIN xc_pm_prj_dept ppd ON p.PROJECT_ID = ppd.PROJECT_ID\nWHERE\n\tp.LEDGER_ID = {?LEDGER_ID?}\nAND ppd.DEPT_ID LIKE '%{#DEPT_ID#}%'\nAND SYSDATE BETWEEN p.START_DATE AND p.END_DATE\nAND NOT EXISTS(SELECT a.PROJECT_ID FROM xc_pm_projects a WHERE a.UP_PROJECT_ID = p.PROJECT_ID)","itemId":"sqlswitcher1","mysql":"SELECT\n\tp.PROJECT_ID,\n\tp.PROJECT_NAME,\n\t(SELECT d.DEPT_NAME FROM xip_pub_depts d where d.DEPT_ID = ppd.DEPT_ID) DEPT_NAME,\n\tp.PROJECT_CODE,\n\tppd.DEPT_ID\nFROM\n\txc_pm_projects p\nLEFT JOIN xc_pm_prj_dept ppd ON p.PROJECT_ID = ppd.PROJECT_ID\nWHERE\n\tp.LEDGER_ID = {?LEDGER_ID?}\nAND ppd.DEPT_ID LIKE '%{#DEPT_ID#}%'\nAND CURDATE() BETWEEN p.START_DATE AND p.END_DATE\nAND NOT EXISTS(SELECT a.PROJECT_ID FROM xc_pm_projects a WHERE a.UP_PROJECT_ID = p.PROJECT_ID)\nAND (p.PROJECT_CODE LIKE '%{#PROJECT_INFO#}%' OR p.PROJECT_NAME LIKE '%{#PROJECT_INFO#}%')"},"children":[],"expanded":false,"type":"sqlswitcher"},{"configs":{"sql":"{#sqlswitcher1#}","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":"seek_icon"}