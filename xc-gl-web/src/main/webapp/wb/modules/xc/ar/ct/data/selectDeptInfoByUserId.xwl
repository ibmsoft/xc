{"inframe":false,"title":"根据当前登录人ID得到所属部门","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT\n\tpe.EMP_ID,\n\tpea.DEPT_ID,\n\tpd.DEPT_NAME\nFROM\n\txip_pub_emp_asg pea\nLEFT JOIN xip_pub_emps pe ON pe.EMP_ID = pea.EMP_ID\nLEFT JOIN xip_pub_users pu ON pe.EMP_ID = pu.EMP_ID\nLEFT JOIN xip_pub_depts pd ON pd.DEPT_ID = pea.DEPT_ID\nWHERE\n\tpu.USER_ID = {?USER_ID?}\nAND pea.ORG_ID = {?ORG_ID?}","type":"object","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}