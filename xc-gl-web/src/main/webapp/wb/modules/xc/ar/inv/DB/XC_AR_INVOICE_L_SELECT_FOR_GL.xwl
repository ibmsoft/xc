{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"autoPage":"false","sql":"SELECT \n  a.*,\n(SELECT \n   c.AR_ACC_ID_INV\nFROM\n  xc_ar_ld_sale_type c,\n  xc_ar_sale_type d \nWHERE c.AR_SALE_TYPE_ID = d.AR_SALE_TYPE_ID  AND d.IS_SYSTEM_AR = '1'\n  AND c.LEDGER_ID = h.LEDGER_ID AND c.AR_SALE_TYPE_ID=a.AR_SALE_TYPE_ID) AR_ACC_ID_INV,\n(SELECT \n   (SELECT i.ACC_NAME FROM xc_gl_accounts i WHERE i.ACC_ID=c.AR_ACC_ID_INV)\nFROM\n  xc_ar_ld_sale_type c,\n  xc_ar_sale_type d \nWHERE c.AR_SALE_TYPE_ID = d.AR_SALE_TYPE_ID  AND d.IS_SYSTEM_AR = '1'\n  AND c.LEDGER_ID = h.LEDGER_ID AND c.AR_SALE_TYPE_ID=a.AR_SALE_TYPE_ID) AR_ACC_ID_INV_NAME,\n  (SELECT AR_SALE_TYPE_NAME FROM xc_ar_sale_type b WHERE b.AR_SALE_TYPE_ID=a.AR_SALE_TYPE_ID) AR_SALE_TYPE_NAME,\n  (SELECT AR_SALE_TYPE_ID FROM xc_ar_sale_type b WHERE b.AR_SALE_TYPE_ID=a.AR_SALE_TYPE_ID) ARPUR_TYPE_ID,\n  (SELECT PRODUCT_NAME FROM xc_gl_products b WHERE b.PRODUCT_ID=a.PRODUCT_ID) PRODUCT_NAME,\n  (SELECT DIM_NAME FROM xc_gl_dimensions b WHERE b.DIM_CODE=a.DIM_CODE) DIM_NAME,\n  (SELECT EMP_NAME FROM XIP_PUB_EMPS WHERE EMP_ID=(SELECT EMP_ID FROM XIP_PUB_USERS b WHERE b.USER_ID=a.CREATED_BY)) CREATED_BY_NAME\nFROM\n  xc_ar_invoice_l a,xc_ar_invoice_h h \n\twhere a.AR_INV_H_ID=h.AR_INV_H_ID and a.AR_INV_H_ID = {?AR_INV_H_ID?}","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}