{"inframe":false,"title":"销售类型查询","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT\n\tarst.AR_SALE_TYPE_ID,\n\tarst.AR_SALE_TYPE_NAME,\n\talst.BG_ITEM_ID,\n\tbi.BG_ITEM_NAME,\n\talst.AR_ACC_ID_INV,\n\tga.ACC_NAME,\n\talst.PRODUCT_ID,\n\tgp.PRODUCT_NAME\nFROM\n\txc_ar_sale_type arst\nLEFT JOIN xc_ar_ld_sale_type alst ON arst.AR_SALE_TYPE_ID = alst.AR_SALE_TYPE_ID\nLEFT JOIN xc_bg_items bi ON alst.BG_ITEM_ID = bi.BG_ITEM_ID\nLEFT JOIN xc_gl_products gp ON alst.PRODUCT_ID = gp.PRODUCT_ID\nLEFT JOIN xc_gl_accounts ga ON alst.AR_ACC_ID_INV = ga.ACC_ID\nWHERE\n\talst.LEDGER_ID = {?LEDGER_ID?}\nAND arst.IS_SYSTEM_AR = '1'\nAND arst.AR_SALE_TYPE_NAME LIKE '%{#AR_SALE_TYPE_NAME#}%'","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":"seek_icon"}