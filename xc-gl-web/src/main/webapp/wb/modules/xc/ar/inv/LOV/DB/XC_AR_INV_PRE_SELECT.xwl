{"inframe":false,"title":"查询销售发票核销预收表","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT \n  a.*,\n  b.AR_PAY_H_CODE,\n  b.GL_DATE,\n  b.DESCRIPTION,\n  (SELECT LEDGER_NAME FROM xc_gl_ledgers c WHERE c.LEDGER_ID = b.LEDGER_ID) LEDGER_NAME,\n  (SELECT CUSTOMER_NAME FROM xc_ar_customers c WHERE c.CUSTOMER_ID = b.CUSTOMER_ID) CUSTOMER_NAME,\n  (SELECT PROJECT_NAME FROM xc_pm_projects c WHERE c.PROJECT_ID = b.PROJECT_ID) PROJECT_NAME,\n  (SELECT DEPT_NAME FROM xip_pub_depts c WHERE c.DEPT_ID = b.DEPT_ID) DEPT_NAME,\n  (SELECT AR_CONTRACT_NAME FROM xc_ar_contract c WHERE c.AR_CONTRACT_ID = b.AR_CONTRACT_ID) AR_CONTRACT_NAME,\n  b.AMOUNT,\n  ((SELECT SUM(TARGET_AMT) FROM xc_ar_cancel_l c WHERE c.TARGET_ID = b.AR_PAY_H_ID)+\n   (SELECT SUM(CANCEL_AMT) FROM xc_ar_inv_pre c WHERE c.AR_PAY_H_ID = b.AR_PAY_H_ID)) TARGET_AMT,\n  (b.AMOUNT-(SELECT IFNULL(SUM(TARGET_AMT),0) FROM xc_ar_cancel_l c WHERE c.TARGET_ID = b.AR_PAY_H_ID)-\n   (SELECT IFNULL(SUM(CANCEL_AMT),0) FROM xc_ar_inv_pre c WHERE c.AR_PAY_H_ID = b.AR_PAY_H_ID)) NO_CANCEL_AMT\nFROM\n  xc_ar_inv_pre a,\n  xc_ar_pay_h b\nWHERE a.AR_PAY_H_ID = b.AR_PAY_H_ID\nAND a.AR_INV_H_ID = {?AR_INV_H_ID?}\nORDER BY b.AR_PAY_H_CODE","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}