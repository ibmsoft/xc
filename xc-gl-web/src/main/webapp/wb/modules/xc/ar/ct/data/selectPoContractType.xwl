{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","serverScript":"var lang = request.getSession().getAttribute(\"XzSessionVars\").get('language');\nrequest.setAttribute(\"language\",lang);","itemId":"module"},"children":[{"configs":{"sql":"SELECT\n\td.VAL_CODE AS \"CONTRACT_TYPE_ID\",\n\tCASE\n      WHEN l.VAL_NAME IS NULL OR l.VAL_NAME = ''\n\t\tTHEN\n          d.VAL_NAME\n\t\tELSE\n          l.VAL_NAME\n      END AS \"CONTRACT_TYPE_NAME\"\nFROM\n\txip_pub_val_set_dtl d\nLEFT JOIN XIP_PUB_VAL_SET_DTL_LANG l ON d.VAL_SET_CODE = l.VAL_SET_CODE\nAND d.VAL_CODE = l.VAL_CODE\nAND l.VAL_SET_LANG = '{#language#}'\nWHERE\n\td.VAL_SET_CODE = 'XC_PO_CONTRACT_TYPE'\nAND ENABLE_FLAG = 'Y'\nORDER BY d.VAL_CODE","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}