{"inframe":false,"title":"合同查询","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"oracle":"SELECT\n\ta.*,\n\tb.CUSTOMER_NAME AS CUSTOMER_NAME\nFROM\n\txc_ar_contract a\nLEFT JOIN xc_ar_customers b ON a.CUSTOMER_ID = b.CUSTOMER_ID\nLEFT JOIN xc_gl_ld_customers c ON a.CUSTOMER_ID = c.CUSTOMER_ID AND a.LEDGER_ID = c.LEDGER_ID AND b.CUSTOMER_ID = a.CUSTOMER_ID\nWHERE\n\ta.LEDGER_ID = {?QUERY_LEDGER_ID?}\nAND a.IS_CLOSE = 'N'\nAND a.CUSTOMER_ID LIKE '%{#QUERY_CUSTOMER_ID#}%'\nAND (a.AR_CONTRACT_CODE LIKE '%{#QUERY_CODE_NAME#}%' OR a.AR_CONTRACT_NAME LIKE '%{#QUERY_CODE_NAME#}%')\nAND c.IS_ENABLED = '1'\nAND a.SYS_AUDIT_STATUS = 'E'\n/* AND SYSDATE BETWEEN b.START_DATE AND b.END_DATE\nAND SYSDATE BETWEEN a.START_DATE AND a.END_DATE */\nORDER BY a.AR_CONTRACT_CODE","itemId":"sqlswitcher1","mysql":"SELECT\n\ta.*,\n\tb.CUSTOMER_NAME AS CUSTOMER_NAME\nFROM\n\txc_ar_contract a\nLEFT JOIN xc_ar_customers b ON a.CUSTOMER_ID = b.CUSTOMER_ID\nLEFT JOIN xc_gl_ld_customers c ON a.CUSTOMER_ID = c.CUSTOMER_ID AND a.LEDGER_ID = c.LEDGER_ID AND b.CUSTOMER_ID = a.CUSTOMER_ID\nWHERE\n\ta.LEDGER_ID = {?QUERY_LEDGER_ID?}\nAND a.IS_CLOSE = 'N'\nAND a.CUSTOMER_ID LIKE '%{#QUERY_CUSTOMER_ID#}%'\nAND (a.AR_CONTRACT_CODE LIKE '%{#QUERY_CODE_NAME#}%' OR a.AR_CONTRACT_NAME LIKE '%{#QUERY_CODE_NAME#}%')\nAND c.IS_ENABLED = '1'\nAND a.SYS_AUDIT_STATUS = 'E'\n/* AND CURDATE() BETWEEN b.START_DATE AND b.END_DATE\nAND CURDATE() BETWEEN a.START_DATE AND a.END_DATE */\nORDER BY a.AR_CONTRACT_CODE"},"children":[],"expanded":false,"type":"sqlswitcher"},{"configs":{"sql":"{#sqlswitcher1#}","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}