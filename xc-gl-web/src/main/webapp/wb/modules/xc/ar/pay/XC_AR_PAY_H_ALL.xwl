}else if(v_status == '3'){\n  doc_status = 'shtg';\n}else if(v_status == '4'){\n  doc_status = 'qz';\n}\n   \nWb.run({\n    url:'m?xwl=xc/ar/pay/XC_AR_PAY_H_NO_C',\n    success:function(app){\n        app.initWin({\n            AR_PAY_H_ID : ar_pay_h_id,\n            DOC_STATUS : doc_status,\n            AR_DOC_CAT_CODE : ar_doc_cat_code,\n            hideFun:function(){\n                Wb.reload(curGrid.store);\n            }\n        });\n    }\n});"},"children":[],"expanded":false,"type":"button"},{"configs":{"text":"{#deleteBtn#}","itemId":"deleteBtn","iconCls":"file_delete_icon"},"events":{"click":"var recs = app.grid1.getSelection();\nif(recs.length === 0){\n    Wb.toast('{#deleteBtnMsg1#}');\n    return ;\n}\nvar deleteRecs = [];\nfor(var i = 0 ; i < recs.length ; i++){\n    if(recs[i].data.V_STATUS == '1' || recs[i].data.V_STATUS === '' || recs[i].data.V_STATUS === null){//只有草稿状态下的单据可以删除\n        deleteRecs.push(recs[i]);\n    }\n}\nif(deleteRecs.length > 0){\n    Wb.confirm('{#deleteBtnconfirm1#} ' + recs.length + ' {#deleteBtnconfirm2#}', function() {\n        Wb.request({\n            url : '{#contextPath#}/arPayAction.do?method=deleteArPay',\n            params : {deleteRecs : Wb.encode(Wb.getData(deleteRecs))},\n            success : function(resp){\n                var obj = Wb.decode(resp.responseText);\n                Wb.toast(obj.msg);\n                app.queryBtn.fireEvent('click');\n            }\n        });\n    });\n}\nelse{\n    Wb.toast('{#deleteBtnMsg2#}');\n}"},"children":[],"expanded":false,"type":"button"},{"configs":{"text":"{#verifyBtn#}","itemId":"verifyBtn","iconCls":"user_edit_icon"},"events":{"click":"var rows = app.grid1.getSelection();\nif(rows.length === 0){\n    Wb.toast(\"{#verfyRecGL#}\");\n    return;\n}\nelse{\n    //单据ID数组\n    var docArray = [];\n    var z = 0;\n    for(var i = 0;i < rows.length;i++){\n        var headId = rows[i].data.V_HEAD_ID;//凭证头ID\n        if(headId === '' || headId === null){\n            z++;\n        }\n        if(rows[i].data.V_STATUS == '1'){\n            var arId = rows[i].data.AR_PAY_H_ID;//单据ID\n            var paramJson = {'arId' : arId,arCatCode : 'SKD'};\n            docArray.push(paramJson);\n        }\n    }\n    if(z > 0){\n        Wb.toast(\"{#verifyBtnMsg2#}\");//请选择已经生成凭证的收款单\n        return;\n    }\n    if(docArray.length === 0){\n        Wb.toast('{#verifyBtnMsg3#}');//单据已经审核通过\n        return;\n    }\n    Wb.request({\n        url: '{#contextPath#}'+'/arDocsAction.do?method=checkArVoucher',\n        params: Wb.apply({arInfo: docArray}),\n        success: function(resp) {\n            var record = Wb.decode(resp.responseText);\n            var msg = record.msg;\n            Wb.reload(app.grid1.store);\n            Wb.toast(msg);\n        }\n\t});\n}"},"children":[],"expanded":false,"type":"button"},{"configs":{"text":"{#cancelVerfyBtn#}","itemId":"cancelVerfyBtn","iconCls":"user_delete_icon"},"events":{"click":"var rows = app.grid1.getSelection();\nif(rows.length === 0){\n    Wb.toast(\"{#cancelVerifyBtnMsg1#}\");\n    return;\n}\nelse{\n    //单据ID数组\n    var docArray = [];\n    var z = 0;\n    for(var i = 0;i < rows.length;i++){\n        var headId = rows[i].data.V_HEAD_ID;//凭证头ID\n        if(headId === '' || headId === null){\n            z++;\n        }\n        if(rows[i].data.V_STATUS == '2' || rows[i].data.V_STATUS == '3'){//2-审核中 3-审核通过\n            var arId = rows[i].data.AR_PAY_H_ID;//单据ID\n            var headId = rows[i].data.V_HEAD_ID;//凭证头ID\n            var paramJson = {'arId' : arId,arCatCode : 'SKD'};\n            docArray.push(paramJson);\n        }\n    }\n    if(z > 0){\n        Wb.toast(\"{#verifyBtnMsg2#}\");//请选择已经生成凭证的收款单！\n        return;\n    }\n    if(docArray.length === 0){\n        Wb.toast(\"{#cancelVerifyBtnMsg2#}\");//请选择已经审核通过的单据\n        return;\n    }\n    Wb.request({\n        url: '{#contextPath#}' + '/arDocsAction.do?method=cancelCheckArVoucher',\n        params: Wb.apply({\n            arInfo: docArray