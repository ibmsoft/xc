{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT * FROM(\n    SELECT\n      a.*,\n      IF(a.AR_DOC_CAT_CODE <> 'YFHYS',(SELECT AR_CAT_NAME FROM xc_ar_doc_cats WHERE AR_CAT_CODE = a.AR_DOC_CAT_CODE),'应付核应收') AS AR_DOC_CAT_NAME,\n      SUM(a.DR_AMT) AS JFJE,\n      SUM(a.CR_AMT) AS DFJE,\n      \"\" AS 'INV_AMOUNT'\n    FROM\n        xc_ar_inv_trans a\n    WHERE (a.SOURCE_ID = {?SOURCE_ID?} OR a.AR_INV_GL_H_ID = {?AR_INV_GL_H_ID?})\n    AND a.AR_DOC_CAT_CODE IN (\"YSHYF\",\"YSHLDC\")\n    GROUP BY a.SOURCE_ID\n\n    UNION\n\n    SELECT\n      a.*,\n      IF(a.AR_DOC_CAT_CODE <> 'YFHYS',(SELECT AR_CAT_NAME FROM xc_ar_doc_cats WHERE AR_CAT_CODE = a.AR_DOC_CAT_CODE),'应付核应收') AS AR_DOC_CAT_NAME,\n      a.DR_AMT AS JFJE,\n      a.CR_AMT AS DFJE,\n      \"\" AS 'INV_AMOUNT'\n    FROM\n        xc_ar_inv_trans a\n    WHERE (a.SOURCE_ID = {?SOURCE_ID?} OR a.AR_INV_GL_H_ID = {?AR_INV_GL_H_ID?})\n    AND a.AR_DOC_CAT_CODE NOT IN (\"YSHYF\",\"YSHLDC\")\n) t\nORDER BY t.CREATION_DATE","itemId":"selectInfo"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}