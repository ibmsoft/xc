{"inframe":false,"title":"查询收款单单据类型","hidden":false,"roles":{"default":1},"children":[{"configs":{"serverScript":"//获取多语言的类别\nvar lang = request.getSession().getAttribute(\"XzSessionVars\").get('language');\nrequest.setAttribute(\"language\",lang);","itemId":"module"},"children":[{"configs":{"sql":"SELECT\n\tdtl.VAL_CODE,\n  CASE \n    WHEN lang.VAL_NAME IS NULL OR lang.VAL_NAME = '' THEN dtl.VAL_NAME\n    ELSE lang.VAL_NAME\n  END AS VAL_NAME\nFROM\n\txip_pub_val_set_dtl dtl\nLEFT JOIN xip_pub_val_set_dtl_lang lang ON (dtl.VAL_CODE = lang.VAL_CODE AND dtl.VAL_SET_CODE = lang.VAL_SET_CODE AND lang.VAL_SET_LANG = '{#language#}')\nWHERE \n\tdtl.VAL_SET_CODE = 'XC_AR_PAY_TYPE'\nAND dtl.ENABLE_FLAG = 'Y'\nORDER BY dtl.VAL_CODE","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":"seek_icon"}