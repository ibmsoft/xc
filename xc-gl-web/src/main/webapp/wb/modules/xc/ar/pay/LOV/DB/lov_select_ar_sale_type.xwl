{"inframe":false,"title":"销售类型查询","hidden":false,"roles":{"default":1},"children":[{"configs":{"serverScript":"var data = app.get();\n\napp.info(data.LEDGER_ID);","itemId":"module"},"children":[{"configs":{"sql":"SELECT\n    t.LEDGER_ID,\n    t.AR_SALE_TYPE_ID,\n    t.BG_ITEM_ID,\n    t.AR_ACC_ID_INV AS ACC_ID,\n    t.PRODUCT_ID,\n    t1.AR_SALE_TYPE_NAME,\n    acc.ACC_NAME,\n    bg.BG_ITEM_NAME,\n\tproduct.PRODUCT_NAME\nFROM\n\txc_ar_ld_sale_type t\n      LEFT JOIN xc_ar_sale_type t1 ON t.AR_SALE_TYPE_ID = t1.AR_SALE_TYPE_ID\n      LEFT JOIN xc_gl_accounts acc ON t.AR_ACC_ID_INV = acc.ACC_ID\n      LEFT JOIN xc_bg_items bg ON t.BG_ITEM_ID = bg.BG_ITEM_ID\n      LEFT JOIN xc_gl_products product ON t.PRODUCT_ID = product.PRODUCT_ID\nWHERE \n\tt.LEDGER_ID = {?LEDGER_ID?}\nAND t1.AR_SALE_TYPE_NAME LIKE '%{#AR_SALE_TYPE_NAME#}%'","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":"seek_icon"}