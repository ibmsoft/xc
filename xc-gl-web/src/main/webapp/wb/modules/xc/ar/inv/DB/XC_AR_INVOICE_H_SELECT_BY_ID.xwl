{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"initScript":"var lang = request.getSession().getAttribute(\"XzSessionVars\").get('language');\nrequest.setAttribute(\"language\",lang);","loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT\n\tarih.*,ardc.AR_CAT_NAME,\n\tpp.PROJECT_NAME,\n\tpd.DEPT_NAME,\n\tgl.LEDGER_NAME,\n\tgl.LEDGER_ID,\n\tarcu.CUSTOMER_NAME,\n\tarco.AR_CONTRACT_NAME,\n    arih.CANCEL_AMT INV_CANCEL_AMT,\n    (SELECT\n       CASE WHEN l.DESCRIPTION IS NULL OR l.DESCRIPTION = '' THEN d.DESCRIPTION ELSE l.DESCRIPTION END DESCRIPTION\n     FROM\n       xip_pub_val_set_dtl d\n     LEFT JOIN XIP_PUB_VAL_SET_DTL_LANG l ON d.VAL_SET_CODE = l.VAL_SET_CODE\n     AND d.VAL_CODE = l.VAL_CODE\n     AND l.VAL_SET_LANG = '{#language#}'\n     WHERE\n       d.VAL_SET_CODE = 'XC_AP_SYS_AUDIT_STATUS'\n     AND d.ENABLE_FLAG = 'Y'\n     AND d.VAL_CODE = arih.SYS_AUDIT_STATUS) AS XC_AP_SYS_AUDIT_STATUS,\n    (SELECT AR_INV_H_CODE FROM xc_ar_invoice_h a WHERE arih.L_AR_INV_H_ID = a.AR_INV_H_ID) L_AR_INV_H_CODE,\n\t(SELECT EMP_NAME FROM xip_pub_emps WHERE EMP_ID=(SELECT EMP_ID FROM xip_pub_users b WHERE b.USER_ID = arih.FIN_USER_ID)) FIN_USER_NAME,\n\t(SELECT EMP_NAME FROM xip_pub_emps WHERE EMP_ID=(SELECT EMP_ID FROM xip_pub_users b WHERE b.USER_ID = arih.PRINT_USER_ID)) PRINT_USER_NAME,\n\t(SELECT EMP_NAME FROM xip_pub_emps WHERE EMP_ID=(SELECT EMP_ID FROM xip_pub_users b WHERE b.USER_ID = arih.CREATED_BY)) CREATED_BY_NAME\nFROM\n\txc_ar_invoice_h arih\nLEFT JOIN xc_ar_doc_cats ardc ON arih.AR_DOC_CAT_CODE = ardc.AR_CAT_CODE\nLEFT JOIN xc_pm_projects pp ON arih.PROJECT_ID = pp.PROJECT_ID\nLEFT JOIN xip_pub_depts pd ON arih.DEPT_ID = pd.DEPT_ID\nLEFT JOIN xc_gl_ledgers gl ON arih.LEDGER_ID = gl.LEDGER_ID\nLEFT JOIN xc_ar_customers arcu ON arih.CUSTOMER_ID = arcu.CUSTOMER_ID\nLEFT JOIN xc_ar_contract arco ON arih.AR_CONTRACT_ID = arco.AR_CONTRACT_ID\nWHERE arih.AR_INV_H_ID = {?AR_INV_H_ID?}","type":"object","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}