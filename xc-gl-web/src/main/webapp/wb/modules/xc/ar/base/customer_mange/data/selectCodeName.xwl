{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module "},"children":[{"configs":{"sql":"SELECT \n  a.VENDOR_NAME as VENDOR_NAME,\n  (SELECT \n    c.CSTM_SORT_NAME \n  FROM\n     xc_ar_customer_sort c\n  WHERE t.SORT1 = c.CSTM_SORT_CODE and c.CSTM_TYPE_CODE='大类1'\n    ) AS SORT1_NAME,\n  (SELECT \n      c.CSTM_SORT_NAME \n    FROM\n       xc_ar_customer_sort c\n    WHERE t.SORT2 = c.CSTM_SORT_CODE and c.CSTM_TYPE_CODE='大类2'\n      ) AS SORT2_NAME,\n  (SELECT \n      c.CSTM_SORT_NAME \n    FROM\n       xc_ar_customer_sort c\n    WHERE t.SORT3 = c.CSTM_SORT_CODE and c.CSTM_TYPE_CODE='大类3'\n      ) AS SORT3_NAME,\n\n  t.* \nFROM\n  xc_ar_customers t   LEFT JOIN xc_ap_vendors a\n    ON t.VENDOR_ID = a.VENDOR_ID \n   \nWHERE (\n    CUSTOMER_CODE LIKE  '%{#codeName#}%'\n    OR CUSTOMER_NAME LIKE  '%{#codeName#}%'\n  ) \nORDER BY CUSTOMER_CODE ASC \n\n\n","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}