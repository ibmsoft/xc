{"inframe":false,"title":"我的销售发票行信息查询","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT\n\taril.*,\n\tarst.AR_SALE_TYPE_NAME,\n\tgp.PRODUCT_NAME,\n\tgd.DIM_NAME\nFROM\n\txc_ar_invoice_l aril\nLEFT JOIN xc_ar_sale_type arst ON aril.AR_SALE_TYPE_ID = arst.AR_SALE_TYPE_ID\nLEFT JOIN xc_gl_products gp ON aril.PRODUCT_ID = gp.PRODUCT_ID\nLEFT JOIN xc_gl_dimensions gd ON aril.DIM_CODE = gd.DIM_CODE\nWHERE aril.AR_INV_H_ID = {?AR_INV_H_ID?}","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":"seek_icon"}