{"inframe":false,"title":"查询会计科目","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"oracle":"SELECT account.ACC_ID,account.ACC_CODE,account.ACC_NAME \n  FROM xc_gl_ld_accounts ldaccount,\n       xc_gl_accounts account\n WHERE ldaccount.ACC_ID = account.ACC_ID\n   AND account.IS_LEAF = 'Y'\n   and account.START_DATE <= sysdate\n   and account.END_DATE >= sysdate\n   AND ldaccount.LEDGER_ID = {?LEDGER_ID?} \n   AND (nvl(account.ACC_CODE,'%') LIKE '%{#acc_name#}%' \n        OR nvl(account.ACC_NAME,'%') LIKE '%{#acc_name#}%' )\n   AND (IS_FOREGIN_CNY = '0' OR IS_FOREGIN_CNY = 0)\nORDER BY ACC_CODE","itemId":"sqlswitcher1","mysql":"SELECT account.ACC_ID,account.ACC_CODE,account.ACC_NAME \n  FROM xc_gl_ld_accounts ldaccount,\n       xc_gl_accounts account\n WHERE ldaccount.ACC_ID = account.ACC_ID\n   AND account.IS_LEAF = 'Y'\n   AND ldaccount.LEDGER_ID = {?LEDGER_ID?} \n   AND (IFNULL(account.ACC_CODE,'%') LIKE '%{#acc_name#}%' \n        OR IFNULL(account.ACC_NAME,'%') LIKE '%{#acc_name#}%' )\n   AND account.START_DATE <= NOW()\n   AND account.END_DATE >=  NOW()\n   AND (IS_FOREGIN_CNY = '0' OR IS_FOREGIN_CNY = 0)\nORDER BY ACC_CODE"},"children":[],"expanded":false,"type":"sqlswitcher"},{"configs":{"sql":"{#sqlswitcher1#}","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}