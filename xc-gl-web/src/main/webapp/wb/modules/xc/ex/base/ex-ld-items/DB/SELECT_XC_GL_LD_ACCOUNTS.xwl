{"inframe":false,"title":"查询会计科目列表","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"select \n  a.ACC_ID,\n  b.ACC_CODE,\n  b.ACC_NAME\nfrom xc_gl_ld_accounts a,xc_gl_accounts b\nwhere a.LEDGER_ID = {?LEDGER_ID?}\n  and a.ACC_ID = b.ACC_ID\n  and b.IS_LEAF = 'Y'\n  and a.IS_FOREGIN_CNY = 0\n  and NOW() between a.START_DATE and a.END_DATE\n  and (b.ACC_CODE like '%{#CODE_NAME#}%' or b.ACC_NAME like '%{#CODE_NAME#}%')\norder by b.ACC_CODE\n\n\n\n","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}