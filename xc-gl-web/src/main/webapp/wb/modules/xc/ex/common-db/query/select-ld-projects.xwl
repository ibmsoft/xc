{"inframe":false,"title":"查询账簿下PA项目信息","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"oracle":"SELECT T.PROJECT_ID, T.PROJECT_CODE, T.PROJECT_NAME, T.LEDGER_ID\n  FROM XC_PM_PROJECTS T, xc_pm_prj_dept xpd\n WHERE T.PROJECT_ID = xpd.PROJECT_ID\n   and T.LEDGER_ID = xpd.LEDGER_ID\n   and T.LEDGER_ID = {?ledgerId?}\n   and xpd.DEPT_ID = {?deptId?}\n   and not exists (select 1 from XC_PM_PROJECTS xp where t.PROJECT_ID = xp.UP_PROJECT_ID)\n   and (t.PROJECT_CODE like '%{#textStr#}%' or t.PROJECT_NAME like '%{#textStr#}%')\n   and t.START_DATE <= sysdate and t.END_DATE >= sysdate\norder by T.PROJECT_CODE ;","itemId":"sqlswitcher1","mysql":"SELECT T.PROJECT_ID, T.PROJECT_CODE, T.PROJECT_NAME, T.LEDGER_ID\n  FROM XC_PM_PROJECTS T, xc_pm_prj_dept xpd\n WHERE T.PROJECT_ID = xpd.PROJECT_ID\n   and T.LEDGER_ID = xpd.LEDGER_ID\n   and T.LEDGER_ID = {?ledgerId?}\n   and xpd.DEPT_ID = {?deptId?}\n   and not exists (select 1 from XC_PM_PROJECTS xp where t.PROJECT_ID = xp.UP_PROJECT_ID)\n   and (t.PROJECT_CODE like '%{#textStr#}%' or t.PROJECT_NAME like '%{#textStr#}%')\n   and t.START_DATE <= NOW() and t.END_DATE >= NOW()\norder by T.PROJECT_CODE ;"},"children":[],"expanded":false,"type":"sqlswitcher"},{"configs":{"sql":"{#sqlswitcher1#}","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":"seek_icon"}