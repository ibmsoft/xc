{"inframe":false,"title":"查询已处理付款单","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"itemId":"sqlswitcher1","oracle":"SELECT  T.EX_PAY_ID, \n        T.EX_PAY_CODE EX_PAY_CODE,\n        T.CA_ID,\n        T.PAY_TYPE,\n        T.EX_PAY_DESC,\n        T.LEDGER_ID,\n        T.CREATED_BY,\n        E.EMP_NAME USER_NAME,\n        A.BANK_ACCOUNT,\n        T.DEPOSIT_BANK_ID,\n        TO_DATE(T.EX_PAY_DATE, 'yyyy-MM-dd') EX_PAY_DATE,\n        T.EX_PAY_T_AMT,\n        T.V_HEAD_ID,\n        T.V_STATUS,\n        T.PAY_STATUS,\n        T.PAY_CAT\n  FROM XIP_PUB_USERS U,XIP_PUB_EMPS E, XC_EX_PAY T \n  LEFT JOIN XC_GL_LD_DEPOSIT_BANK A ON T.DEPOSIT_BANK_ID = A.DEPOSIT_BANK_ID\nWHERE T.LEDGER_ID = {?LEDGER_ID?}\n  AND (T.EX_PAY_CODE LIKE '%{#EX_PAY_CODE#}%' OR T.EX_PAY_DESC LIKE '%{#EX_PAY_CODE#}%')\n  AND {#amount#}\n  AND T.PAY_CAT = 'new'\n  AND T.CREATED_BY = U.USER_ID\n  AND U.EMP_ID = E.EMP_ID\n  AND E.EMP_NAME LIKE '%{#CREATED_BY#}%'","mysql":"SELECT  T.EX_PAY_ID, \n        T.EX_PAY_CODE EX_PAY_CODE,\n        T.CA_ID,\n        T.PAY_TYPE,\n        T.EX_PAY_DESC,\n        T.LEDGER_ID,\n        T.CREATED_BY,\n        E.EMP_NAME USER_NAME,\n        A.BANK_ACCOUNT,\n        T.DEPOSIT_BANK_ID,\n        DATE_FORMAT(T.EX_PAY_DATE, '%Y-%m-%d') EX_PAY_DATE,\n        T.EX_PAY_T_AMT,\n        T.V_HEAD_ID,\n        T.V_STATUS,\n        T.PAY_STATUS,\n        T.PAY_CAT\n  FROM XIP_PUB_USERS U,XIP_PUB_EMPS E, XC_EX_PAY T \n  LEFT JOIN XC_GL_LD_DEPOSIT_BANK A ON T.DEPOSIT_BANK_ID = A.DEPOSIT_BANK_ID\nWHERE T.LEDGER_ID = {?LEDGER_ID?}\n  AND (T.EX_PAY_CODE LIKE '%{#EX_PAY_CODE#}%' OR T.EX_PAY_DESC LIKE '%{#EX_PAY_CODE#}%')\n  AND {#amount#}\n  AND T.PAY_CAT = 'new'\n  AND T.PAY_STATUS = 'Y'\n  AND T.V_STATUS = '3'\n  AND T.CREATED_BY = U.USER_ID\n  AND U.EMP_ID = E.EMP_ID\n  AND E.EMP_NAME LIKE '%{#CREATED_BY#}%'"},"children":[],"expanded":false,"type":"sqlswitcher"},{"configs":{"sql":"{#sqlswitcher1#}","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":"seek_icon"}