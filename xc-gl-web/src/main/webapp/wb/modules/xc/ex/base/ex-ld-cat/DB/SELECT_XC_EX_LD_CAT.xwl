{"inframe":false,"title":"查询账簿费用单据类型分配表","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"select \n\tt1.EX_LD_CAT_ID,\n\tt1.LEDGER_ID,\n\tt1.EX_CAT_CODE,\n\tt2.EX_CAT_NAME,\n\tt1.PROCESS_ID,\n\tpro.PROCESS_NAME,\n    cat.CAT_CODE ATT_CAT_CODE,\n    cat.CAT_NAME ATT_CAT_NAME,\n\tt1.RULE_CODE,\n    rule.RULE_DESC,\n\t(select count(*) from xc_ex_ld_cat_items item where item.LEDGER_ID = t1.LEDGER_ID and item.EX_CAT_CODE = t1.EX_CAT_CODE) as EX_ITEM_ENABLED\nfrom\n\txc_ex_ld_cat t1\n\tleft join xip_wf_processes pro on pro.PROCESS_ID = t1.PROCESS_ID\n\tleft join xc_gl_rules rule on rule.RULE_CODE = t1.RULE_CODE\n    left join xip_pub_att_cat cat on t1.ATT_CAT_CODE = cat.CAT_CODE\n\t,\n\txc_ex_cat t2\nwhere t1.EX_CAT_CODE = t2.EX_CAT_CODE\n  and t1.LEDGER_ID = {?LEDGER_ID?}\n  and (t1.EX_CAT_CODE LIKE '%{#CODE_NAME#}%' or t2.EX_CAT_NAME LIKE '%{#CODE_NAME#}%')\norder by \n  t1.EX_CAT_CODE","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}