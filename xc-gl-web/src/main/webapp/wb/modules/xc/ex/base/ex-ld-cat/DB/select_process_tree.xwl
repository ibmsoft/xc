{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"select tree.TREE_ID as 'id' , tree.TREE_CODE as 'code' , tree.TREE_NAME as 'text', tree.UP_ID as 'parent_id', tree.IS_LEAF as 'leaf'\nfrom\n(\nselect org.ORG_ID as TREE_ID,org.ORG_CODE as TREE_CODE,org.ORG_NAME as TREE_NAME, '-1' as UP_ID, '_false' as IS_LEAF\nfrom xip_pub_orgs org,xc_gl_ledgers ledger\nwhere org.ORG_ID =  ledger.ORG_ID\n  and ledger.LEDGER_ID = {?LEDGER_ID?}\nunion \nselect 'commom_process' as TREE_ID,'commom_process' as TREE_CODE,'公共流程' as TREE_NAME,'-1' as UP_ID,'_false' as IS_LEAF\nunion\nselect t1.PROCESS_ID as TREE_ID,t1.PROCESS_CODE as TREE_CODE,t1.PROCESS_NAME as TREE_NAME,'commom_process' as UP_ID,'_true' as IS_LEAF\nfrom xip_wf_processes t1,xip_wf_entities t2\nwhere t1.ENTITY_ID = t2.ENTITY_ID\n  and t2.ENTITY_CODE = 'XC_EX_DOCS'\n\tand (t1.ORG_ID = '' OR t1.ORG_ID IS NULL)\n  and t1.ENABLE_FLAG = 'Y'\nunion\nselect t1.PROCESS_ID as TREE_ID,t1.PROCESS_CODE as TREE_CODE,t1.PROCESS_NAME as TREE_NAME,t1.ORG_ID as UP_ID,'_true' as IS_LEAF\nfrom xip_wf_processes t1,xip_wf_entities t2,xc_gl_ledgers t3,xip_pub_orgs org\nwhere t1.ENTITY_ID = t2.ENTITY_ID\n\tand t2.ENTITY_CODE = 'XC_EX_DOCS'\n\tand t3.LEDGER_ID = {?LEDGER_ID?}\n\tand t1.ORG_ID = t3.ORG_ID\n  and t1.ORG_ID = org.ORG_ID\n\tand t1.ENABLE_FLAG = 'Y'\n) tree\nwhere UP_ID ={?parent_id?}\n  and case tree.IS_LEAF\n        when '_true' then (tree.TREE_CODE like '%{#code_name#}%' or tree.TREE_NAME like '%{#code_name#}%')\n        else 1=1\n      end \n\n\n\n\n\n","type":"tree","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}