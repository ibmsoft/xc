{"inframe":false,"title":"查询账簿费用项分配表","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT\n\tt.EX_LD_ITEM_ID,\n\tt.LEDGER_ID,\n\tt.EX_ITEM_ID,\n\tt1.EX_ITEM_CODE,\n\tt1.EX_ITEM_NAME,\n\tt1.EX_ITEM_DESC,\n    t1.EX_ITEM_UP_ID,\n    ex.EX_ITEM_NAME EX_ITEM_UP_NAME,\n\tt.ACC_ID,\n\tacc.ACC_NAME,\n\tt.BG_ITEM_ID,\n\tbg.BG_ITEM_NAME,\n\tt.IS_ENABLED\nFROM\n\txc_ex_ld_items t\n    left join xc_gl_accounts acc on t.ACC_ID = acc.ACC_ID\n    left join xc_bg_items bg on bg.BG_ITEM_ID = t.BG_ITEM_ID,\n\txc_ex_items t1\n    left join xc_ex_items ex on ex.EX_ITEM_ID = t1.EX_ITEM_UP_ID\nWHERE\n\tt.EX_ITEM_ID = t1.EX_ITEM_ID\nAND t.LEDGER_ID = {?LEDGER_ID?}\nAND (\n\tt1.EX_ITEM_CODE LIKE '%{#EX_ITEM_CODE_NAME#}%'\n\tOR t1.EX_ITEM_NAME LIKE '%{#EX_ITEM_CODE_NAME#}%'\n)\nORDER BY \n\tt1.EX_ITEM_CODE","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}