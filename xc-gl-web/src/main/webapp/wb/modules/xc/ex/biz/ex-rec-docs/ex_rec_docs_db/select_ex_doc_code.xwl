{"inframe":false,"title":"查询单据编号","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT doc.EX_DOC_ID,doc.EX_DOC_CODE,cat.EX_CAT_NAME,xpf.FORM_URL PC_A_FORM_URL\n  FROM xc_ex_docs doc,xc_ex_cat cat  \n   left join xip_wf_process_forms xpf on cat.pc_a_form_id = xpf.form_id\n WHERE doc.LEDGER_ID = {?ledger_id?}\n   AND doc.EX_CAT_CODE = cat.EX_CAT_CODE\n   AND cat.IS_SIGN = 'Y'\n   AND SIGN_STATUS != '2' \n   AND SIGN_STATUS != 2\n   AND (AUDIT_STATUS = 'C' OR AUDIT_STATUS = 'E')\nORDER BY doc.EX_DOC_CODE","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":"seek_icon"}