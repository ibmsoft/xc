{"inframe":false,"title":"查询所有的还款余额","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"oracle":"select b.EMP_CODE,\n       b.EMP_NAME,\n       ( select d.dept_name\n        from xip_pub_emp_asg c,xip_pub_depts d , xc_gl_ledgers e\n        where c.DEPT_ID = d.DEPT_ID\n          and c.ORG_ID = e.ORG_ID\n          and e.LEDGER_ID = {?ledger_id?}\n          and rownum = 1\n          ) DEPT_NAME,\n       t.IA_BAL\n  from xc_ex_ia_fact t, xip_pub_users a, xip_pub_emps b\n where t.EX_USER_ID = a.USER_ID\n   and a.EMP_ID = b.EMP_ID\n   and t.LEDGER_ID = {?ledger_id?}\n   and t.ORDERBY in\n       (select max(a.ORDERBY)\n          from xc_ex_ia_fact a\n         where a.LEDGER_ID = {?ledger_id?}\n         group by a.LEDGER_ID, a.EX_USER_ID)\n order by t.LEDGER_ID, t.EX_USER_ID, t.ORDERBY\n","itemId":"sqlswitcher1","mysql":"select b.EMP_CODE,\n       b.EMP_NAME,\n       (select group_concat(distinct d.DEPT_NAME)\n          from xip_pub_emp_asg c, xip_pub_depts d, xc_gl_ledgers e\n         where c.DEPT_ID = d.DEPT_ID\n           and c.ORG_ID = e.ORG_ID\n           and e.LEDGER_ID = {?ledger_id?}\n       ) DEPT_NAME,\n       t.IA_BAL\n  from xc_ex_ia_fact t, xip_pub_users a, xip_pub_emps b\n where t.EX_USER_ID = a.USER_ID\n   and a.EMP_ID = b.EMP_ID\n   and t.LEDGER_ID = {?ledger_id?}\n   and t.ORDERBY in\n       (select max(a.ORDERBY)\n          from xc_ex_ia_fact a\n         where a.LEDGER_ID = {?ledger_id?}\n         group by a.LEDGER_ID, a.EX_USER_ID)\n order by t.LEDGER_ID, t.EX_USER_ID, t.ORDERBY\n"},"children":[],"expanded":false,"type":"sqlswitcher"},{"configs":{"sql":"{#sqlswitcher1#}","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":"seek_icon"}