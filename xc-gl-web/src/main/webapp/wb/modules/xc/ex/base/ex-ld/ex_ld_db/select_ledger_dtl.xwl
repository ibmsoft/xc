{"inframe":false,"title":"查询账薄明细","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"oracle":"SELECT\n\tle.LEDGER_ID,\n    le.LEDGER_CODE,\n    le.LEDGER_NAME,\n    le.ACC_HRCY_ID,\n    le.EX_HRCY_ID,\n    exhr.EX_HRCY_NAME,\n\n    le.EX_CR_ACC_ID,\n    glac.ACC_NAME AS EX_CR_ACC_NAME,\n    le.EX_IA_ACC_ID,\n    glac2.ACC_NAME AS EX_IA_ACC_NAME,\n    le.IS_CR_PAY,\n    iscrpay.VAL_NAME AS IS_CR_PAY_NAME\nFROM\n    XC_GL_LEDGERS le\nLEFT JOIN XC_EX_HRCY exhr ON le.EX_HRCY_ID = exhr.EX_HRCY_ID\nLEFT JOIN xc_gl_accounts glac ON le.EX_CR_ACC_ID = glac.ACC_ID\nLEFT JOIN xc_gl_accounts glac2 ON le.EX_IA_ACC_ID = glac2.ACC_ID\nLEFT JOIN xip_pub_val_set_dtl iscrpay ON le.IS_CR_PAY = iscrpay.VAL_CODE AND iscrpay.VAL_SET_CODE='YN'\nWHERE le.IS_VALID = 'Y'\n   AND (EXISTS (SELECT 1\n          FROM XC_GL_LD_SEC A\n         WHERE le.LEDGER_ID = A.LEDGER_ID\n           AND A.USER_ID = {?XIP.userId?})\n    OR EXISTS (SELECT 1\n          FROM XIP_PUB_USER_ROLES XUR, XIP_PUB_ROLE XR, XC_GL_LD_SEC XLC\n         WHERE XUR.ROLE_ID = XR.ROLE_ID\n           AND XR.ROLE_ID = XLC.ROLE_ID\n           AND XLC.LEDGER_ID = le.LEDGER_ID\n           AND XUR.USER_ID = {?XIP.userId?}))\n    AND nvl(le.LEDGER_ID ,'%') like '%{#ledger_id#}%'\nORDER BY le.LEDGER_CODE","itemId":"sqlswitcher1","mysql":"SELECT\n\tle.LEDGER_ID,\n    le.LEDGER_CODE,\n    le.LEDGER_NAME,\n    le.ACC_HRCY_ID,\n    le.EX_HRCY_ID,\n    exhr.EX_HRCY_NAME,\n\n    le.EX_CR_ACC_ID,\n    glac.ACC_NAME AS EX_CR_ACC_NAME,\n    le.EX_IA_ACC_ID,\n    glac2.ACC_NAME AS EX_IA_ACC_NAME,\n    le.IS_CR_PAY,\n    iscrpay.VAL_NAME AS IS_CR_PAY_NAME\nFROM\n    XC_GL_LEDGERS AS le\nLEFT JOIN XC_EX_HRCY AS exhr ON le.EX_HRCY_ID = exhr.EX_HRCY_ID\nLEFT JOIN xc_gl_accounts AS glac ON le.EX_CR_ACC_ID = glac.ACC_ID\nLEFT JOIN xc_gl_accounts AS glac2 ON le.EX_IA_ACC_ID = glac2.ACC_ID\nLEFT JOIN xip_pub_val_set_dtl AS iscrpay ON le.IS_CR_PAY = iscrpay.VAL_CODE AND iscrpay.VAL_SET_CODE='YN'\nWHERE le.IS_VALID = 'Y'\n  AND EXISTS (SELECT 1\n          FROM XC_GL_LD_SEC A\n         WHERE le.LEDGER_ID = A.LEDGER_ID\n           AND A.ROLE_ID = {?XIP.roleId?}\n           AND A.USER_ID = {?XIP.userId?})\n    AND IFNULL(le.LEDGER_ID ,'%') like '%{#ledger_id#}%'\nORDER BY le.LEDGER_CODE"},"children":[],"expanded":false,"type":"sqlswitcher"},{"configs":{"sql":"{#sqlswitcher1#}","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}