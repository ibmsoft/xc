{"inframe":false,"title":"查询账簿下预算项目信息","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"oracle":"select A.LEDGER_ID, A.BG_ITEM_ID, B.BG_ITEM_CODE, B.BG_ITEM_NAME\n  from xc_bg_ld_items a, xc_bg_items b\n where a.bg_item_id = b.bg_item_id\n   and b.start_date <= sysdate\n   and b.end_date >= sysdate\n   and a.is_enabled = 'Y'\n   and a.ledger_id = {?ledgerId?}\n   and not exists (select 1 from xc_bg_items c where b.BG_ITEM_ID = c.BG_ITEM_UP_ID)\n   and (b.bg_item_code like '%{#textStr#}%' or b.bg_item_name like '%{#textStr#}%')\norder by b.bg_item_code","itemId":"sqlswitcher1","mysql":"select A.LEDGER_ID, A.BG_ITEM_ID, B.BG_ITEM_CODE, B.BG_ITEM_NAME\n  from xc_bg_ld_items a, xc_bg_items b\n where a.bg_item_id = b.bg_item_id\n   and b.start_date <= CURRENT_TIMESTAMP()\n   and b.end_date >= CURRENT_TIMESTAMP()\n   and a.is_enabled = 'Y'\n   and a.ledger_id = {?ledgerId?}\n   and not exists (select 1 from xc_bg_items c where b.BG_ITEM_ID = c.BG_ITEM_UP_ID)\n   and (b.bg_item_code like '%{#textStr#}%' or b.bg_item_name like '%{#textStr#}%')\norder by b.bg_item_code"},"children":[],"expanded":false,"type":"sqlswitcher"},{"configs":{"sql":"{#sqlswitcher1#}","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":"seek_icon"}