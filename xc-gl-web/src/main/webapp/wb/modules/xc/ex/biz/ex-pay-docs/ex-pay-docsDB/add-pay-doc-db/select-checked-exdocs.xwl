{"inframe":false,"title":"查询挂账支付且已复核的报销单","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"itemId":"sqlswitcher1","oracle":"SELECT A.EX_DOC_ID,\n       A.EX_DOC_CODE,\n       A.EX_CAT_CODE,\n       B.EX_CAT_NAME,\n       A.LEDGER_ID,\n       A.ORG_ID,\n       A.BIZ_DATE,\n       A.DEPT_ID,\n       XD.DEPT_NAME,\n       A.PROJECT_ID,\n       XP.PROJECT_NAME,\n       A.EX_REASON,\n       A.EX_ITEM_ID,\n       A.BG_ITEM_ID,\n       A.AMOUNT,\n       A.APPLY_EX_DOC_ID,\n       A.CANCEL_AMT,\n\t   (A.AMOUNT-A.CANCEL_AMT) AS TOPIC_PAY_AMT,\n       A.V_HEAD_ID,\n       A.V_STATUS,\n       A.PAY_METHOD,\n       A.EX_PAY_ID,\n       e.EMP_NAME,\n       c.EMP_ID,\n       c.BANK_NAME,\n\t\t\t c.BANK_NO\n  FROM XC_EX_CAT B, \n      xc_ex_ext_emps C,\n      xip_pub_users D,\n      xip_pub_emps E,\n      XC_EX_DOCS A \n  LEFT JOIN XIP_PUB_DEPTS XD ON A.DEPT_ID = XD.DEPT_ID\n  LEFT JOIN XC_PM_PROJECTS XP ON A.PROJECT_ID = XP.PROJECT_ID\n  WHERE B.EX_CAT_CODE = A.EX_CAT_CODE\n    AND C.EMP_ID = d.EMP_ID\n    and d.EMP_ID = e.EMP_ID\n    and d.USER_ID = a.CREATED_BY\n    AND A.PAY_METHOD ='2'\n    AND A.V_STATUS  = '3'\n    AND NVL(A.EX_PAY_ID,'') = ''\n    AND A.LEDGER_ID = {?LEDGER_ID?}\n    AND A.EX_DOC_CODE like '%{#docCode#}%'\n","mysql":"SELECT A.EX_DOC_ID,\n       A.EX_DOC_CODE,\n       A.EX_CAT_CODE,\n       B.EX_CAT_NAME,\n       A.LEDGER_ID,\n       A.ORG_ID,\n       A.BIZ_DATE,\n       A.DEPT_ID,\n       XD.DEPT_NAME,\n       A.PROJECT_ID,\n       XP.PROJECT_NAME,\n       A.EX_REASON,\n       A.EX_ITEM_ID,\n       A.BG_ITEM_ID,\n       A.AMOUNT,\n       A.APPLY_EX_DOC_ID,\n       A.CANCEL_AMT,\n       (A.AMOUNT - A.CANCEL_AMT) AS TOPIC_PAY_AMT,\n       A.V_HEAD_ID,\n       A.V_STATUS,\n       A.PAY_METHOD,\n       A.EX_PAY_ID,\n       e.EMP_NAME,\n       c.EMP_ID,\n       c.BANK_NAME,\n       c.BANK_NO\n  FROM XC_EX_CAT      B,\n       xc_ex_ext_emps C,\n       xip_pub_users  D,\n       xip_pub_emps   E,\n       XC_EX_DOCS     A\n  LEFT JOIN XIP_PUB_DEPTS XD\n    ON A.DEPT_ID = XD.DEPT_ID\n  LEFT JOIN XC_PM_PROJECTS XP\n    ON A.PROJECT_ID = XP.PROJECT_ID\n WHERE B.EX_CAT_CODE = A.EX_CAT_CODE\n   AND C.EMP_ID = d.EMP_ID\n   and d.EMP_ID = e.EMP_ID\n   and d.USER_ID = a.CREATED_BY\n   AND A.PAY_METHOD = '2'\n   AND A.V_STATUS = '3'\n   AND IFNULL(A.EX_PAY_ID, '') = ''\n   AND A.LEDGER_ID = {?ledgerId?}\n   AND A.EX_DOC_CODE like '%{#docCode#}%'\n"},"children":[],"expanded":false,"type":"sqlswitcher"},{"configs":{"sql":"{#sqlswitcher1#}","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":"seek_icon"}