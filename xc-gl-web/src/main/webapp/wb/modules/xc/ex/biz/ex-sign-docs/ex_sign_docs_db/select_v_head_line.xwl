{"inframe":false,"title":"查询凭证信息","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT account.ACC_NAME,line.SUMMARY,line.ACCOUNT_DR,line.ACCOUNT_CR\n  FROM xc_gl_v_heads head,\n       xc_gl_v_lines line,\n       xc_gl_ccid ccid,\n       xc_ex_pay pay,\n       xc_gl_accounts account\n WHERE pay.EX_PAY_ID = {?EX_PAY_ID?}\n   AND pay.V_HEAD_ID = head.V_HEAD_ID\n   AND head.V_HEAD_ID = line.V_HEAD_ID\n   AND ccid.CCID = line.CCID\n   AND ccid.ACC_ID = account.ACC_ID\nORDER BY  account.ACC_CODE","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":"seek_icon"}