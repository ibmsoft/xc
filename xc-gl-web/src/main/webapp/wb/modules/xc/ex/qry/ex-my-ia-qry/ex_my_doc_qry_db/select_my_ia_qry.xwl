{"inframe":false,"title":"查询我的备用金","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"itemId":"sqlswitcher1","oracle":"SELECT\n   t.EX_IA_FACT_AMT,\n   t.LEDGER_ID,\n   t.EX_DOC_ID,\n   to_char(t.CREATION_DATE,'yyyy-mm-dd') AS CREATION_DATE,\n   doc.EX_DOC_CODE,\n   doc.EX_REASON,\n   t.IA_BAL,\n   doc.EX_CAT_CODE\n FROM xc_ex_ia_fact t\nLEFT JOIN xc_ex_docs doc ON t.EX_DOC_ID = doc.EX_DOC_ID\nWHERE t.ex_user_id = {?XIP.userId?} \n  AND t.LEDGER_ID = {?ledger_id?}\nORDER BY t.CREATION_DATE","mysql":"SELECT\n   t.EX_IA_FACT_AMT,\n   t.LEDGER_ID,\n   t.EX_DOC_ID,\n   t.EX_IA_TYPE,\n   DATE_FORMAT(t.CREATION_DATE,'%Y-%m-%d') AS CREATION_DATE,\n   doc.EX_DOC_CODE,\n   doc.EX_REASON,\n   t.IA_BAL,\n   doc.EX_CAT_CODE\n FROM xc_ex_ia_fact t\nLEFT JOIN xc_ex_docs doc ON t.EX_DOC_ID = doc.EX_DOC_ID\nWHERE t.ex_user_id = {?XIP.userId?} \n  AND t.LEDGER_ID = {?ledger_id?}\nORDER BY t.CREATION_DATE"},"children":[],"expanded":false,"type":"sqlswitcher"},{"configs":{"sql":"{#sqlswitcher1#}","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":"seek_icon"}