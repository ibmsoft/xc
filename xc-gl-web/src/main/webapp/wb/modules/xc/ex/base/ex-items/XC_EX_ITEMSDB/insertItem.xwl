{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","serverScript":"//在同一体系下面不能有相同的编码\napp.run('select 1 from xc_ex_items where EX_ITEM_CODE ={?EX_ITEM_CODE?} and EX_HRCY_ID={?EX_HRCY_ID?}',{\nerrorText:'编码为\"'+app.get('EX_ITEM_CODE')+'\"的费用项已经在该体系中存在,请修改!'\n});\n//在同一体系下面不能有相同的名称\napp.run('select 1 from xc_ex_items where EX_ITEM_NAME ={?EX_ITEM_NAME?} and EX_HRCY_ID={?EX_HRCY_ID?}',{\nerrorText:'名称为\"'+app.get('EX_ITEM_NAME')+'\"的费用项已经在该体系中存在,请修改!'\n});\n//为当前记录生成一个uuid，作为EX_HRCY_ID\n var EX_ITEM_ID = java.util.UUID.randomUUID();\nrequest.setAttribute('EX_ITEM_ID',EX_ITEM_ID);\napp.update({\n tableName:'xc_ex_items',\n  mode:'insert'\n});","itemId":"module"},"children":[],"expanded":false,"type":"module"}],"pageLink":"","iconCls":""}