{"inframe":false,"title":"查询付款单详情","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT \n      pay.LEDGER_ID,\n      pay.EX_PAY_ID,\n      pay.V_HEAD_ID,      \n\n      bank.ACCOUNT_NAME,\n      bank.DEPOSIT_BANK_NAME,\n      bank.BANK_ACCOUNT,\n      \n      dtl.PAY_AMT,    \n \n      docs.AMOUNT,\n      docs.CANCEL_AMT,\n      docs.EX_DOC_CODE\n\n  FROM \n      xc_ex_pay  pay\nLEFT JOIN xc_gl_ld_deposit_bank bank ON pay.DEPOSIT_BANK_ID = bank.DEPOSIT_BANK_ID\nLEFT JOIN xc_ex_pay_dtl dtl ON pay.EX_PAY_ID = dtl.EX_PAY_ID\nLEFT JOIN xc_ex_docs docs ON dtl.EX_DOC_ID = docs.EX_DOC_ID\n\n WHERE pay.EX_PAY_ID = {?EX_PAY_ID?}\nORDER BY  pay.EX_PAY_DATE DESC","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":"seek_icon"}