{"inframe":false,"title":"根据账薄ID查询账薄信息","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT\n\tle.LEDGER_ID,\n    le.LEDGER_CODE,\n    le.LEDGER_NAME,\n    le.ACC_HRCY_ID,\n    le.EX_HRCY_ID,\n    exhr.EX_HRCY_NAME,\n\n    le.EX_CR_ACC_ID,\n    glac.ACC_NAME AS EX_CR_ACC_NAME,\n    le.EX_IA_ACC_ID,\n    glac2.ACC_NAME AS EX_IA_ACC_NAME,\n    le.IS_CR_PAY,\n    iscrpay.VAL_NAME AS IS_CR_PAY_NAME\nFROM\n    XC_GL_LEDGERS le\nLEFT JOIN XC_EX_HRCY exhr ON le.EX_HRCY_ID = exhr.EX_HRCY_ID\nLEFT JOIN xc_gl_accounts glac ON le.EX_CR_ACC_ID = glac.ACC_ID\nLEFT JOIN xc_gl_accounts glac2 ON le.EX_IA_ACC_ID = glac2.ACC_ID\nLEFT JOIN xip_pub_val_set_dtl iscrpay ON le.IS_CR_PAY = iscrpay.VAL_CODE AND iscrpay.VAL_SET_CODE='YN'\n\n WHERE le.LEDGER_ID = {?ledger_id?}\n","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}