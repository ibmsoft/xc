{"inframe":false,"title":"支付单成功之后信息查询","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT ep.EX_PAY_ID,\n       ep.DEPOSIT_BANK_ID,\n       ep.PAY_TYPE,gldb.DEPOSIT_BANK_NAME,\n       ep.CA_ID,\n       CONCAT(gci.CA_CODE, gci.CA_NAME) AS CA_DISPLAY_NAME\n  FROM xc_gl_ld_pr_modes glpm,\n       xc_ex_pay ep\n  left join xc_gl_ld_deposit_bank gldb on ep.LEDGER_ID = gldb.LEDGER_ID and ep.DEPOSIT_BANK_ID = gldb.DEPOSIT_BANK_ID\n  left join xc_gl_cash_items gci on ep.CA_ID = gci.CA_ID\n WHERE ep.EX_PAY_ID = {?EX_PAY_ID?}\n   AND ep.LEDGER_ID = glpm.LEDGER_ID\n   AND ep.PAY_TYPE = glpm.MODE_ID;\n","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":"seek_icon"}