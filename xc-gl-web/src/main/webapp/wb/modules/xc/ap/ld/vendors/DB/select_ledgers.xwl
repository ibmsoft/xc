{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT T.*\n  FROM XC_GL_LEDGERS T\n WHERE EXISTS (SELECT 1\n          FROM XC_GL_LD_SEC A\n         WHERE T.LEDGER_ID = A.LEDGER_ID\n           AND A.USER_ID = {?XIP.userId?}\n           AND A.ROLE_ID = {?XIP.roleId?})\n    AND T.IS_VALID = 'Y' AND (LEDGER_CODE like '%{#QUERY_CODE_NAME#}%' OR LEDGER_NAME like '%{#QUERY_CODE_NAME#}%')\nORDER BY LEDGER_CODE  limit {?bigint.start?},{?bigint.limit?}\n\n","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}