{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT\n\tp.project_id,\n\tconcat(a.dept_name,'-',p.project_name) as \"project_name\",\n\tp.project_code,\n\tppd.dept_id,\n\ta.dept_name\nFROM\n\txc_pm_prj_dept ppd\nright join xc_pm_projects p ON p.project_id = ppd.project_id\nleft join xip_pub_depts a on ppd.dept_id = a.dept_id\nWHERE\np.ledger_id = {?ledgerId?}\nAND ppd.dept_id like '%{#deptId#}%'\nAND CURDATE() BETWEEN p.start_date AND p.end_date\nAND NOT EXISTS(SELECT a.project_id FROM xc_pm_projects a WHERE a.up_project_id = p.project_id)","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":"seek_icon"}