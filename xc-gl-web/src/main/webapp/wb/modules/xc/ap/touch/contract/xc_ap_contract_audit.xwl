{"inframe":true,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"serverScript":"//获取业务单据ID\nvar bizId = request.getParameter(\"businessId\");\nrequest.setAttribute('bizId',bizId);","itemId":"module"},"children":[{"configs":{"layout":"fit","itemId":"viewport1"},"children":[{"configs":{"layout":"fit","scrollable":"true","itemId":"container1"},"children":[{"configs":{"itemId":"content"},"events":{"initialize":"//业务单据ID\nvar bizId = '{#bizId#}';\n//合同信息赋值\nif(bizId!=''&&bizId!=null&&bizId!=underfined){\n  Wb.request({\n    url:'m?xwl=xc/ap/ct/data/getContractInfo',\n    params:{'contractId':bizId},\n    success:function(resp){\n      var rec = Wb.decode(resp.responseText);\n      if(rec.total>0){\n        Wb.request({\n          url:'m?xwl=xc/ap/ct/data/getContractInfo',\n          params:{'contractId':bizId},\n          success:function(resp){\n            var rec = Wb.decode(resp.responseText);\n            if(rec.total>0){\n              Wb.setValue(app.form1,rec.rows[0]);\n              app.START_DATE.setValue(rec.rows[0].start);\n              app.END_DATE.setValue(rec.rows[0].end);\n            }\n          }\n        });\n      }else{\n        Wb.request({\n          url:'m?xwl=xc/ap/ct/data/getContractHisInfoByHisId',\n          params:{'contractHisId':bizId},\n          success:function(resp){\n            var rec = Wb.decode(resp.responseText);\n            if(rec.total>0){\n              Wb.setValue(app.form1,rec.rows[0]);\n              app.START_DATE.setValue(rec.rows[0].start);\n              app.END_DATE.setValue(rec.rows[0].end);\n            }\n\n          }\n        });\n      }\n    }\n  });\n}\n\n"},"children":[{"configs":{"labelWidth":"100","readOnly":"true","label":"组织","labelAlign":"left","itemId":"ORG_NAME"},"children":[],"expanded":false,"type":"ttext"},{"configs":{"labelWidth":"100","readOnly":"true","label":"成本中心","labelAlign":"left","itemId":"DEPT_NAME"},"children":[],"expanded":false,"type":"ttext"},{"configs":{"label