{"inframe":false,"title":"合同查询","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"oracle":"SELECT\n\tAP_CONTRACT_ID,\n\tAP_CONTRACT_CODE,\n\tAP_CONTRACT_NAME,\n\tSTART_DATE,\n\tEND_DATE,\n\tVENDOR_ID\nFROM\n\txc_ap_contract\nWHERE\n\tSYSDATE BETWEEN START_DATE AND END_DATE\nAND LEDGER_ID = {?LEDGER_ID?}\nAND VENDOR_ID = {?VENDOR_ID?}\nORDER BY\n\tAP_CONTRACT_CODE","itemId":"sqlswitcher1","mysql":"SELECT\n\tac.AP_CONTRACT_ID,\n\tac.AP_CONTRACT_CODE,\n\tac.AP_CONTRACT_NAME,\n\tac.START_DATE,\n\tac.END_DATE,\n\tac.VENDOR_ID,\n\tav.VENDOR_NAME\nFROM\n\txc_ap_contract ac,\n\txc_ap_vendors av\nWHERE\n\tac.VENDOR_ID = av.VENDOR_ID\nAND CURDATE() BETWEEN ac.START_DATE AND ac.END_DATE\nAND ac.LEDGER_ID = {?LEDGER_ID?}\nAND ac.VENDOR_ID LIKE '%{#VENDOR_ID#}%'\nORDER BY\n\tac.AP_CONTRACT_CODE"},"children":[],"expanded":false,"type":"sqlswitcher"},{"configs":{"sql":"{#sqlswitcher1#}","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":"seek_icon"}