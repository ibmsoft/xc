{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"itemId":"module"},"children":[{"configs":{"sql":"SELECT \n  AP_INV_GL_H_CODE,\n  DESCRIPTION,\n  IFNULL(AMOUNT ,0) AMOUNT,\n  IFNULL(TAX_AMT ,0) TAX_AMT,\n  IFNULL(TAX_RATE ,0) TAX_RATE,\n  IFNULL(INV_AMOUNT ,0) INV_AMOUNT,\n  IFNULL(CANCEL_AMT,0) CANCEL_AMT,\n  (SELECT \n    IFNULL(SUM(TARGET_AMT) ,0)\n  FROM\n    xc_ap_cancel_l L \n  WHERE L.TARGET_ID = a.AP_INV_GL_H_ID) CANCEL_AMT_SUM,\n  PAID_AMT,\n  (SELECT \n    SUM(AMOUNT) \n  FROM\n    xc_ap_pay_l L \n  WHERE L.AP_INV_GL_H_ID = a.AP_INV_GL_H_ID) PAID_AMT_SUM,\n  NO_PAY_AMT,\n  NO_PAY_AMT - (\n    REQ_AMT - \n    (SELECT \n      SUM(l.AMOUNT) \n    FROM\n      xc_ap_pay_h h,\n      xc_ap_pay_l l \n    WHERE h.AP_PAY_H_ID = l.AP_PAY_H_ID \n      AND h.SOURCE = 'REQ' \n      AND l.AP_INV_GL_H_ID = a.AP_INV_GL_H_ID)\n  ) NO_PAY_AMT_SUM,\n  ADJ_AMT,\n  (SELECT \n    SUM(ADJ_AMT) \n  FROM\n    xc_ap_inv_gl_adj L \n  WHERE L.AP_INV_GL_H_ID = A.AP_INV_GL_H_ID) ADJ_AMT_SUM,\n  REQ_AMT,\n  (SELECT \n    SUM(AMOUNT) \n  FROM\n    xc_ap_pay_req_l L \n  WHERE L.AP_INV_GL_H_ID = A.AP_INV_GL_H_ID) REQ_AMT_SUM,\n  NO_REQ_AMT \nFROM\n  xc_ap_inv_gl_h a \nWHERE AP_INV_GL_H_CODE LIKE '%{#AP_INV_GL_H_CODE#}%' AND DESCRIPTION  LIKE '%{#DESCRIPTION#}%'","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}