{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"select * from (\nSELECT \n   a.*,\n   a.NO_PAY_AMT-a.OCCUPY_AMT as PAYABLE,\n   (SELECT AP_CAT_NAME FROM xc_ap_doc_cats b WHERE b.AP_CAT_CODE=a.AP_DOC_CAT_CODE) AP_DOC_CAT_NAME,\n   (SELECT AP_CONTRACT_CODE FROM xc_ap_contract b WHERE b.AP_CONTRACT_ID=a.AP_CONTRACT_ID) AP_CONTRACT_CODE,\n   (SELECT AP_CONTRACT_NAME FROM xc_ap_contract b WHERE b.AP_CONTRACT_ID=a.AP_CONTRACT_ID) AP_CONTRACT_NAME,\n   (SELECT DEPT_NAME FROM xip_pub_depts b WHERE b.DEPT_ID=a.DEPT_ID) DEPT_NAME,\n   (SELECT PROJECT_NAME FROM xc_pm_projects b WHERE b.PROJECT_ID=a.PROJECT_ID) PROJECT_NAME,\n   (SELECT ACC_NAME FROM xc_gl_accounts b WHERE b.acc_id=a.AP_ACC_ID_DEBT) AP_ACC_NAME_DEBT,\n   (SELECT EMP_NAME FROM XIP_PUB_EMPS WHERE EMP_ID=(SELECT EMP_ID FROM XIP_PUB_USERS b WHERE b.USER_ID=a.CREATED_BY)) CREATED_BY_NAME\nFROM\n  xc_ap_inv_gl_h  a where a.V_STATUS=3 and IFNULL(a.NO_PAY_AMT,'')<>0\n\n) t \nwhere \nPAYABLE>0\nand\nledger_id like '%{#ledgerId#}%'\nand\nvendor_id like '%{#vendorId#}%'\nand\n(ap_contract_id = '{#contractId#}' or ap_contract_id is null)\nand\nap_inv_gl_h_code like '%{#invGlHCode#}%'\nor\nCREATED_BY_NAME like '%{invGlHCode}%'\norder by\nCREATION_DATE desc\n","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":"seek_icon"}