{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module "},"children":[{"configs":{"sql":"SELECT \n  c.CUSTOMER_ID AS CUSTOMER_ID_C,\n  c.CUSTOMER_CODE AS CUSTOMER_CODE,\n  c.CUSTOMER_NAME AS CUSTOMER_NAME,\n  t.VENDOR_ID,\n  t.VENDOR_CODE,\n  t.VENDOR_NAME,\n  t.TAX_NO,\n  t.ADDRESS,\n  t.VENDOR_TYPE,\n  t.CONTACT,\n  t.TEL,\n  t.MOBILE,\n  t.QQ,\n  t.WECHAT,\n  t.EMAIL,\n  t.CUSTOMER_ID,\n  t.START_DATE,\n  t.END_DATE,\n  t.CREATION_DATE,\n  t.CREATED_BY,\n  t.LAST_UPDATE_DATE,\n  t.LAST_UPDATED_BY \nFROM\n  xc_ap_vendors t \n  LEFT JOIN xc_ar_customers c \n    ON t.CUSTOMER_ID = c.CUSTOMER_ID \nWHERE (\n    VENDOR_CODE LIKE  '%{#codeName#}%'\n    OR VENDOR_NAME LIKE  '%{#codeName#}%'\n  ) \nORDER BY VENDOR_CODE ASC \n\n\n","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}