{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"itemId":"module"},"children":[{"configs":{"sql":"SELECT \n  a.*,\n  b.LEDGER_NAME,\n  b.LEDGER_CODE,\n  e.VENDOR_NAME,\n  e.VENDOR_CODE,\n  i.PROJECT_NAME,\n  g.DEPT_NAME,\n  m.EMP_NAME,\n  (select h.VAL_NAME from xip_pub_val_set_dtl h where a.CONTRACT_CLASSIFY = h.VAL_CODE and h.VAL_SET_CODE = 'XC_PO_CONTRACT_TYPE') as 'CONTRACT_TYPE_NAME',\n  k.ORG_NAME,\n  m.EMP_ID\nFROM\n  xc_ap_contract a LEFT JOIN XC_GL_LEDGERS b ON  a.LEDGER_ID=b.LEDGER_ID\n  LEFT JOIN XC_AP_VENDORS e ON a.VENDOR_ID = e.VENDOR_ID \n  LEFT JOIN xc_pm_projects i ON i.project_id = a.project_id\n  LEFT JOIN xip_pub_depts g ON g.DEPT_ID = a.DEPT_ID\n  LEFT JOIN xip_pub_emps m ON m.EMP_ID = a.CONTRACT_USER\n  LEFT JOIN xip_pub_orgs k ON a.ORG_ID = k.ORG_ID\n  WHERE  a.ORG_ID = '{#orgId#}'\n     AND  IFNULL(a.IS_CLOSE,'') like  '%{#isClose#}%'\n    AND  ( a.AP_CONTRACT_NAME LIKE  '%{#contractName#}%'\n    or a.AP_CONTRACT_CODE LIKE  '%{#contractName#}%')\n\tAND a.AUDIT_STATUS like '%{#auditStatus#}%'\n\tAND a.SYS_AUDIT_STATUS like '%{#sysAuditStatus#}%'\n\tAND a.AP_CONTRACT_TYPE like '%{#contractType#}%'\n\tAND a.CONTRACT_CLASSIFY like '%{#contractClassify#}%'\n\tAND if('{#startDate#}'='','1=1',a.CREATION_DATE>='{#startDate#}')\n    AND if('{#endDate#}'='','1=1',a.CREATION_DATE<='{#endDate#}')\norder by a.CREATION_DATE","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}