on() {\n    var invoiceArray = [];\n    for(var i = 0;i < recs.length;i++){\n        var cancelData =  {AP_INV_H_ID : recs[i].data.AP_INV_H_ID,AP_INV_GL_H_ID : recs[i].data.AP_INV_GL_H_ID};\n        invoiceArray.push(cancelData);\n    }\n    //调用删除销售发票的后台程序\n    Wb.request({\n        url : '{#contextPath#}' + '/apDocsAction.do?method=cancelFinApInvGl',\n        params : Wb.apply({apInfo: invoiceArray}),\n        success : function(resp){\n            var record = Wb.decode(resp.responseText);\n            var msg = record.msg;\n            Wb.toast(msg);\n            app.queryBtn.fireEvent('click');\n        }\n    });\n});"},"children":[],"expanded":false,"type":"button"},{"configs":{"text":"{#verfyBtn#}","itemId":"verfyBtn"},"events":{"click":"var rows = app.grid1.getSelection();\nif(rows.length === 0){\n    Wb.toast('{#verfyRec#}');\n    return;\n}\nelse{\n    //单据ID数组\n    var docArray = [];\n    var i = 0;//判断是否生成凭证\n    var j = 0;//判断是否审核通过\n    Wb.each(rows,function(r){\n        var apId = r.data.AP_INV_GL_H_ID;//单据ID\n        var headId = r.data.V_HEAD_ID;//凭证头ID\n        if(headId === '' || headId === null){\n            i++;\n        }\n        if(r.data.V_STATUS == '1'){\n            var paramJson = {'headId' : headId,'apId':apId,apCatCode:'YFD'} ;\n            docArray.push(paramJson) ;\n        }\n    });\n    if(i > 0){\n        Wb.toast('{#verfyRec1#}');\n        return;\n    }\n    if(docArray.length === 0){\n        Wb.toast('{#verfyRec2#}');\n        return;\n    }\n    Wb.request({\n        url: '{#contextPath#}'+'/apVoucherAction.do?method=checkVoucher',\n        params: Wb.apply({apInfo: docArray}),\n        success: function(resp) {\n            var record = Wb.decode(resp.responseText);\n            var msg = record.msg;\n            app.queryBtn.fireEvent('click');\n            Wb.toast(msg);\n        }\n\t});\n}"},"children":[],"expanded":false,"type":"button"},{"configs":{"text":"{#cancelVerfyBtn#}","itemId":"cancelVerfyBtn"},"events":{"click":"var rows = app.grid1.getSelection();\nif (rows.length === 0) {\n    Wb.toast('{#cancelVerfyRec#}');\n    return;\n}\nelse{\n    //单据ID数组\n    var docArray = [];\n    var z = 0;\n    for(var i = 0;i < rows.length;i++){\n        var apId = rows[i].data.AP_INV_GL_H_ID;//单据ID\n        var headId = rows[i].data.V_HEAD_ID;//凭证头ID\n        if(headId === '' || headId === null){\n            z++;\n        }\n        if(rows[i].data.V_STATUS == '3'){\n            var paramJson = {'headId' : headId,'apId':apId,apCatCode:'YFD'} ;\n            docArray.push(paramJson);\n        }\n    }\n    if(z > 0){\n        Wb.toast('{#verfyRec2#}');\n        return;\n    }\n    if(docArray.length === 0){\n        Wb.toast('{#cancelVerfyRec2#}');\n        return;\n    }\n    Wb.request({\n        url: '{#contextPath#}' + '/apVoucherAction.do?method=cancelCheckVoucher',\n        params: Wb.apply({apInfo: docArray}),\n        success: function(resp) {\n            var record = Wb.decode(resp.responseText);\n            var msg = record.msg;\n            app.queryBtn.fireEvent('click');\n            Wb.toast(msg);\n        }\n    });\n}"},"children":[],"expanded":false,"type":"button"},{"configs":{"text":"{#seekBtn#}","itemId":"seekBtn","iconCls":"seek_icon"},"events":{"click":"var curGrid = app.grid1;\nvar rec = curGrid.getSelection();\nif (rec.length > 1 || rec.length === 0) {//大于一条或没选择会警告\n    Wb.toast('{#seekRec#}');\n    return;\n}\nWb.run({\n  url: 'm?xwl=xc/ap/gl/XC_AP_INV_GL_H_NO_C', \n  params : {'businessId' : rec[0].data.AP_INV_H_ID},\n  success: function(apps) {\n    apps.initWin({\n\t\tAP_INV_GL_H_ID:rec[0].data.AP_INV_GL_H_ID,\n        IS_INSERT:'S',\n        AP_DOC_CAT_CODE:rec[0].data.AP_DOC_CAT_CODE,\n        hideFun:function(){\n          app.queryBtn.fireEvent('click');\n        }\n    });\n  }\n});"},"children":[],"expanded":false,"type":"button"},{"configs":{"text":"{#cancelBtn#}","itemId":"cancelBtn"},"events":{"click":"var curGrid = app.grid1;\nvar r = curGrid.getSelectionModel().getSelection()[0];\nvar rec = curGrid.getSelection();\nif (rec.length > 1 || rec.length === 0) {//大于一条或没选择会警告\n    Wb.toast('{#cancelRec#}');\n    return;\n}\nif (Wb.isEmpty(r.get('V_STATUS')) || parseInt(r.get('V_STATUS'))<3) {\n    Wb.toast('{#cancelRec1#}');\n    return;\n}\nvar AP_INV_H_ID = r.get('AP_INV_H_ID');//发票ID\nif(r.get('CANCEL_AMT') !== null && r.get('CANCEL_AMT') !== '' && r.get('CANCEL_AMT') !== 0){\n\tAP_INV_H_ID = null;\n}\nWb.request({\n    url: 'm?xwl=xc/ap/gl/DB/XC_AP_INV_GL_H_PUB_ERROR_ALERT',\n    params: {\n        AP_INV_GL_H_ID: r.get('AP_INV_GL_H_ID'),\n        sql: 'select 1 FROM xc_ap_inv_gl_h WHERE AP_INV_GL_H_ID={?AP_INV_GL_H_ID?} AND NO_PAY_AMT=0',\n        errorMsg: '{#cancelRec2#}'\n    },\n    success : function(){\n        Wb.run({\n            url: 'm?xwl=xc/ap/cancel/XC_AP_CANCEL_H_NO_C', \n            success: function(apps) {\n                apps.initWin({\n                    AP_CANCEL_H_ID:'',\n                    AP_INV_GL_H_ID:r.get('AP_INV_GL_H_ID'),\n                    AP_INV_H_ID:AP_INV_H_ID,\n                    IS_INSERT:'Y',\n                    flag:'cancel',\n                    AP_DOC_CAT_CODE:r.get('AP_DOC_CAT_CODE'),\n                    AP_CANCEL_TYPE:'YFHYUF',\n                    VENDOR_NAME:r.get('VENDOR_NAME'),\n                    VENDOR_ID:r.get('VENDOR_ID'),\n                    hideFun:function(){\n                        app.queryBtn.fireEvent('click');\n                    }\n                });\n            }\n        });\n    }\n});"},"children":[],"expanded":false,"type":"button"},{"configs":{"text":"{#payBtn#}","itemId":"payBtn"},"events":{"click":"var curGrid = app.grid1;\nvar r = curGrid.getSelectionModel().getSelection()[0];\nvar ap_doc_cat_code = 'FKD_CGJS';\nvar rec = curGrid.getSelection();\nif (rec.length > 1 || rec.length === 0) {//大于一条或没选择会警告\n    Wb.toast('{#payRec#}');\n    return;\n}\nif (Wb.isEmpty(r.get('V_STATUS')) || parseInt(r.get('V_STATUS'))<3) {\n    Wb.toast('{