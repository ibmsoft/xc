{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT \n  a.*,\n  b.LEDGER_NAME,\n  b.LEDGER_CODE,\n  e.VENDOR_NAME,\n  e.VENDOR_CODE,\n  i.PROJECT_NAME,\n  g.DEPT_NAME,\n  m.EMP_NAME,\n  o.CONTRACT_TYPE_NAME,\n  k.ORG_NAME,\n  m.EMP_ID,\n  h.VAL_NAME as 'AP_CONTRACT_TYPE_NAME',\n date_format(a.START_DATE,'%Y-%m-%d') as 'start',\n date_format(a.END_DATE,'%Y-%m-%d') as 'end'\nFROM\n  xc_ap_contract a LEFT JOIN XC_GL_LEDGERS b ON  a.LEDGER_ID=b.LEDGER_ID\n  LEFT JOIN XC_AP_VENDORS e ON a.VENDOR_ID = e.VENDOR_ID \n  LEFT JOIN xc_pm_projects i ON i.project_id = a.project_id\n  LEFT JOIN xip_pub_depts g ON g.DEPT_ID = a.DEPT_ID\n  LEFT JOIN xip_pub_emps m ON m.EMP_ID = a.CONTRACT_USER\n  LEFT JOIN xc_po_contract_type  o ON a.CONTRACT_CLASSIFY = o.CONTRACT_TYPE_ID\n  LEFT JOIN xip_pub_orgs k ON a.ORG_ID = k.ORG_ID\n  LEFT JOIN xip_pub_val_set_dtl h ON a.AP_CONTRACT_TYPE = h.VAL_CODE\n  WHERE \n\ta.AP_CONTRACT_ID = '{#contractId#}'","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}