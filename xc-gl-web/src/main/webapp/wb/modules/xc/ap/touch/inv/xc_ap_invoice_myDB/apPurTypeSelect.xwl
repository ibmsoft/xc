{"inframe":false,"title":"采购类型查询","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT\n\tapt.AP_PUR_TYPE_ID,\n\tapt.AP_PUR_TYPE_NAME,\n\talpt.LEDGER_ID,\n\talpt.BG_ITEM_ID,\n\tbi.BG_ITEM_NAME,\n\talpt.AP_ACC_ID_INV,\n\tga.ACC_NAME\nFROM\n\txc_ap_ld_pur_type alpt\nLEFT JOIN xc_ap_pur_type apt ON alpt.AP_PUR_TYPE_ID = apt.AP_PUR_TYPE_ID\nLEFT JOIN xc_bg_items bi ON alpt.BG_ITEM_ID = bi.BG_ITEM_ID\nLEFT JOIN xc_gl_accounts ga ON alpt.AP_ACC_ID_INV = ga.ACC_ID\nWHERE\n\talpt.LEDGER_ID = {?LEDGER_ID?}\nAND apt.IS_SYSTEM_AP = '1'\nAND apt.AP_PUR_TYPE_NAME LIKE '%{#AP_PUR_TYPE_NAME#}%'","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":"seek_icon"}