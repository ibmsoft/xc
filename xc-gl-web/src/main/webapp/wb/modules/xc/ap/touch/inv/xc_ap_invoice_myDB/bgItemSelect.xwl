{"inframe":false,"title":"预算项目查询","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"oracle":"SELECT\n\tbi.BG_ITEM_ID,\n\tbi.BG_ITEM_CODE,\n\tbi.BG_ITEM_NAME,\n\tbli.BG_LD_ITEM_ID\nFROM\n\txc_bg_ld_items bli,\n\txc_bg_items bi\nWHERE\n\tbli.BG_ITEM_ID = bi.BG_ITEM_ID\nAND bli.LEDGER_ID = {?LEDGER_ID?}\nAND SYSDATE BETWEEN bi.START_DATE AND bi.END_DATE)\nAND NOT EXISTS(SELECT a.BG_ITEM_ID FROM xc_bg_items a WHERE a.BG_ITEM_UP_ID = bi.BG_ITEM_ID)\nAND bli.IS_ENABLED = 'Y'","itemId":"sqlswitcher1","mysql":"SELECT\n\tapt.AP_PUR_TYPE_NAME,\n\talpt.BG_ITEM_ID,\n\tbi.BG_ITEM_NAME\nFROM\n\txc_ap_pur_type apt,\n\txc_ap_ld_pur_type alpt\nLEFT JOIN xc_bg_items bi ON alpt.BG_ITEM_ID = bi.BG_ITEM_ID\nWHERE\n\tapt.AP_PUR_TYPE_ID = alpt.AP_PUR_TYPE_ID\nAND apt.AP_PUR_TYPE_ID = {?AP_PUR_TYPE_ID?}\nAND alpt.LEDGER_ID = {?LEDGER_ID?}"},"children":[],"expanded":false,"type":"sqlswitcher"},{"configs":{"sql":"{#sqlswitcher1#}","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":"seek_icon"}