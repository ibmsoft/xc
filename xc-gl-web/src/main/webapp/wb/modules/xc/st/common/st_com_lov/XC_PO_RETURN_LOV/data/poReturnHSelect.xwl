{"inframe":false,"title":"选择退货通知单","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT\n\tprh.*,\n\tpoh.ORDER_H_CODE,\n\tpoh.ORDER_H_NAME,\n\tav.VENDOR_CODE,\n\tav.VENDOR_NAME,\n\tac.AP_CONTRACT_CODE,\n\tac.AP_CONTRACT_NAME,\n\tpd.DEPT_NAME\nFROM\n\txc_po_return_h prh\nLEFT JOIN xc_po_order_h poh ON prh.ORDER_H_ID = poh.ORDER_H_ID\nLEFT JOIN xc_ap_vendors av ON prh.VENDOR_ID = av.VENDOR_ID\nLEFT JOIN xc_ap_contract ac ON poh.CONTRACT_ID = ac.AP_CONTRACT_ID\nLEFT JOIN xip_pub_depts pd ON prh.DEPT_ID = pd.DEPT_ID\nWHERE\n\tprh.ORG_ID = {?QUERY_ORG_ID?}\nAND prh.LEDGER_ID = {?QUERY_LEDGER_ID?}\nAND av.VENDOR_ID LIKE '%{#QUERY_VENDOR_ID#}%'\nAND poh.ORDER_H_ID LIKE '%{#QUERY_ORDER_ID#}%'\nAND (prh.RETURN_H_CODE LIKE '%{#QUERY_CODE_NAME#}%' OR prh.RETURN_H_NAME LIKE '%{#QUERY_CODE_NAME#}%')\nAND prh.WAREHOUSE_ID = {?QUERY_WAREHOUSE_ID?}\nAND prh.SYS_AUDIT_STATUS = 'E'\nORDER BY prh.RETURN_H_CODE DESC","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}