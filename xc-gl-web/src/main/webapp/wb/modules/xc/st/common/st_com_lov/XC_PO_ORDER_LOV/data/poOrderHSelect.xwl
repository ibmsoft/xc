{"inframe":false,"title":"选择采购订单","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT\n\tpoh.*,\n\tav.VENDOR_CODE,\n\tav.VENDOR_NAME,\n\tac.AP_CONTRACT_CODE,\n\tac.AP_CONTRACT_NAME,\n\tpd.DEPT_NAME,\n\tpp.PROJECT_NAME\nFROM\n\txc_po_order_h poh\nLEFT JOIN xc_ap_vendors av ON poh.VENDOR_ID = av.VENDOR_ID\nLEFT JOIN xc_ap_contract ac ON poh.CONTRACT_ID = ac.AP_CONTRACT_ID\nLEFT JOIN xip_pub_depts pd ON poh.DEPT_ID = pd.DEPT_ID\nLEFT JOIN xc_pm_projects pp ON poh.PROJECT_ID = pp.PROJECT_ID\nWHERE\n\tpoh.ORG_ID = {?QUERY_ORG_ID?}\nAND poh.LEDGER_ID = {?QUERY_LEDGER_ID?}\nAND av.VENDOR_ID LIKE '%{#QUERY_VENDOR_ID#}%'\nAND (poh.ORDER_H_CODE LIKE '%{#QUERY_CODE_NAME#}%' OR poh.ORDER_H_NAME LIKE '%{#QUERY_CODE_NAME#}%')\nAND poh.SYS_AUDIT_STATUS = 'E'\nAND poh.IS_CLOSE = 'N'\nORDER BY poh.ORDER_H_CODE DESC","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}