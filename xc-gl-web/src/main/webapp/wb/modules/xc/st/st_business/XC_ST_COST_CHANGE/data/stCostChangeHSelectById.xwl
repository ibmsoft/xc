{"inframe":false,"title":"库存成本调整单主表查询","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT\n\tscch.*,\n\tgl.LEDGER_NAME,\n\tpo.ORG_NAME,\n\tsbt.BUSINESS_TYPE_CODE,\n\tsbt.BUSINESS_TYPE_NAME,\n\tsw.WAREHOUSE_CODE,\n\tsw.WAREHOUSE_NAME,\n\tpe.EMP_NAME AS \"ADJUST_PERSON\",\n\tsar.ADJUST_REASON\nFROM\n\txc_st_cost_change_h scch\nLEFT JOIN xc_gl_ledgers gl ON scch.LEDGER_ID = gl.LEDGER_ID\nLEFT JOIN xip_pub_orgs po ON scch.ORG_ID = po.ORG_ID\nLEFT JOIN xc_st_business_type sbt ON scch.BUSINESS_TYPE_ID = sbt.BUSINESS_TYPE_ID\nLEFT JOIN xc_st_warehouse sw ON scch.WAREHOUSE_ID = sw.WAREHOUSE_ID\nLEFT JOIN xip_pub_users pu ON scch.PERSON_ID = pu.USER_ID\nLEFT JOIN xip_pub_emps pe ON pu.EMP_ID = pe.EMP_ID\nLEFT JOIN xc_st_adjust_reason sar ON scch.ADJUST_REASON_ID = sar.ADJUST_REASON_ID\nWHERE scch.COST_CHANGE_H_ID = {?COST_CHANGE_H_ID?}","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}