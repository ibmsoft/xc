{"inframe":false,"title":"账簿会计日期查询","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"oracle":"SELECT\n\tMIN(gp.START_DATE) AS START_DATE,\n\tgp.END_DATE,\n\tslp.ST_PERIODS,\n\tslp.PERIODS_STATUS,\n\tgp.IS_ENABLED,\n\tADD_MONTHS(TO_DATE(gp.END_DATE,'yy-mm-dd'),-1) LAST_END_DATE\nFROM\n\txc_st_ld_periods slp,\n\txc_gl_period gp\nWHERE\n\tslp.LEDGER_ID = {?LEDGER_ID?}\nAND slp.ST_PERIODS = gp.PERIOD_CODE\nAND slp.PERIODS_STATUS = '1'\nAND gp.IS_ENABLED = '1';","itemId":"sqlswitcher1","mysql":"SELECT\n\tMIN(gp.START_DATE) AS START_DATE,\n\tgp.END_DATE,\n\tslp.ST_PERIODS,\n\tslp.PERIODS_STATUS,\n\tgp.IS_ENABLED,\n\tDATE_SUB(gp.END_DATE,INTERVAL 1 MONTH) LAST_END_DATE\nFROM\n\txc_st_ld_periods slp,\n\txc_gl_period gp\nWHERE\n\tslp.LEDGER_ID = {?LEDGER_ID?}\nAND slp.ST_PERIODS = gp.PERIOD_CODE\nAND slp.PERIODS_STATUS = '1'\nAND gp.IS_ENABLED = '1';"},"children":[],"expanded":false,"type":"sqlswitcher"},{"configs":{"sql":"{#sqlswitcher1#}","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":"seek_icon"}