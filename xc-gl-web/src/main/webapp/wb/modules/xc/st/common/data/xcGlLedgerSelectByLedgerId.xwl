{"inframe":false,"title":"根据账簿ID查询账簿信息","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT\n\tgl.*,\n    (select i.ACC_NAME from xc_gl_accounts i where i.ACC_ID = gl.ST_ACC_ID_DIFF) ST_ACC_ID_DIFF_NAME,\n    (select i.ACC_NAME from xc_gl_accounts i where i.ACC_ID = gl.ST_ACC_ID_ZG) ST_ACC_ID_ZG_NAME,\n    (SELECT\n        CASE\n            WHEN l.VAL_NAME IS NULL OR l.VAL_NAME = '' THEN\n                d.VAL_NAME\n            ELSE\n                l.VAL_NAME\n        END VAL_NAME\n    FROM\n        xip_pub_val_set_dtl d\n    LEFT JOIN xip_pub_val_set_dtl_lang l ON d.VAL_SET_CODE = l.VAL_SET_CODE AND d.VAL_CODE = l.VAL_CODE AND l.VAL_SET_LANG = '{#language#}'\n    WHERE\n        d.VAL_SET_CODE = 'XC_ST_COST_METHOD'\n    AND d.ENABLE_FLAG = 'Y'\n    AND d.VAL_CODE = gl.ST_COST_METHOD) ST_COST_METHOD_NAME,\n    (select i.MAT_TYPE_SET_NAME from xc_st_mat_type_set i where i.MAT_TYPE_SET_ID = gl.MAT_TYPE_SET_ID) MAT_TYPE_SET_NAME\nFROM\n\txc_gl_ledgers gl\nWHERE\n\tgl.LEDGER_ID = {?LEDGER_ID?}","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}