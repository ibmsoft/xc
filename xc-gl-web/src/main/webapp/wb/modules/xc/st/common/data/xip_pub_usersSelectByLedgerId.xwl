{"title":"获取用户根据账簿","inframe":false,"hidden":false,"roles":{"default":1},"pageLink":"","children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT\n\tpu.USER_ID,\n\tpu.USER_NAME,\n\tpe.EMP_ID,\n\tpe.EMP_CODE,\n\tpe.EMP_NAME,\n\tgle.LEDGER_ID\nFROM\n\txip_pub_users pu\nLEFT JOIN xip_pub_emps pe ON pu.EMP_ID = pe.EMP_ID\nLEFT JOIN xc_gl_ld_emps gle ON pe.EMP_ID = gle.EMP_ID\nWHERE\n\tgle.LEDGER_ID = {?QUERY_LEDGER_ID?}\nAND (pu.USER_NAME LIKE '%{#QUERY_CODE_NAME#}%' OR pe.EMP_CODE LIKE '%{#QUERY_CODE_NAME#}%' OR pe.EMP_NAME LIKE '%{#QUERY_CODE_NAME#}%')\nORDER BY pe.EMP_CODE","itemId":"dataprovider1"},"children":[],"type":"dataprovider","expanded":false}],"type":"module","expanded":true}],"iconCls":""}