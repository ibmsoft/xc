{"inframe":false,"title":"成本核算方法查询（值集）","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","initScript":"var lang = request.getSession().getAttribute(\"XzSessionVars\").get('language'); request.setAttribute(\"language\",lang);","itemId":"module"},"children":[{"configs":{"sql":"SELECT\n\tCASE\n\t\tWHEN l.VAL_NAME IS NULL OR l.VAL_NAME = '' THEN\n\t\t\td.VAL_NAME\n\t\tELSE\n\t\t\tl.VAL_NAME\n\tEND VAL_NAME,\n\td.VAL_CODE\nFROM\n\txip_pub_val_set_dtl d\nLEFT JOIN xip_pub_val_set_dtl_lang l ON d.VAL_SET_CODE = l.VAL_SET_CODE AND d.VAL_CODE = l.VAL_CODE AND l.VAL_SET_LANG = '{#language#}'\nWHERE\n\td.VAL_SET_CODE = 'XC_ST_COST_METHOD'\nAND d.ENABLE_FLAG = 'Y'","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}