{"inframe":false,"title":"货位树形查询","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT * FROM (\n\tSELECT\n\t\tsl.LOCATION_ID \"id\",\n\t\tsl.LOCATION_CODE \"code\",\n\t\tsl.LOCATION_NAME \"text\",\n\t\tsl.WAREHOUSE_ID \"warehouse_id\",\n\t\tsl.UPPER_ID \"parent_id\",\n\t\tsl.STATUS,\n\t\tCASE WHEN (sl.IS_LAST = 'Y')\n\t\tTHEN\n\t\t\t'true'\n\t\tELSE\n\t\t\t'false'\n\t\tEND AS \"leaf\",\n\t\t(SELECT t1.LOCATION_NAME FROM xc_st_location t1 WHERE t1.LOCATION_ID = sl.UPPER_ID) AS \"UP_TYPE\"\n\tFROM\n\t\txc_st_location sl\n\tWHERE sl.UPPER_ID = {?parent_id?}\n\tAND sl.WAREHOUSE_ID = {?WAREHOUSE_ID?}\n\t{#whereSql#}\n) sl1\nORDER BY sl1.leaf","itemId":"dataprovider1","type":"tree"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}