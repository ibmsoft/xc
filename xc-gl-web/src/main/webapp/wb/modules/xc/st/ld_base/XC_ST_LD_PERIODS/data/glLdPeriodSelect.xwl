{"inframe":false,"title":"未同步的会计期查询","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","serverScript":"var lang = request.getSession().getAttribute(\"XzSessionVars\").get('language');\nrequest.setAttribute(\"language\",lang);","itemId":"module"},"children":[{"configs":{"sql":"SELECT\n\tlp.LEDGER_ID,\n\tlp.PERIOD_CODE,\n\tlp.LD_PERIOD_STATUS,\n\t(SELECT\n       CASE WHEN l.VAL_NAME IS NULL OR l.VAL_NAME = '' THEN d.VAL_NAME ELSE l.VAL_NAME END DESCRIPTION\n     FROM\n       xip_pub_val_set_dtl d\n     LEFT JOIN XIP_PUB_VAL_SET_DTL_LANG l ON d.VAL_SET_CODE = l.VAL_SET_CODE\n     AND d.VAL_CODE = l.VAL_CODE\n     AND l.VAL_SET_LANG = '{#language#}'\n     WHERE\n       d.VAL_SET_CODE = 'XC_GL_PERIOD_STATUS'\n     AND d.ENABLE_FLAG = 'Y'\n     AND d.VAL_CODE = lp.LD_PERIOD_STATUS) AS LD_PERIOD_STATUS_NAME,\n\tp.YEAR,\n\tp.QUARTER,\n\tp.START_DATE,\n\tp.END_DATE,\n\tp.IS_ADJ_PERIOD\nFROM\n\txc_gl_period p\nLEFT JOIN xc_gl_ld_period lp ON p.PERIOD_CODE = lp.PERIOD_CODE\nWHERE lp.LEDGER_ID = {?LEDGER_ID?}\nAND lp.PERIOD_CODE NOT IN (SELECT b.ST_PERIODS FROM xc_st_ld_periods b WHERE b.LEDGER_ID = {?LEDGER_ID?})\nAND lp.LD_PERIOD_STATUS = '1'\nORDER BY p.START_DATE desc","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}