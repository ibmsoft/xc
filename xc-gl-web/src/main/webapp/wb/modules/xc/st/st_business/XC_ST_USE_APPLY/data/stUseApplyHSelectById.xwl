{"inframe":false,"title":"领用申请单主表查询","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT\n\tsuah.*,\n\tgl.LEDGER_NAME,\n\tpo.ORG_NAME,\n\tsbt.BUSINESS_TYPE_CODE,\n\tsbt.BUSINESS_TYPE_NAME,\n\tsw.WAREHOUSE_CODE,\n\tsw.WAREHOUSE_NAME,\n\tpd.DEPT_NAME,\n\tpp.PROJECT_NAME,\n\tpe.EMP_NAME AS \"APPLY_PERSON\"\nFROM\n\txc_st_use_apply_h suah\nLEFT JOIN xc_gl_ledgers gl ON suah.LEDGER_ID = gl.LEDGER_ID\nLEFT JOIN xip_pub_orgs po ON suah.ORG_ID = po.ORG_ID\nLEFT JOIN xc_st_business_type sbt ON suah.BUSINESS_TYPE_ID = sbt.BUSINESS_TYPE_ID\nLEFT JOIN xc_st_warehouse sw ON suah.WAREHOUSE_ID = sw.WAREHOUSE_ID\nLEFT JOIN xip_pub_depts pd ON suah.DEPT_ID = pd.DEPT_ID\nLEFT JOIN xc_pm_projects pp ON suah.PROJECT_ID = pp.PROJECT_ID\nLEFT JOIN xip_pub_users pu ON suah.APPLY_PERSON_ID = pu.USER_ID\nLEFT JOIN xip_pub_emps pe ON pu.EMP_ID = pe.EMP_ID\nWHERE suah.USE_APPLY_H_ID = {?USE_APPLY_H_ID?}","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}