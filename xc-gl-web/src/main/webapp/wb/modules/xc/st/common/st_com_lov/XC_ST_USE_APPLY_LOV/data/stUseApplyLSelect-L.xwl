{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"initScript":"var lang = request.getSession().getAttribute(\"XzSessionVars\").get('language');\nrequest.setAttribute(\"language\",lang);","loginRequired":"false","serverScript":"var sql,data = app.get();\nsql = ' WHERE 1 = 1';\nif (!Wb.isEmpty(data.USE_APPLY_H_ID)) {\n    sql+=' AND sual.USE_APPLY_H_ID = {?USE_APPLY_H_ID?}';\n}\nif (!Wb.isEmpty(data.LEDGER_ID)) {\n    sql+=' AND sual.LEDGER_ID = {?LEDGER_ID?}';\n}\nif (!Wb.isEmpty(data.MATERIAL_CODE_NAME)) {\n    sql+=' AND (sm.MATERIAL_CODE LIKE \"%'+data.MATERIAL_CODE_NAME+'%\" OR sm.MATERIAL_NAME LIKE \"%'+data.MATERIAL_CODE_NAME+'%\")';\n}\nrequest.setAttribute('sql', sql);","itemId":"module"},"children":[{"configs":{"oracle":"SELECT\n\tsual.*,\n\tsm.MATERIAL_CODE,\n\tsm.MATERIAL_NAME,\n\tsm.SPECIFICATION,\n\tgd.DIM_NAME,\n\t(SELECT\n       CASE WHEN l.VAL_NAME IS NULL OR l.VAL_NAME = '' THEN d.VAL_NAME ELSE l.VAL_NAME END DESCRIPTION\n     FROM\n       xip_pub_val_set_dtl d\n     LEFT JOIN XIP_PUB_VAL_SET_DTL_LANG l ON d.VAL_SET_CODE = l.VAL_SET_CODE\n     AND d.VAL_CODE = l.VAL_CODE\n     AND l.VAL_SET_LANG = '{#language#}'\n     WHERE\n       d.VAL_SET_CODE = 'XC_ST_MATERIAL_PROPERTY'\n     AND d.ENABLE_FLAG = 'Y'\n     AND d.VAL_CODE = sm.MATERIAL_PRO) AS MATERIAL_PRO_NAME,\n    smt.MATERIAL_TYPE_NAME,\n\tswi.ENTRY_H_ID,\n\tswi.ENTRY_L_ID,\n\tswi.QTY AS \"XCSL\",\n\tswi.AMOUNT AS \"JCCB\"\nFROM\n\txc_st_use_apply_l sual\nLEFT JOIN xc_st_use_apply_h suah ON sual.USE_APPLY_H_ID = suah.USE_APPLY_H_ID\nLEFT JOIN xc_st_material sm ON sual.MATERIAL_ID = sm.MATERIAL_ID\nLEFT JOIN xc_st_material_type smt ON sm.MATERIAL_TYPE_ID = smt.MATERIAL_TYPE_ID\nLEFT JOIN xc_gl_dimensions gd ON sual.DIM_ID = gd.DIM_ID\nLEFT JOIN xc_st_wh_inventory swi ON (sual.MATERIAL_ID = swi.MATERIAL_ID AND suah.WAREHOUSE_ID = swi.WAREHOUSE_ID)\n{#sql#}\nAND (NVL(sual.QTY,0) - IFNULL(sual.DELEVERED_QTY,0) > 0)\nAND swi.QTY > 0\nORDER BY sual.USE_APPLY_L_CODE","itemId":"sqlswitcher1","mysql":"SELECT\n\tsual.*,\n\tsm.MATERIAL_CODE,\n\tsm.MATERIAL_NAME,\n\tsm.SPECIFICATION,\n\tgd.DIM_NAME,\n\t(SELECT\n       CASE WHEN l.VAL_NAME IS NULL OR l.VAL_NAME = '' THEN d.VAL_NAME ELSE l.VAL_NAME END DESCRIPTION\n     FROM\n       xip_pub_val_set_dtl d\n     LEFT JOIN XIP_PUB_VAL_SET_DTL_LANG l ON d.VAL_SET_CODE = l.VAL_SET_CODE\n     AND d.VAL_CODE = l.VAL_CODE\n     AND l.VAL_SET_LANG = '{#language#}'\n     WHERE\n       d.VAL_SET_CODE = 'XC_ST_MATERIAL_PROPERTY'\n     AND d.ENABLE_FLAG = 'Y'\n     AND d.VAL_CODE = sm.MATERIAL_PRO) AS MATERIAL_PRO_NAME,\n\tswbs.SERIAL_BATCH_NUM,\n\tswbs.B_SERIAL_ID,\n\tIF(swbs.SERIAL_BATCH_NUM IS NULL,SUM(swi.QTY),SUM((IFNULL(swbs.QTY,0) - IFNULL(swbs.DELEVERED_QTY,0)))) \"XCSL\",\n    IF(swbs.SERIAL_BATCH_NUM IS NULL,SUM(swi.QTY),SUM((IFNULL(swbs.QTY,0) - IFNULL(swbs.DELEVERED_QTY,0)))) / swi.QTY * swi.AMOUNT \"JCCB\",\n    smt.MATERIAL_TYPE_NAME,\n\tswi.ENTRY_H_ID,\n\tswi.ENTRY_L_ID\nFROM\n\txc_st_use_apply_l sual\nLEFT JOIN xc_st_use_apply_h suah ON sual.USE_APPLY_H_ID = suah.USE_APPLY_H_ID\nLEFT JOIN xc_st_material sm ON sual.MATERIAL_ID = sm.MATERIAL_ID\nLEFT JOIN xc_st_material_type smt ON sm.MATERIAL_TYPE_ID = smt.MATERIAL_TYPE_ID\nLEFT JOIN xc_gl_dimensions gd ON sual.DIM_ID = gd.DIM_ID\nLEFT JOIN xc_st_wh_inventory swi ON (sual.MATERIAL_ID = swi.MATERIAL_ID AND suah.WAREHOUSE_ID = swi.WAREHOUSE_ID)\nLEFT JOIN xc_st_wh_b_ser swbs ON (swi.MATERIAL_ID = sual.MATERIAL_ID AND (SELECT WAREHOUSE_ID from xc_st_wh_entry_h WHERE ENTRY_H_ID = swbs.BUSINESS_H_ID) = suah.WAREHOUSE_ID)\n{#sql#}\nAND (IFNULL(sual.QTY,0) - IFNULL(sual.DELEVERED_QTY,0) > 0)\nAND swi.QTY > 0\nGROUP BY swbs.SERIAL_BATCH_NUM,sm.MATERIAL_ID\nORDER BY swi.CREATION_DATE,swbs.CREATION_DATE,sm.MATERIAL_CODE,swbs.SERIAL_BATCH_NUM"},"children":[],"expanded":false,"type":"sqlswitcher"},{"configs":{"sql":"{#sqlswitcher1#}","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"mo