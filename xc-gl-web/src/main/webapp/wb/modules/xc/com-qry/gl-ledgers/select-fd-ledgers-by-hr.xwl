{"title":"按HR分配关系查询账簿信息","inframe":false,"hidden":false,"roles":{"default":1},"pageLink":"","children":[{"configs":{"itemId":"module"},"children":[{"configs":{"sql":"SELECT T.LEDGER_ID,\n       T.LEDGER_CODE,\n       T.LEDGER_NAME,\n       T.START_PERIOD_CODE,\n       T.ORG_ID,\n       T.EX_HRCY_ID,\n       T.BG_IS_CHK,\n       T.ACC_HRCY_ID,\n       T.CURRENCY_CODE\n  FROM XC_GL_LEDGERS T\n WHERE EXISTS (SELECT 1\n          FROM (SELECT DISTINCT XEA.EMP_ID, XEA.ORG_ID, XU.USER_ID\n                  FROM XIP_PUB_USERS XU, XIP_PUB_EMPS XE, XIP_PUB_EMP_ASG XEA\n                 WHERE XU.EMP_ID = XE.EMP_ID\n                   AND XE.EMP_ID = XEA.EMP_ID\n                   AND XU.USER_ID = {?XIP.userId?}\n                ) TMP\n         WHERE T.ORG_ID = TMP.ORG_ID)\n","itemId":"dataprovider1"},"children":[],"type":"dataprovider","expanded":false}],"type":"module","expanded":true}],"iconCls":"seek_icon"}