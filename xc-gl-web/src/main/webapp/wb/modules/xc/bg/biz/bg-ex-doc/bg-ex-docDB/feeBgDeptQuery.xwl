{"inframe":false,"title":"费用预算成本中心查询","hidden":true,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT d.DEPT_ID, d.DEPT_CODE, d.DEPT_NAME\n  FROM xip_pub_depts d, xc_gl_ledgers l\n WHERE l.ORG_ID = d.ORG_ID\n   AND l.LEDGER_ID = {?LEDGER_ID?}\n   AND (d.DEPT_NAME LIKE '%{#dept_info#}%' OR d.DEPT_CODE LIKE '%{#dept_info#}%')\n order by d.DEPT_CODE\n","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}