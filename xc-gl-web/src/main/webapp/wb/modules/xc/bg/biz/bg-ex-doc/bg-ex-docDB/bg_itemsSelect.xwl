{"inframe":false,"title":"预算项表查询","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT\n\tbi.BG_ITEM_ID,\n\tbi.BG_ITEM_CODE,\n\tbi.BG_ITEM_NAME,\n\tbli.BG_LD_ITEM_ID\nFROM\n\txc_bg_ld_items bli,\n\txc_bg_items bi\nWHERE\n\tbli.BG_ITEM_ID = bi.BG_ITEM_ID\nAND bli.LEDGER_ID = {?LEDGER_ID?}\nAND (bi.BG_ITEM_NAME LIKE '%{#item_info#}%' OR bi.BG_ITEM_CODE LIKE '%{#item_info#}%')\nAND ({?NOWDATE?} BETWEEN bi.START_DATE AND bi.END_DATE)\nAND NOT EXISTS(SELECT a.BG_ITEM_ID FROM xc_bg_items a WHERE a.BG_ITEM_UP_ID = bi.BG_ITEM_ID)\nAND bli.IS_ENABLED = 'Y'\norder by bi.BG_ITEM_CODE","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":"seek_icon"}