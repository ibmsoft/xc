{"inframe":false,"title":"项目信息查询","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT\n\tp.PROJECT_ID,\n\tp.PROJECT_NAME,\n    p.PROJECT_CODE,\n    p.PRJ_CTRL_DIM\nFROM\n\txc_pm_projects p\nWHERE\n\tp.LEDGER_ID = {?LEDGER_ID?}\nAND (p.PROJECT_NAME LIKE '%{#pro_info#}%' OR p.PROJECT_CODE LIKE '%{#pro_info#}%')\nAND ({?NOWDATE?} BETWEEN p.START_DATE AND p.END_DATE)\nAND NOT EXISTS(SELECT a.PROJECT_ID FROM xc_pm_projects a WHERE a.UP_PROJECT_ID = p.PROJECT_ID)","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":"seek_icon"}