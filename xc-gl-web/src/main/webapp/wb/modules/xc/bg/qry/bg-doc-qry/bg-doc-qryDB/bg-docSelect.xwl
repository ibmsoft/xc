{"inframe":false,"title":"预算单查询","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT\n\tbd.BG_DOC_ID,\n\tbd.BG_DOC_CODE,\n\tbd.BG_DOC_NAME,\n\tbd.LEDGER_ID,\n\tbd.BG_VER_ID,\n\tbcv.BG_VER_NAME,\n    bcv.BG_VER_PERIOD,\n\tbd.BG_CAT_CODE,\n\tbc.BG_CAT_NAME,\n\tbd.BG_DOC_DESC,\n\tbd.PROJECT_ID,\n\tpp.PROJECT_NAME,\n\tbd.INS_CODE,\n\tbd.ADUIT_USERS,\n\tbd.AUDIT_DATE,\n\tbd.AUDIT_STATUS,\n\tbd.AUDIT_STATUS_DESC,\n\tbd.CREATION_DATE,\n\tbd.CREATED_BY,\n\tbd.LAST_UPDATE_DATE,\n\tbd.LAST_UPDATED_BY,\n\tpe.EMP_NAME\nFROM\n\txc_bg_docs bd\nLEFT JOIN xc_bg_cat bc ON bd.BG_CAT_CODE = bc.BG_CAT_CODE\nLEFT JOIN xc_bg_cost_ver bcv ON bd.BG_VER_ID = bcv.BG_VER_ID\nLEFT JOIN xc_pm_projects pp ON bd.PROJECT_ID = pp.PROJECT_ID,\n xip_pub_emps pe,\n xip_pub_users pu\nWHERE\n\tpe.EMP_ID = pu.EMP_ID\nAND pu.USER_ID = bd.CREATED_BY\nAND bd.LEDGER_ID = {?LEDGER_ID?}\nAND bc.BG_CAT_NAME LIKE '%{#BG_CAT_NAME#}%'\nORDER BY bd.CREATION_DATE DESC","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":"seek_icon"}