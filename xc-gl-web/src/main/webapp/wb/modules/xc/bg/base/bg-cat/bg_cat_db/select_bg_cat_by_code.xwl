{"title":"根据单据类型编码查询具体记录","inframe":false,"hidden":false,"roles":{"default":1},"pageLink":"","children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT \n      bgcat.BG_CAT_CODE,\n      bgcat.BG_CAT_NAME,\n\n      bgcat.PROCESS_ID,\n      process.PROCESS_NAME,      \n\n      bgcat.PC_W_FORM_ID,\n      pcw.FORM_DESC AS PC_W_FORM_NAME,\n\n      bgcat.PC_A_FORM_ID,\n      pca.FORM_DESC AS PC_A_FORM_NAME,\n\n      bgcat.PC_P_FORM_ID,\n      pcp.FORM_DESC AS PC_P_FORM_NAME,\n\n      bgcat.M_W_FORM_ID,\n      mw.FORM_DESC AS M_W_FORM_NAME,\n\n      bgcat.M_A_FORM_ID,\n      ma.FORM_DESC AS M_A_FORM_NAME,\n\n      bgcat.BG_CAT_DESC\n FROM xc_bg_cat bgcat\n\nLEFT JOIN xip_wf_processes process ON bgcat.PROCESS_ID = process.PROCESS_ID\nLEFT JOIN xip_wf_process_forms pcw ON bgcat.PC_W_FORM_ID = pcw.FORM_ID\nLEFT JOIN xip_wf_process_forms pca ON bgcat.PC_A_FORM_ID = pca.FORM_ID\nLEFT JOIN xip_wf_process_forms pcp ON bgcat.PC_P_FORM_ID = pcp.FORM_ID\nLEFT JOIN xip_wf_process_forms mw ON bgcat.M_W_FORM_ID = mw.FORM_ID\nLEFT JOIN xip_wf_process_forms ma ON bgcat.M_A_FORM_ID = ma.FORM_ID\n WHERE bgcat.BG_CAT_CODE = {?cat_code?}","itemId":"dataprovider1"},"children":[],"type":"dataprovider","expanded":false}],"type":"module","expanded":true}],"iconCls":"seek_icon"}