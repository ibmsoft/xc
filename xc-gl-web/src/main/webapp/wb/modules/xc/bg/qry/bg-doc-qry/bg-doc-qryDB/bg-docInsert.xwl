{"inframe":false,"title":"预算单增加","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","serverScript":"//检查是否存在重复的BG_DOC_CODE\napp.run('select 1 from xc_bg_docs where BG_DOC_CODE={?BG_DOC_CODE?} and LEDGER_ID={?LEDGER_ID?}',{\n    errorText: '编码\"' + app.get('BG_DOC_CODE') + '\" 已经存在。'\n});","itemId":"module"},"children":[{"configs":{"sql":"INSERT INTO xc_bg_docs (\n\tBG_DOC_ID,\n\tBG_DOC_CODE,\n\tBG_DOC_NAME,\n\tLEDGER_ID,\n\tBG_VER_ID,\n\tBG_CAT_CODE,\n\tBG_DOC_DESC,\n\tPROJECT_ID,\n\tINS_CODE,\n\tAUDIT_STATUS,\n    AUDIT_STATUS_DESC,\n\tAUDIT_DATE,\n\tADUIT_USERS,\n\tCREATION_DATE,\n\tCREATED_BY,\n    LAST_UPDATE_DATE,\n    LAST_UPDATED_BY\n)\nVALUES(\n\t{?BG_DOC_ID?},\n    {?BG_DOC_CODE?},\n    {?BG_DOC_NAME?},\n  \t{?LEDGER_ID?},\n  \t{?BG_VER_ID?},\n    {?BG_CAT_CODE?},\n    {?BG_DOC_DESC?},\n    {?PROJECT_ID?},\n  \t{?INS_CODE?},\n    'A',\n  \t'起草',\n    {?AUDIT_DATE?},\n    {?ADUIT_USERS?},\n  \t{?timestamp.sys.date?},\n    {?XIP.userId?},\n  \t{?timestamp.sys.date?},\n    {?XIP.userId?}\n)","itemId":"query1"},"children":[],"expanded":false,"type":"query"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":"file_add_icon"}