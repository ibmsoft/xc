{"inframe":false,"title":"会计年度查询","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT\n\tcount(*) AS \"COUNT\",\n\tbd.BG_DOC_ID,\n\tbd.BG_DOC_CODE,\n\tbd.BG_DOC_NAME,\n\tbd.LEDGER_ID,\n\tgl.LEDGER_NAME,\n\tbd.BG_VER_ID,\n\tbcv.BG_VER_NAME,\n\tbcv.BG_VER_PERIOD,\n\tbd.BG_CAT_CODE,\n\tbc.BG_CAT_NAME,\n\tbd.BG_DOC_DESC,\n\tbd.PROJECT_ID,\n\tpp.PROJECT_NAME,\n\tbd.INS_CODE,\n\tbd.ADUIT_USERS,\n\tbd.AUDIT_DATE,\n\tbd.AUDIT_STATUS,\n\tbd.AUDIT_STATUS_DESC,\n\tbd.CREATION_DATE,\n\tbd.CREATED_BY,\n\tbd.LAST_UPDATE_DATE,\n\tbd.LAST_UPDATED_BY\nFROM\n\txc_bg_docs bd\nLEFT JOIN xc_gl_ledgers gl ON bd.LEDGER_ID = gl.LEDGER_ID\nLEFT JOIN xc_pm_projects pp ON bd.PROJECT_ID = pp.PROJECT_ID\nLEFT JOIN xc_bg_cost_ver bcv ON bd.BG_VER_ID = bcv.BG_VER_ID\nLEFT JOIN xc_bg_cat bc ON bd.BG_CAT_CODE = bc.BG_CAT_CODE\nWHERE\n\tbd.BG_DOC_ID = {?BG_DOC_ID?}","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":"seek_icon"}