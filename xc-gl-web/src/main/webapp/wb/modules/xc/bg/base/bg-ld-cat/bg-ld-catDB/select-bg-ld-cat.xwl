{"inframe":false,"title":"查询账簿下的预算单类型","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"select LC.BG_CAT_CODE,\n       BC.BG_CAT_NAME,\n       BC.BG_CAT_DESC,     \n       LC.PROCESS_ID,\n       wp.PROCESS_NAME,\n       LC.RULE_CODE,\n       gr.RULE_DESC,\n       LC.BG_LD_CAT_ID \n  from XC_BG_CAT BC,XC_BG_LD_CAT LC\n  left join xip_wf_processes wp on LC.PROCESS_ID = wp.PROCESS_ID\n  left join xc_gl_rules gr on LC.RULE_CODE = gr.RULE_CODE\n where LC.BG_CAT_CODE = BC.BG_CAT_CODE\n   and LC.LEDGER_ID = {?LEDGER_ID?}","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":"seek_icon"}