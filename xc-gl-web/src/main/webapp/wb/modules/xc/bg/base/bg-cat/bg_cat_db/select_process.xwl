{"inframe":false,"title":"查询默认审批流程","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"oracle":"SELECT \n       processes.PROCESS_ID,\n       processes.PROCESS_CODE,\n       processes.PROCESS_NAME\n  FROM xip_wf_processes processes,\n       xip_wf_entities entities\n WHERE processes.ENTITY_ID = entities.ENTITY_ID\n   AND processes.ENABLE_FLAG='Y'\n   AND entities.ENTITY_CODE = 'XC_BG_DOCS'\n   AND (nvl(processes.PROCESS_CODE,'%') LIKE '%{#process_name#}%' \n        OR nvl(processes.PROCESS_NAME,'%') LIKE '%{#process_name#}%')\nORDER BY processes.PROCESS_CODE","itemId":"sqlswitcher1","mysql":"SELECT \n       processes.PROCESS_ID,\n       processes.PROCESS_CODE,\n       processes.PROCESS_NAME\n  FROM xip_wf_processes processes,\n       xip_wf_entities entities\n WHERE processes.ENTITY_ID = entities.ENTITY_ID\n   AND processes.ENABLE_FLAG='Y'\n   AND entities.ENTITY_CODE = 'XC_BG_DOCS'\n   AND (IFNULL(processes.PROCESS_CODE,'%') LIKE '%{#process_name#}%' \n        OR IFNULL(processes.PROCESS_NAME,'%') LIKE '%{#process_name#}%')\nORDER BY processes.PROCESS_CODE"},"children":[],"expanded":false,"type":"sqlswitcher"},{"configs":{"sql":"{#sqlswitcher1#}","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":"seek_icon"}