{"inframe":false,"title":"费用预算汇总","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT\n\tSUBSTR(bcb.PERIOD_CODE, 6) AS \"PERIOD_CODE\",\n\tbcb.BG_AMT,\n\tbcb.BG_ITEM_ID,\n\tbcb.BG_YEAR,\n\tbcb.DEPT_ID,\n\tbi.BG_ITEM_NAME,\n\tpd.DEPT_NAME,bcv.BG_VER_ID\nFROM xc_bg_cost_bal bcb\nLEFT JOIN xc_bg_items bi ON bcb.BG_ITEM_ID = bi.BG_ITEM_ID\nLEFT JOIN xip_pub_depts pd ON pd.DEPT_ID = bcb.DEPT_ID\nLEFT JOIN xc_bg_cost_ver bcv on bcv.BG_VER_PERIOD = bcb.BG_YEAR\nWHERE bcv.BG_VER_ID = {?BG_VER_ID?}\nAND bcb.LEDGER_ID = {?LEDGER_ID?}","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":"seek_icon"}