{"inframe":false,"title":"根据账薄ID查询账薄信息","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT\n\tle.LEDGER_ID,\n    le.LEDGER_CODE,\n    le.LEDGER_NAME,\n    le.ACC_HRCY_ID,\n    le.BG_HRCY_ID,\n    bghr.BG_HRCY_NAME,\n\n    le.BG_EX_PERIOD,\n    experiod.VAL_NAME AS BG_EX_PERIOD_NAME,\n\n    le.BG_IS_CASH,\n    iscash.VAL_NAME AS BG_IS_CASH_NAME,\n    le.BG_IS_CHK,\n    ischk.VAL_NAME AS BG_IS_CHK_NAME,\n    le.BG_IS_SPECIAL,\n    isspecial.VAL_NAME AS BG_IS_SPECIAL_NAME\nFROM\n    XC_GL_LEDGERS le\nLEFT JOIN XC_BG_HRCY bghr ON le.BG_HRCY_ID = bghr.BG_HRCY_ID\nLEFT JOIN xip_pub_val_set_dtl iscash ON le.BG_IS_CASH = iscash.VAL_CODE AND iscash.VAL_SET_CODE='YN'\nLEFT JOIN xip_pub_val_set_dtl ischk ON le.BG_IS_CHK = ischk.VAL_CODE AND ischk.VAL_SET_CODE='YN'\nLEFT JOIN xip_pub_val_set_dtl isspecial ON le.BG_IS_SPECIAL = isspecial.VAL_CODE AND isspecial.VAL_SET_CODE='YN'\nLEFT JOIN xip_pub_val_set_dtl experiod ON le.BG_EX_PERIOD = experiod.VAL_CODE AND experiod.VAL_SET_CODE='XC_BG_EX_CTRL_PERIOD'\n\n WHERE le.LEDGER_ID = {?ledger_id?}\n","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}