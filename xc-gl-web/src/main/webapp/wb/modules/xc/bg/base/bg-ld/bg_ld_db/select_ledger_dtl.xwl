{"inframe":false,"title":"查询账薄明细","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"oracle":"SELECT\n\tle.LEDGER_ID,\n    le.LEDGER_CODE,\n    le.LEDGER_NAME,\n    le.ACC_HRCY_ID,\n    le.BG_HRCY_ID,\n    bghr.BG_HRCY_NAME,\n\n    le.BG_EX_PERIOD,\n    experiod.VAL_NAME AS BG_EX_PERIOD_NAME,\n\n    le.BG_IS_CASH,\n    iscash.VAL_NAME AS BG_IS_CASH_NAME,\n    le.BG_IS_CHK,\n    ischk.VAL_NAME AS BG_IS_CHK_NAME,\n    le.BG_IS_SPECIAL,\n    isspecial.VAL_NAME AS BG_IS_SPECIAL_NAME\nFROM\n    XC_GL_LEDGERS le\nLEFT JOIN XC_BG_HRCY bghr ON le.BG_HRCY_ID = bghr.BG_HRCY_ID\nLEFT JOIN xip_pub_val_set_dtl iscash ON le.BG_IS_CASH = iscash.VAL_CODE AND iscash.VAL_SET_CODE='YN'\nLEFT JOIN xip_pub_val_set_dtl ischk ON le.BG_IS_CHK = ischk.VAL_CODE AND ischk.VAL_SET_CODE='YN'\nLEFT JOIN xip_pub_val_set_dtl isspecial ON le.BG_IS_SPECIAL = isspecial.VAL_CODE AND isspecial.VAL_SET_CODE='YN'\nLEFT JOIN xip_pub_val_set_dtl experiod ON le.BG_EX_PERIOD = experiod.VAL_CODE AND experiod.VAL_SET_CODE='XC_BG_EX_CTRL_PERIOD'\nWHERE le.IS_VALID = 'Y'\n   AND (EXISTS (SELECT 1\n          FROM XC_GL_LD_SEC A\n         WHERE le.LEDGER_ID = A.LEDGER_ID\n           AND A.USER_ID = {?XIP.userId?})\n    OR EXISTS (SELECT 1\n          FROM XIP_PUB_USER_ROLES XUR, XIP_PUB_ROLE XR, XC_GL_LD_SEC XLC\n         WHERE XUR.ROLE_ID = XR.ROLE_ID\n           AND XR.ROLE_ID = XLC.ROLE_ID\n           AND XLC.LEDGER_ID = le.LEDGER_ID\n           AND XUR.USER_ID = {?XIP.userId?}))\n    AND nvl(le.LEDGER_ID,'%') like '%{#ledger_id#}%'\nORDER BY le.LEDGER_CODE ASC","itemId":"sqlswitcher1","mysql":"SELECT\n\tle.LEDGER_ID,\n    le.LEDGER_CODE,\n    le.LEDGER_NAME,\n    le.ACC_HRCY_ID,\n    le.BG_HRCY_ID,\n    bghr.BG_HRCY_NAME,\n\n    le.BG_EX_PERIOD,\n    experiod.VAL_NAME AS BG_EX_PERIOD_NAME,\n\n    le.BG_IS_CASH,\n    iscash.VAL_NAME AS BG_IS_CASH_NAME,\n    le.BG_IS_CHK,\n    ischk.VAL_NAME AS BG_IS_CHK_NAME,\n    le.BG_IS_SPECIAL,\n    isspecial.VAL_NAME AS BG_IS_SPECIAL_NAME\nFROM\n    XC_GL_LEDGERS AS le\nLEFT JOIN XC_BG_HRCY AS bghr ON le.BG_HRCY_ID = bghr.BG_HRCY_ID\nLEFT JOIN xip_pub_val_set_dtl AS iscash ON le.BG_IS_CASH = iscash.VAL_CODE AND iscash.VAL_SET_CODE='YN'\nLEFT JOIN xip_pub_val_set_dtl AS ischk ON le.BG_IS_CHK = ischk.VAL_CODE AND ischk.VAL_SET_CODE='YN'\nLEFT JOIN xip_pub_val_set_dtl AS isspecial ON le.BG_IS_SPECIAL = isspecial.VAL_CODE AND isspecial.VAL_SET_CODE='YN'\nLEFT JOIN xip_pub_val_set_dtl AS experiod ON le.BG_EX_PERIOD = experiod.VAL_CODE AND experiod.VAL_SET_CODE='XC_BG_EX_CTRL_PERIOD'\nWHERE le.IS_VALID = 'Y'\n  AND EXISTS (SELECT 1\n          FROM XC_GL_LD_SEC A\n         WHERE le.LEDGER_ID = A.LEDGER_ID\n           AND A.ROLE_ID = {?XIP.roleId?}\n           AND A.USER_ID = {?XIP.userId?})\n    AND IFNULL(le.LEDGER_ID ,'%') like '%{#ledger_id#}%'\nORDER BY le.LEDGER_CODE ASC"},"children":[],"expanded":false,"type":"sqlswitcher"},{"configs":{"sql":"{#sqlswitcher1#}","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}