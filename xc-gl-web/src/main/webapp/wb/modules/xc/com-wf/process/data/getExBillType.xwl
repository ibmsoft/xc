{"inframe":false,"title":"获取报销单据类型","hidden":true,"roles":{"default":1},"children":[{"configs":{"itemId":"module"},"children":[{"configs":{"oracle":"select \n c.BILL_TYPE_CODE,c.BILL_TYPE_NAME,c.PROCESS_ID from (\nSELECT\n\tt.EX_CAT_CODE AS \"BILL_TYPE_CODE\",\n\tt.EX_CAT_NAME AS \"BILL_TYPE_NAME\",\n  CASE  nvl(p.PROCESS_ID,'') WHEN '' THEN t.PROCESS_ID ELSE p.PROCESS_ID END as PROCESS_ID\nFROM\n\txc_ex_cat t,xc_ex_ld_cat p\nWHERE\n  t.EX_CAT_CODE = p.EX_CAT_CODE\nAND\n  (t.PROCESS_ID IS NOT NULL OR p.PROCESS_ID IS NOT NULL)\nAND p.LEDGER_ID={?ledgerId?}\n) c,xip_wf_processes d \nWHERE \n c.PROCESS_ID = d.PROCESS_ID\nAND \n d.PROCESS_CODE LIKE 'XC_PRESET%'\norder by c.BILL_TYPE_CODE ASC","itemId":"sqlswitcher","mysql":"select \n c.BILL_TYPE_CODE,c.BILL_TYPE_NAME,c.PROCESS_ID from (\nSELECT\n\tt.EX_CAT_CODE AS \"BILL_TYPE_CODE\",\n\tt.EX_CAT_NAME AS \"BILL_TYPE_NAME\",\n  CASE  ifnull(p.PROCESS_ID,'') WHEN '' THEN t.PROCESS_ID ELSE p.PROCESS_ID END as PROCESS_ID\nFROM\n\txc_ex_cat t,xc_ex_ld_cat p\nWHERE\n  t.EX_CAT_CODE = p.EX_CAT_CODE\nAND\n  (t.PROCESS_ID IS NOT NULL OR p.PROCESS_ID IS NOT NULL)\nAND p.LEDGER_ID={?ledgerId?}\n) c,xip_wf_processes d \nWHERE \n c.PROCESS_ID = d.PROCESS_ID\nAND \n d.PROCESS_CODE LIKE 'XC_PRESET%'\norder by c.BILL_TYPE_CODE ASC"},"children":[],"expanded":false,"type":"sqlswitcher"},{"configs":{"sql":"{#sqlswitcher#}\n","itemId":"getExBillType"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}