{"inframe":false,"title":"获取采购单据类型","hidden":true,"roles":{"default":1},"children":[{"configs":{"itemId":"module"},"children":[{"configs":{"sql":"select \n c.BILL_TYPE_CODE,c.BILL_TYPE_NAME,c.PROCESS_ID,c.DOC_CLASS from (\nSELECT\n\tt.PO_CAT_CODE AS \"BILL_TYPE_CODE\",\n\tt.PO_CAT_NAME AS \"BILL_TYPE_NAME\",\n    t.DOC_CLASS,\n CASE  ifnull(p.PROCESS_ID,'') WHEN '' THEN t.PROCESS_ID ELSE p.PROCESS_ID END as PROCESS_ID\nFROM\n\txc_po_doc_cats t,xc_po_ld_doc_cats p\nWHERE\n  t.PO_CAT_CODE = p.PO_DOC_CAT_CODE\nAND\n  (t.PROCESS_ID IS NOT NULL OR p.PROCESS_ID IS NOT NULL)\nAND\n  p.LEDGER_ID={?ledgerId?}\n) c,xip_wf_processes d \nWHERE \n c.PROCESS_ID = d.PROCESS_ID\nAND \n d.PROCESS_CODE LIKE 'XC_PRESET%'\norder by c.DOC_CLASS ASC,c.BILL_TYPE_CODE ASC","itemId":"getPoBillType"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}