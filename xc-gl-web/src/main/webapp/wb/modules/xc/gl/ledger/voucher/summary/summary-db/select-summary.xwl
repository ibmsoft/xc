{"inframe":false,"title":"查询","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT T.LEDGER_NAME, S.V_SUMMARY_ID, S.LEDGER_ID, S.V_SUMMARY, S.ASS_KEY,S.CREATION_DATE,S.CREATED_BY,S.LAST_UPDATE_DATE,S.LAST_UPDATED_BY\n  FROM XC_GL_LEDGERS T,XC_GL_V_SUMMARIES S\n WHERE (EXISTS (SELECT 1\n          FROM XC_GL_LD_SEC A\n         WHERE T.LEDGER_ID = A.LEDGER_ID\n           AND A.USER_ID = {?XIP.userId?})\n    OR EXISTS (SELECT 1\n          FROM XIP_PUB_USER_ROLES XUR, XIP_PUB_ROLE XR, XC_GL_LD_SEC XLC\n         WHERE XUR.ROLE_ID = XR.ROLE_ID\n           AND XR.ROLE_ID = XLC.ROLE_ID\n           AND XLC.LEDGER_ID = T.LEDGER_ID\n           AND XUR.USER_ID = {?XIP.userId?}))\n\tAND S.LEDGER_ID = T.LEDGER_ID\n\tAND (S.LEDGER_ID LIKE '%{#ledger_code#}%' OR S.LEDGER_ID IS NULL)\n\t\n\n\n","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}