{"inframe":false,"title":"查询账簿可追加的调整期","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT\n\tt.PERIOD_CODE,\n\tt. YEAR,\n\tt. QUARTER,\n\tt.START_DATE,\n\tt.END_DATE\nFROM\n\tXC_GL_PERIOD t\nWHERE\n\tt.IS_ADJ_PERIOD = 'Y'\nAND t.START_DATE >= (\n\tSELECT\n\t\tp.START_DATE\n\tFROM\n\t\tXC_GL_PERIOD p\n\tWHERE\n\t\tp.PERIOD_CODE = {?startPeriodCode?}\n)\nAND t.START_DATE <= (\n\tSELECT\n\t\tmax(a.START_DATE)\n\tFROM\n\t\txc_gl_period a,\n\t\txc_gl_ld_period b\n\tWHERE\n\t\ta.PERIOD_CODE = b.PERIOD_CODE\n\tAND b.LEDGER_ID = {?ledgerId?}\n)\nAND NOT EXISTS (select 1 from xc_gl_ld_period p where  p.PERIOD_CODE = t.PERIOD_CODE AND p.LEDGER_ID = {?ledgerId?})\nAND t.PERIOD_CODE LIKE '%{#queryPeriodCode#}%'\n ORDER BY t.START_DATE asc\n","itemId":"dataprovider"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}