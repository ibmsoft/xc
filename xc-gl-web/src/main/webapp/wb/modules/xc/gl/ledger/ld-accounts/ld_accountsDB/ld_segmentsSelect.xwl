{"inframe":false,"title":"科目辅助核算段查询","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"oracle":"SELECT\n\tglas.LD_SEG_ID,\n\tglas.SEG_CODE,\n\tgas.SEG_NAME,\n    decode(glas.LD_SEG_ALIAS,null,SEG_NAME,LD_SEG_ALIAS) as \"LD_SEG_ALIAS\",\n    decode({?selectRows?},'1',0,decode((select glaas.IS_ENABLED from xc_gl_ld_acc_ass_seg glaas where glaas.LD_ACC_ID={?LD_ACC_ID?} and glaas.LD_SEG_ID=glas.LD_SEG_ID),1,1,0)) as \"IS_ENABLED\"\nFROM\n\txc_gl_ass_segments gas,\n\txc_gl_ld_ass_segments glas\nWHERE\n\tgas.SEG_CODE = glas.SEG_CODE\nAND glas.LEDGER_ID={?LEDGER_ID?}\nAND glas.IS_ENABLED=1\nORDER BY gas.ORDERBY","itemId":"sqlswitcher1","mysql":"SELECT\n\tglas.LD_SEG_ID,\n\tglas.SEG_CODE,\n\tgas.SEG_NAME,\n    if(glas.LD_SEG_ALIAS is null,SEG_NAME,LD_SEG_ALIAS) as \"LD_SEG_ALIAS\",\n    if({?selectRows?}<>'1',0,if((select glaas.IS_ENABLED from xc_gl_ld_acc_ass_seg glaas where glaas.LD_ACC_ID={?LD_ACC_ID?} and glaas.LD_SEG_ID=glas.LD_SEG_ID)=1,1,0)) as \"IS_ENABLED\"\nFROM\n\txc_gl_ass_segments gas,\n\txc_gl_ld_ass_segments glas\nWHERE\n\tgas.SEG_CODE = glas.SEG_CODE\nAND glas.LEDGER_ID={?LEDGER_ID?}\nAND glas.IS_ENABLED=1\nORDER BY gas.ORDERBY"},"children":[],"expanded":false,"type":"sqlswitcher"},{"configs":{"sql":"{#sqlswitcher1#}","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}