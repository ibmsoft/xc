{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"oracle":"select abc.seg_id,abc.seg_code,abc.seg_name\nfrom\n(\nselect a.VENDOR_ID as seg_id,\n       a.VENDOR_CODE as seg_code,\n       a.VENDOR_NAME as seg_name\nfrom XC_AP_VENDORS a\nwhere a.VENDOR_ID not in (select b.VENDOR_ID from XC_GL_LD_VENDORS b where b.LEDGER_ID = {?ledger_id?} )\n  and {?seg?} = 'XC_AP_VENDORS'\n  and (a.VENDOR_CODE like '%{#codeOrName#}%' or a.VENDOR_NAME like '%{#codeOrName#}%')\n  and sysdate between a.START_DATE and a.END_DATE\nunion\n  \nselect a.CUSTOMER_ID as seg_id,\n  a.CUSTOMER_CODE as seg_code,\n  a.CUSTOMER_NAME as seg_name\nfrom XC_AR_CUSTOMERS a\nwhere a.CUSTOMER_ID not in (select b.CUSTOMER_ID from XC_GL_LD_CUSTOMERS b where b.LEDGER_ID = {?ledger_id?} )\n  and {?seg?} ='XC_AR_CUSTOMERS'\n  and (a.CUSTOMER_CODE like '%{#codeOrName#}%' or a.CUSTOMER_NAME like '%{#codeOrName#}%')\n  and sysdate between a.START_DATE and a.END_DATE\n\nunion\n  \nselect a.PRODUCT_ID as seg_id,\n       a.PRODUCT_CODE as seg_code,\n       a.PRODUCT_NAME as seg_name\nfrom XC_GL_PRODUCTS a\nwhere a.PRODUCT_ID not in (select b.PRODUCT_ID from XC_GL_LD_PRODUCTS b where b.LEDGER_ID = {?ledger_id?} )\n  and {?seg?} = 'XC_GL_PRODUCTS'\n  and (a.PRODUCT_CODE like '%{#codeOrName#}%' or a.PRODUCT_NAME like '%{#codeOrName#}%')\n  and sysdate between a.START_DATE and a.END_DATE\n  \nunion\n\nselect DEPTS.DEPT_ID as seg_id,\n       DEPTS.DEPT_CODE as seg_code,\n       DEPTS.DEPT_NAME as seg_name\nfrom XIP_PUB_DEPTS DEPTS,XC_GL_LEDGERS LEDGERS\nwhere DEPTS.DEPT_ID not in (select b.DEPT_ID from XC_GL_LD_DEPTS b where b.LEDGER_ID = {?ledger_id?} )\n  and {?seg?} ='XIP_PUB_DEPTS'\n  and LEDGERS.LEDGER_ID = {?ledger_id?}\n  and DEPTS.ORG_ID = LEDGERS.ORG_ID\n  and DEPTS.ENABLE_FLAG = 'Y'\n  and 0 = exists(select 1 from XIP_PUB_DEPTS dept where dept.UP_DP_ID = DEPTS.DEPT_ID)\n  and (DEPTS.DEPT_CODE like '%{#codeOrName#}%' or DEPTS.DEPT_CODE like '%{#codeOrName#}%')\n\nunion\n\nselect \n  ORGS.ORG_ID as seg_id,\n  ORGS.ORG_CODE as seg_code,\n  ORGS.ORG_NAME as seg_name\nfrom XIP_PUB_ORGS ORGS\nwhere ORGS.ORG_ID not in (select O.ORG_ID from XC_GL_LD_ORGS O where O.LEDGER_ID = {?ledger_id?})\n  and {?seg?} ='XIP_PUB_ORGS'\n  and ORGS.ENABLE_FLAG = 'Y'\n  and ORGS.ORG_ID <> {?org_id?}\n  and ORGS.ORG_TYPE = 'O'\n  and (ORGS.ORG_CODE like '%{#codeOrName#}%' or ORGS.ORG_NAME like '%{#codeOrName#}%')\n\nunion\n\nselect \n  distinct\n  EMPS.EMP_ID as seg_id,\n  EMPS.EMP_CODE as seg_code,\n  EMPS.EMP_NAME as seg_name\nfrom XIP_PUB_EMPS EMPS, XIP_PUB_EMP_ASG EMP_ASG\nwhere EMPS.EMP_ID not in (select O.EMP_ID from XC_GL_LD_EMPS O where O.LEDGER_ID = {?ledger_id?})\n  and {?seg?} ='XIP_PUB_EMPS'\n  and EMPS.ATTRIBUTE2 = 'Y'\n  and EMP_ASG.ORG_ID = {?org_id?}\n  and EMPS.EMP_ID = EMP_ASG.EMP_ID\n  and EMP_ASG.ENABLED = 'Y'\n  and (EMPS.EMP_CODE like '%{#codeOrName#}%' or EMPS.EMP_NAME like '%{#codeOrName#}%')\n) abc\norder by seg_code\n\n\n","itemId":"sqlswitcher1","mysql":"select abc.seg_id,abc.seg_code,abc.seg_name\nfrom\n(\nselect a.VENDOR_ID as seg_id,\n       a.VENDOR_CODE as seg_code,\n       a.VENDOR_NAME as seg_name\nfrom XC_AP_VENDORS a\nwhere a.VENDOR_ID not in (select b.VENDOR_ID from XC_GL_LD_VENDORS b where b.LEDGER_ID = {?ledger_id?} )\n  and {?seg?} = 'XC_AP_VENDORS'\n  and (a.VENDOR_CODE like '%{#codeOrName#}%' or a.VENDOR_NAME like '%{#codeOrName#}%')\n  and NOW() between a.START_DATE and a.END_DATE\nunion\n  \nselect a.CUSTOMER_ID as seg_id,\n  a.CUSTOMER_CODE as seg_code,\n  a.CUSTOMER_NAME as seg_name\nfrom XC_AR_CUSTOMERS a\nwhere a.CUSTOMER_ID not in (select b.CUSTOMER_ID from XC_GL_LD_CUSTOMERS b where b.LEDGER_ID = {?ledger_id?} )\n  and {?seg?} ='XC_AR_CUSTOMERS'\n  and (a.CUSTOMER_CODE like '%{#codeOrName#}%' or a.CUSTOMER_NAME like '%{#codeOrName#}%')\n  and NOW() between a.START_DATE and a.END_DATE\n\nunion\n  \nselect a.PRODUCT_ID as seg_id,\n       a.PRODUC