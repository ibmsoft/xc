{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"select xga.ACC_NAME,\nxga.ACC_ID,\nxga.ACC_CODE\nfrom xc_gl_accounts xga, xc_gl_ld_accounts  xgla\nwhere xga.ACC_ID = xgla.ACC_ID\nand xgla.LEDGER_ID = {?ledgerId?}\nand xga.IS_LEAF = 'Y' and (xga.ACC_CODE like '%{#codeOrName#}%' or xga.ACC_NAME like '%{#codeOrName#}%')\nAND (xga.IS_BANK_ACC = 'Y' OR xga.IS_CASH_ACC = 'Y')\nAND xga.IS_LEAF = 'Y'\norder by xga.ACC_CODE ASC\n","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}