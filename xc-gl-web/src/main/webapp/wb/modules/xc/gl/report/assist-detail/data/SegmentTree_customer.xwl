{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"itemId":"module"},"children":[{"configs":{"sql":"select ac.CUSTOMER_NAME \"text\",\n\t   lc.CUSTOMER_ID \"id\",\n\t   ac.CUSTOMER_CODE \"code\",\n\t   'true' as \"leaf\"\n\tfrom xc_gl_ld_customers lc,\n\txc_ar_customers ac\nwhere \nac.CUSTOMER_ID = lc.CUSTOMER_ID\nAND \nlc.LEDGER_ID = {?ledgerId?}\nAND\ncase \nwhen '{#startCode#}' = '' and '{#endCode#}' = ''\nthen 1=1\nwhen '{#startCode#}' = '' and '{#endCode#}' <> ''\nthen ac.CUSTOMER_CODE <= '{#endCode#}'\nwhen '{#startCode#}' <> '' and '{#endCode#}' <> ''\nthen ac.CUSTOMER_CODE >= '{#startCode#}' and ac.CUSTOMER_CODE <= '{#endCode#}'\nwhen '{#startCode#}' <> '' and '{#endCode#}' = ''\nthen ac.CUSTOMER_CODE >= '{#startCode#}'\nend\nand exists (select 1 from xc_gl_balance a ,xc_gl_ccid b where a.LEDGER_ID = b.LEDGER_ID and a.CCID = b.CCID and b.CUSTOMER_ID = ac.CUSTOMER_ID)\nORDER BY\nac.CUSTOMER_CODE ASC","itemId":"dataprovider1","type":"tree"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}