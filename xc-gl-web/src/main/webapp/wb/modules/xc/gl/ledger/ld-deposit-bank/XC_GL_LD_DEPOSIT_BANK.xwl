 false;\n}else   //验证银行账号不能为空\nif (bankAccount === null || bankAccount === '' || bankAccount === undefined) {\n  Wb.toast(\"银行账号不能为空！\");\n  app.BANK_ACCOUNT.focus();\n  return false;\n}else   //验证银行账号不能为空\nif (accId === null || accId === '' || accId === undefined) {\n  Wb.toast(\"会计科目不能为空！\");\n  app.ACC_NAME_EDITOR.focus();\n  return false;\n}\nelse  //验证结束时间不为空时不大于开始时间\nif(startDate > endDate && endDate !== null){\n    Wb.toast(\"结束时间不能早于开始时间！\");\n  return false;\n}\n// 执行添加操作\nif(flag===\"add\"){\n //  发送请求\n Wb.request({\n  url: 'm?xwl=xc/gl/ledger/ld-deposit-bank/ld_DepositDB/insertDeposit',\n  params:{'accId':accId,'currencyId':currencyId,'accountName':accountName,'bankName':bankName,'depositBankName':depositBankName,'bankAccount':bankAccount,'corres':corres,'startDate':startDate,'endDate':endDate,'desc':desc,'ledgerId':ledgerId ,'Did':app.DEPOSIT_BANK_ID.getValue()},\n  success: function(resp) {\n       var response = Wb.decode(resp.responseText);\n      if(response.flag == '0'){\n      app.addOrupdateWin.close();\n         Wb.toast('增加成功!') ;\n   app.depositGrid.getStore().load({\n  params:{\n    LEDGER_ID:app.queryCombo.getValue()\n  }\n    });\n     \n    }\n    else{\n     Wb.toast('增加失败!') ;\n    }\n\n}});\n}\n//执行修改操作\nif(flag===\"edit\"){\n   Wb.request({\n  url: 'm?xwl=xc/gl/ledger/ld-deposit-bank/ld_DepositDB/updateDeposit',\n  params:{'accId':app.ACC_ID.getValue(),'currencyId':app.CURRENCY_CODE.getValue(),'accountName':app.ACCOUNT_NAME.getValue(),'bankName':app.BANK_NAME.getValue(),'depositBankName':app.DEPOSIT_BANK_NAME.getValue(),'bankAccount':app.BANK_ACCOUNT.getValue(),'corres':app.CORRESPONDENT.getValue(),'startDate':app.START_DATE.getValue(),'endDate':app.END_DATE.getValue(),'desc':app.DEPOSIT_BANK_DESC.getValue(),'Did':app.DEPOSIT_BANK_ID.getValue()},\n  success: function(resp) {\n    var returnobj=Wb.decode(resp.responseText);\n    if(returnobj.flag=='0'){\n       app.addOrupdateWin.close();\n        app.depositGrid.getStore().load({\n     params:{\n      LEDGER_ID:app.queryCombo.getValue()\n      }\n    });\n      Wb.toast('修改成功!') ;\n    }else{\n     Wb.toast('修改失败!') ;\n    }\n   \n    }\n  \n});\n}\n","close":"//关闭后清除信息\napp.ACC_ID.reset();\napp.ACCOUNT_NAME.reset();\napp.BANK_ACCOUNT.reset();\napp.BANK_NAME.reset();\napp.CORRESPONDENT.reset();\napp.DEPOSIT_BANK_DESC.reset();\napp.DEPOSIT_BANK_NAME.reset();\napp.END_DATE.reset();\napp.START_DATE.reset();\napp.CURRENCY_CODE.reset();"},"children":[{"configs":{"hidden":"true","itemId":"DEPOSIT_BANK_ID"},"children":[],"expanded":false,"type":"text"},{"configs":{"labelWidth":"45","height":"22","style":"margin-left:60px;","displayField":"LEDGER_NAME","hidden":"true","width":"220","valueField":"LEDGER_ID","itemId":"LEDGER_ID","fieldLabel":"账 簿","y":"16","x":"30"},"events":{"select":" app.queryBtn.fireEvent('click');"},"children":[{"configs":{"itemId":"store","url":"m?xwl=xc/com-qry/gl-ledgers/select-sec-fd-ledgers"},"children":[],"expanded":false,"type":"store"}],"expanded":false,"type":"combo"},{"configs":{"labelWidth":"80","width":"240","margin":"20 0 5 20","labelAlign":"right","itemId":"ACCOUNT_NAME","fieldLabel":"<font color='red'>*<\/font>{#show_ACCOUNT_NAME#}"},"children":[],"expanded":false,"type":"text"},{"configs":{"labelWidth":"80","displayField":"newCURRENCY_NAME","width":"280","valueField":"CURRENCY_CODE","margin":"20 0 5 10","labelAlign":"right","itemId":"CURRENCY_NAME","fieldLabel":"{#show_CURRENCY_NAME#}","editable":"false"},"events":{"select":"var currenty_id=app.CURRENCY_NAME.getValue();\n//alert(currenty_id);\napp.CURRENCY_CODE.setValue(currenty_id);\nreturn;","beforequery":"// TODO: 对下拉框增加模糊查询处理\nvar combo = plan.combo ;\nif(!plan.forceAll){\n  var value = plan.query;\n  combo.store.filterBy(function(record,id){\n    var text = record.get(combo.displayField);\n    return (text.indexOf(value)!=-1);\n  });\n  combo.expand();\n  return false;\n}"},"children":[{"configs":{"itemId":"store","url":"m?xwl=xc/gl/ledger/ld-deposit-bank/ld_Depos