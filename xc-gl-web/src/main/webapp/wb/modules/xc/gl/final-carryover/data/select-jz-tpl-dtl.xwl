{"inframe":false,"title":"查询结转模板明细","hidden":true,"roles":{"default":1},"children":[{"configs":{"itemId":"module"},"children":[{"configs":{"oracle":"SELECT\n\tt.TPL_DTL_ID,\n\tt.JZ_TPL_ID,\n\tt.ZC_ACC_ID,\n\t(\n\t\tSELECT\n\t\t\ta.ACC_CODE||' '||a.ACC_NAME\n\t\tFROM\n\t\t\txc_gl_accounts a\n\t\tWHERE\n\t\t\ta.ACC_ID = t.ZC_ACC_ID\n\t\tAND a.ACC_HRCY_ID = {?accHrcyId?}\n\t) AS ZC_ACC_NAME,\n\tt.SUM_ACC_ID,\n\t(\n\t\tSELECT\n\t\t\ta.ACC_CODE||' '||a.ACC_NAME\n\t\tFROM\n\t\t\txc_gl_accounts a\n\t\tWHERE\n\t\t\ta.ACC_ID = t.SUM_ACC_ID\n\t\tAND a.ACC_HRCY_ID = {?accHrcyId?}\n\t) AS SUM_ACC_NAME,\n\tt.BAL_RULE,\n    CASE t.BAL_RULE WHEN '1' THEN '借方取数' WHEN '-1' THEN '贷方取数' ELSE '余额取数' END AS BAL_RULE_NAME,\n\tt.BAL_DR,\n    CASE t.BAL_DR WHEN '1' THEN '借' WHEN '-1' THEN '贷' END AS BAL_DR_NAME,\n\tt.ACC_SEGMENTS,\n\tt.SUMMARY,\n    '' AS OUT_ACC_DIRECTION,\n    '' AS OUT_ACC_SEG,\n    '' AS DATA_ACC_DIRECTION\nFROM\n\txc_gl_jz_tpl_dtl t\nWHERE\n\tt.JZ_TPL_ID = {?jzTplId?}\n\n","itemId":"sqlswitcher","mysql":"SELECT\n\tt.TPL_DTL_ID,\n\tt.JZ_TPL_ID,\n\tt.ZC_ACC_ID,\n\t(\n\t\tSELECT\n\t\t\tCONCAT(a.ACC_CODE,' ',a.ACC_NAME)\n\t\tFROM\n\t\t\txc_gl_accounts a\n\t\tWHERE\n\t\t\ta.ACC_ID = t.ZC_ACC_ID\n\t\tAND a.ACC_HRCY_ID = {?accHrcyId?}\n\t) AS ZC_ACC_NAME,\n\tt.SUM_ACC_ID,\n\t(\n\t\tSELECT\n\t\t\tCONCAT(a.ACC_CODE,' ',a.ACC_NAME)\n\t\tFROM\n\t\t\txc_gl_accounts a\n\t\tWHERE\n\t\t\ta.ACC_ID = t.SUM_ACC_ID\n\t\tAND a.ACC_HRCY_ID = {?accHrcyId?}\n\t) AS SUM_ACC_NAME,\n\tt.BAL_RULE,\n    CASE t.BAL_RULE WHEN '1' THEN '借方取数' WHEN '-1' THEN '贷方取数' ELSE '余额取数' END AS BAL_RULE_NAME,\n\tt.BAL_DR,\n    CASE t.BAL_DR WHEN '1' THEN '借' WHEN '-1' THEN '贷' END AS BAL_DR_NAME,\n\tt.ACC_SEGMENTS,\n\tt.SUMMARY,\n    (select p.BALANCE_DIRECTION from xc_gl_accounts p WHERE p.ACC_ID = t.ZC_ACC_ID) AS OUT_ACC_DIRECTION,\n    '' AS OUT_ACC_SEG,\n     (select p.BALANCE_DIRECTION from xc_gl_accounts p WHERE p.ACC_ID = t.SUM_ACC_ID) AS DATA_ACC_DIRECTION\nFROM\n\txc_gl_jz_tpl_dtl t\nWHERE\n\tt.JZ_TPL_ID = {?jzTplId?}\n\n"},"children":[],"expanded":false,"type":"sqlswitcher"},{"configs":{"sql":"{#sqlswitcher#}","itemId":"query"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}