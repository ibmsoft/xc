{"inframe":false,"title":"根据账薄ID查询相关信息","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT \n\tle.LEDGER_ID,\n\tLEDGER_CODE,\n\tLEDGER_NAME,\n\tle.ACC_HRCY_ID,\n    hr.ACC_HRCY_NAME,\n\tle.ORG_ID,\n    CONCAT(gs.ORG_CODE,gs.ORG_NAME) AS ORG_NAME,\n\tLEDGER_ACC_DESC\n  FROM \n     XC_GL_LEDGERS le,\n     XIP_PUB_ORGS  gs,\n     XC_GL_ACC_HRCY hr \n WHERE le.ORG_ID = gs.ORG_ID\n   AND le.ACC_HRCY_ID = hr.ACC_HRCY_ID\n   AND le.LEDGER_ID = {?ledger_id?}\n\n","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}