{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"select distinct\n       s.LD_CUST_SEGVAL_ID,\n       s.LEDGER_ID,\n       s.SEG_CODE,\n       s.LD_CUST_SEGVAL_CODE,\n       s.LD_CUST_SEGVAL_NAME,\n       s.UP_CUST_SEGVAL_ID,\n       s.START_DATE,\n       s.LD_CUST_SEGVAL_DESC,\n       s.IS_LEAF,\n       (select LD_CUST_SEGVAL_CODE from xc_gl_ld_cust_ass_segvals s1 where s1.LD_CUST_SEGVAL_ID=s.UP_CUST_SEGVAL_ID) \n       UP_CUST_SEGVAL_CODE,\n       if(s.END_DATE='9999-12-31 00:00:00',null,s.END_DATE) END_DATE,\n       s.LAST_UPDATE_DATE,\n      s.LAST_UPDATED_BY\nfrom xc_gl_ld_cust_ass_segvals s,xc_gl_ld_ass_segments s2\nwhere (s.LD_CUST_SEGVAL_ID={?ID?} or s.UP_CUST_SEGVAL_ID={?ID?})\n      and s2.SEG_CODE=s.SEG_CODE \n      and s2.IS_ENABLED=1  \n      and   s.SEG_CODE = {?seg_code?}\n      and   s.LEDGER_ID = {?ledger_id?} \n      order by \n      s.SEGVAL_LEVEL asc,\n      s.START_DATE asc","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}