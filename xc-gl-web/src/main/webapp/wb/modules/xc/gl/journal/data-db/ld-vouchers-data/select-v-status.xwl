{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT t.VAL_CODE CODE,t.VAL_NAME NAME \nFROM xip_pub_val_set_dtl t \nWHERE t.VAL_SET_CODE = 'XC_GL_V_STATUS' \n  and (case when '{#op#}' = 'record' then\n        1=1\n       when '{#op#}' = 'check' then\n        (t.VAL_CODE in ('2','3'))\n       when '{#op#}' = 'sign' then\n        1=1\n       when '{#op#}' = 'account' then\n        (t.VAL_CODE in ('3','4'))\n       else\n       1=1\n       end)\norder by t.VAL_CODE","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":"seek_icon"}