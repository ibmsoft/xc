{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"itemId":"module"},"children":[{"configs":{"oracle":"select seg.CODE,seg.NAME\nfrom \n(\nSELECT a.VENDOR_CODE AS \"CODE\",a.VENDOR_CODE || '|' || a.VENDOR_NAME AS \"NAME\"\nFROM xc_ap_vendors a,xc_gl_ld_vendors b\nWHERE a.VENDOR_ID = b.VENDOR_ID\n  AND b.LEDGER_ID = {?ledgerId?}\n  AND {?segCode?} = 'XC_AP_VENDORS'\nunion\nSELECT b.CUSTOMER_CODE AS \"CODE\",b.CUSTOMER_CODE || '|' || b.CUSTOMER_NAME AS \"NAME\"\nFROM xc_gl_ld_customers a,xc_ar_customers b\nWHERE a.CUSTOMER_ID = b.CUSTOMER_ID\n  AND b.START_DATE < sysdate\n  AND b.END_DATE > sysdate\n  AND a.LEDGER_ID = {?ledgerId?} \n  AND a.IS_ENABLED = 1 \n  AND {?segCode?} = 'XC_AR_CUSTOMERS'\nunion\nSELECT a.EMP_CODE AS \"CODE\",a.EMP_CODE || '|' || a.EMP_NAME AS \"NAME\"\nFROM xip_pub_emps a,xc_gl_ld_emps b\nWHERE a.EMP_ID = b.EMP_ID\n  AND b.LEDGER_ID ={?ledgerId?}\n  AND {?segCode?} = 'XIP_PUB_EMPS'\nunion\nSELECT a.ORG_CODE AS \"CODE\",a.ORG_CODE || '|' || a.ORG_NAME as \"NAME\"\nFROM xip_pub_orgs a,xc_gl_ld_orgs b\nWHERE a.ORG_ID = b.ORG_ID\n  AND a.ENABLE_FLAG = 'Y'\n  AND b.IS_ENABLED = 1\n  AND NOT EXISTS (select 1 from xip_pub_orgs t where t.UP_ORG_ID = a.ORG_ID)\n  AND b.LEDGER_ID = {?ledgerId?}\n  AND {?segCode?} = 'XIP_PUB_ORGS'\nunion\nSELECT a.PRODUCT_CODE AS \"CODE\",a.PRODUCT_CODE || '|' ||a.PRODUCT_NAME  AS \"NAME\"\nFROM xc_gl_products a,xc_gl_ld_products b\nWHERE a.PRODUCT_ID = b.PRODUCT_ID\n  AND b.LEDGER_ID= {?ledgerId?}\n  AND {?segCode?} = 'XC_GL_PRODUCTS'\nunion\nSELECT a.DEPT_CODE AS \"CODE\",a.DEPT_CODE || '|' || a.DEPT_NAME as \"NAME\"\nFROM xip_pub_depts a,xc_gl_ld_depts b\nWHERE a.DEPT_ID = b.DEPT_ID\n  AND NOT EXISTS (select 1 from xip_pub_depts t where t.UP_DP_ID = a.DEPT_ID)\n  AND b.LEDGER_ID = {?ledgerId?}\n  AND {?segCode?} = 'XIP_PUB_DEPTS'\nunion\nSELECT a.PROJECT_CODE AS \"CODE\",a.PROJECT_CODE || '|' || a.PROJECT_NAME AS \"NAME\"\nFROM xc_pm_projects a\nWHERE a.LEDGER_ID = {?ledgerId?}\n  AND NOT EXISTS (SELECT 1 FROM\txc_pm_projects b WHERE b.UP_PROJECT_ID = a.PROJECT_ID)\n  AND {?segCode?} = 'XC_PM_PROJECTS'\nunion\nSELECT a.LD_CUST_SEGVAL_CODE AS  \"CODE\",a.LD_CUST_SEGVAL_CODE || '|' || a.LD_CUST_SEGVAL_NAME  AS \"NAME\"\nFROM xc_gl_ld_cust_ass_segvals a\nWHERE a.SEG_CODE={?segCode?}\n  AND a.LEDGER_ID = {?ledgerId?}\n  AND a.IS_LEAF = 'Y'\n) seg\norder by seg.CODE","itemId":"sqlswitcher1","mysql":"select seg.CODE,seg.NAME\nfrom \n(\nSELECT a.VENDOR_CODE AS \"CODE\",a.VENDOR_NAME AS \"NAME\"\nFROM xc_ap_vendors a,xc_gl_ld_vendors b\nWHERE a.VENDOR_ID = b.VENDOR_ID\n  AND b.LEDGER_ID = {?ledgerId?}\n  AND {?segCode?} = 'XC_AP_VENDORS'\nunion\nSELECT b.CUSTOMER_CODE AS \"CODE\",b.CUSTOMER_NAME AS \"NAME\"\nFROM xc_gl_ld_customers a,xc_ar_customers b\nWHERE a.CUSTOMER_ID = b.CUSTOMER_ID\n  AND b.START_DATE < NOW()\n  AND b.END_DATE > NOW()\n  AND a.LEDGER_ID = {?ledgerId?} \n  AND a.IS_ENABLED = 1 \n  AND {?segCode?} = 'XC_AR_CUSTOMERS'\nunion\nSELECT a.EMP_CODE AS \"CODE\",a.EMP_NAME AS \"NAME\"\nFROM xip_pub_emps a,xc_gl_ld_emps b\nWHERE a.EMP_ID = b.EMP_ID\n  AND b.LEDGER_ID ={?ledgerId?}\n  AND {?segCode?} = 'XIP_PUB_EMPS'\nunion\nSELECT a.ORG_CODE AS \"CODE\",a.ORG_NAME as \"NAME\"\nFROM xip_pub_orgs a,xc_gl_ld_orgs b\nWHERE a.ORG_ID = b.ORG_ID\n  AND a.ENABLE_FLAG = 'Y'\n  AND b.IS_ENABLED = 1\n  AND NOT EXISTS (select 1 from xip_pub_orgs t where t.UP_ORG_ID = a.ORG_ID)\n  AND b.LEDGER_ID = {?ledgerId?}\n  AND {?segCode?} = 'XIP_PUB_ORGS'\nunion\nSELECT a.PRODUCT_CODE AS \"CODE\",a.PRODUCT_NAME  AS \"NAME\"\nFROM xc_gl_products a,xc_gl_ld_products b\nWHERE a.PRODUCT_ID = b.PRODUCT_ID\n  AND b.LEDGER_ID= {?ledgerId?}\n  AND {?segCode?} = 'XC_GL_PRODUCTS'\nunion\nSELECT a.DEPT_CODE AS \"CODE\",a.DEPT_NAME AS \"NAME\"\nFROM xip_pub_depts a,xc_gl_ld_depts b\nWHERE a.DEPT_ID = b.DEPT_ID\n  AND NOT EXISTS (select 1 from xip_pub_depts t where t.UP_DP_ID = a.DEPT_ID)\n  AND b.LEDGER_ID = {?ledgerId?}\n  AND {?segCode?} = 'XIP_PUB_DEPTS'\nunion\nSELECT a.PROJECT_CODE AS \"CODE\",a.PROJECT_NAME AS \"NAME\"\nFROM xc_pm_projects a\nWHERE a.LEDGER_ID = {?ledgerId?}\n  AND NOT EXISTS (SELECT 1 FROM xc_pm_projects b WHERE b.UP_PROJECT_ID = a.PROJECT_ID)\n  AND {?segCode?} = 'XC_PM_PROJECTS'\nunion\nSELECT a.LD_CUST_SEGVAL_CODE AS  \"CODE\",a.LD_CUST_SEGVAL_NAME AS \"NAME\"\nFROM xc_gl_ld_cust_ass_segvals a\nWHERE a.SEG_CODE={?segCode?}\n  AND a.LEDGER_ID = {?ledgerId?}\n  AND a.IS_LEAF = 'Y'\n) seg\norder by seg.CODE"},"children":[],"expanded":false,"type":"sqlswitcher"},{"configs":{"sql":"{#sqlswitcher1#}","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":"seek_icon"}