{"inframe":false,"title":"账簿科目查询","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"oracle":"SELECT\n\tgla.LEDGER_ID,\n\tgla.LD_ACC_ID,\n\tgla.START_DATE,\n\tgla.END_DATE,\n\tgla.IS_FOREGIN_CNY,\n\tgla.DEFAULT_CURRENCY,\n\tgc.CURRENCY_NAME,\n\tgla.IS_AMOUNT,\n\tgla.DIM_ID,\n\tgd.DIM_NAME,\n\tgla.BG_ITEM_ID,\n\tbi.BG_ITEM_NAME,\n\tgla.LAST_UPDATED_BY,\n\tgla.LAST_UPDATE_DATE,\n\tgla.CREATION_DATE,\n\tgla.CREATED_BY,\n\tgla.ACC_SEG_UNION,\n\tga.ACC_ID,\n\tga.ACC_CODE,\n\tga.ACC_NAME,\n\tga.ACC_CATEGORY_CODE,\n\tgac.ACC_CATEGORY_NAME,\n\tga.ACC_GROUP_ID,\n\tgag.ACC_GROUP_NAME,\n\tga.BALANCE_DIRECTION,\n\tga.ACC_HRCY_ID,\n\tga.IS_BANK_ACC,\n\tga.IS_CASH_ACC,\n\tga.IS_CASH_ITEM,\n\tga.START_DATE AS \"ACC_START_DATE\",\n\tga.END_DATE AS \"ACC_END_DATE\",\n\tgl.LEDGER_NAME\nFROM xc_gl_ledgers gl,\n     xc_gl_acc_category gac,\n     xc_gl_accounts ga\n     LEFT JOIN xc_gl_acc_group gag ON ga.ACC_GROUP_ID = gag.ACC_GROUP_ID,\n     xc_gl_ld_accounts gla\n     LEFT JOIN xc_gl_currencies gc ON gla.DEFAULT_CURRENCY = gc.CURRENCY_CODE\n     LEFT JOIN xc_gl_dimensions gd ON gla.DIM_ID = gd.DIM_ID\n     LEFT JOIN xc_bg_items bi ON gla.BG_ITEM_ID = bi.BG_ITEM_ID \nWHERE gla.LEDGER_ID = gl.LEDGER_ID \nand gla.ACC_ID = ga.ACC_ID\nand ga.ACC_CATEGORY_CODE = gac.ACC_CATEGORY_CODE\nand (ga.ACC_CODE LIKE '%{#ACC_INFO%#}' OR ga.ACC_NAME LIKE '%{#ACC_INFO%#}%' )\nAND gac.ACC_CATEGORY_NAME LIKE '{#ACC_CATEGORY_INFO#}%'\nAND gl.LEDGER_ID = {?LEDGER_ID?}\nAND ga.ACC_GROUP_ID = decode(ga.ACC_GROUP_ID,null,ga.ACC_GROUP_ID,{?ACC_GROUP_ID?})\nORDER BY ga.ACC_CODE ASC","itemId":"sqlswitcher1","mysql":"SELECT\n\tgla.LEDGER_ID,\n\tgla.LD_ACC_ID,\n\tgla.START_DATE,\n\tgla.END_DATE,\n\tgla.IS_FOREGIN_CNY,\n\tgla.DEFAULT_CURRENCY,\n\tgc.CURRENCY_NAME,\n\tgla.IS_AMOUNT,\n\tgla.DIM_ID,\n\tgd.DIM_NAME,\n\tgla.BG_ITEM_ID,\n\tbi.BG_ITEM_NAME,\n\tgla.LAST_UPDATED_BY,\n\tgla.LAST_UPDATE_DATE,\n\tgla.CREATION_DATE,\n\tgla.CREATED_BY,\n\tgla.ACC_SEG_UNION,\n\tga.ACC_ID,\n\tga.ACC_CODE,\n\tga.ACC_NAME,\n\tga.ACC_CATEGORY_CODE,\n\tgac.ACC_CATEGORY_NAME,\n\tga.ACC_GROUP_ID,\n\tgag.ACC_GROUP_NAME,\n\tga.BALANCE_DIRECTION,\n\tga.ACC_HRCY_ID,\n\tga.IS_BANK_ACC,\n\tga.IS_CASH_ACC,\n\tga.IS_CASH_ITEM,\n\tga.START_DATE AS \"ACC_START_DATE\",\n\tga.END_DATE AS \"ACC_END_DATE\",\n\tgl.LEDGER_NAME\nFROM xc_gl_ledgers gl,\n     xc_gl_acc_category gac,\n     xc_gl_accounts ga\n     LEFT JOIN xc_gl_acc_group gag ON ga.ACC_GROUP_ID = gag.ACC_GROUP_ID,\n     xc_gl_ld_accounts gla\n     LEFT JOIN xc_gl_currencies gc ON gla.DEFAULT_CURRENCY = gc.CURRENCY_CODE\n     LEFT JOIN xc_gl_dimensions gd ON gla.DIM_ID = gd.DIM_ID\n     LEFT JOIN xc_bg_items bi ON gla.BG_ITEM_ID = bi.BG_ITEM_ID \nWHERE gla.LEDGER_ID = gl.LEDGER_ID \nand gla.ACC_ID = ga.ACC_ID\nand ga.ACC_CATEGORY_CODE = gac.ACC_CATEGORY_CODE\nand (ga.ACC_CODE LIKE '%{#ACC_INFO#}%' OR ga.ACC_NAME LIKE '%{#ACC_INFO#}%')\nAND gac.ACC_CATEGORY_NAME LIKE '{#ACC_CATEGORY_INFO#}%'\nAND gl.LEDGER_ID = {?LEDGER_ID?}\nAND IFNULL(ga.ACC_GROUP_ID,'%%') like '{#ACC_GROUP_ID#}%'\nORDER BY ga.ACC_CODE ASC"},"children":[],"expanded":false,"type":"sqlswitcher"},{"configs":{"sql":"{#sqlswitcher1#}","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}