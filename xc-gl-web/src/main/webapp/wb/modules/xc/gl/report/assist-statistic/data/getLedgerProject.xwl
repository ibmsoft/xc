{"inframe":false,"title":"获取账簿项目信息","hidden":true,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"itemId":"sqlswitcher","oracle":"SELECT\n\ta.PROJECT_CODE AS \"CODE\",\n    a.PROJECT_CODE || '|' || a.PROJECT_NAME AS \"NAME\"\nFROM\n\txc_pm_projects a\nWHERE\n\ta.LEDGER_ID = {?ledgerId?}\nAND NOT EXISTS (\n\tSELECT\n\t\t1\n\tFROM\n\t\txc_pm_projects b\n\tWHERE\n\t\tb.UP_PROJECT_ID = a.PROJECT_ID\n)\nORDER BY\n\ta.PROJECT_CODE ASC","mysql":"SELECT\n\ta.PROJECT_CODE AS \"CODE\",\n\tCONCAT(\n\t\ta.PROJECT_CODE,\n\t\t'|',\n\t\ta.PROJECT_NAME\n\t) AS \"NAME\"\nFROM\n\txc_pm_projects a\nWHERE\n\ta.LEDGER_ID = {?ledgerId?}\nAND NOT EXISTS (\n\tSELECT\n\t\t1\n\tFROM\n\t\txc_pm_projects b\n\tWHERE\n\t\tb.UP_PROJECT_ID = a.PROJECT_ID\n)\nORDER BY\n\ta.PROJECT_CODE ASC"},"children":[],"expanded":false,"type":"sqlswitcher"},{"configs":{"sql":"{#sqlswitcher#}","itemId":"getLedgerProject"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}