{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT T.LEDGER_ID as \"CODE\", T.LEDGER_CODE LEDGER_CODE, T.LEDGER_NAME as \"NAME\"\n  FROM XC_GL_LEDGERS T\n WHERE (T.LEDGER_ID IN (SELECT LEDGER_ID\n          FROM XC_GL_LD_SEC A\n         WHERE T.LEDGER_ID = A.LEDGER_ID\n           AND A.USER_ID = {?XIP.userId?})\n OR  T.LEDGER_ID IN (SELECT LEDGER_ID\n          FROM XIP_PUB_USER_ROLES XUR, XIP_PUB_ROLE XR, XC_GL_LD_SEC XLC\n         WHERE XUR.ROLE_ID = XR.ROLE_ID\n           AND XR.ROLE_ID = XLC.ROLE_ID\n           AND XLC.LEDGER_ID = T.LEDGER_ID\n           AND XUR.USER_ID = {?XIP.userId?}))","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}