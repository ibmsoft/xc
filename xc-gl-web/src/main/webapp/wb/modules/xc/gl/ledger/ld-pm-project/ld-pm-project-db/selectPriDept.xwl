{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"  select xpd.PRJ_DEPT_ID,\n         xpm.LEDGER_ID,\n         xpm.PROJECT_ID,\n         xpm.PROJECT_CODE,\n         xpm.PROJECT_NAME,\n         xpd.DEPT_ID,\n         xpd.USER_ID,\n         xd.DEPT_NAME,\n         xue.EMP_NAME \n  from xc_pm_projects xpm, xc_pm_prj_dept xpd\n    LEFT JOIN xip_pub_depts xd on xpd.DEPT_ID = xd.DEPT_ID\n    LEFT JOIN (select xu.USER_ID,xu.USER_NAME,xe.EMP_ID,xe.EMP_CODE,xe.EMP_NAME\n        from xip_pub_users xu,xip_pub_emps xe \n        where xu.EMP_ID = xe.EMP_ID ) xue on xpd.USER_ID = xue.USER_ID\n  where xpm.PROJECT_ID = xpd.PROJECT_ID\n    and xpd.LEDGER_ID = {?LEDGER_ID?}\n    and xpm.PROJECT_ID = {?PROJECT_ID?}\n  ORDER BY xpm.PROJECT_CODE, xd.dept_code asc","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}