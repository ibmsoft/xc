{"inframe":false,"title":"查询账簿级收付款方式","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT B.LD_MODE_ID,\n       B.LEDGER_ID,\n       B.MODE_ID,\n       C.MODE_TYPE,\n       C.MODE_NAME,\n       C.MODE_DESC,\n       B.PAY_ACC_ID,\n       D.ACC_NAME AS PAY_ACC_NAME,\n       B.REC_ACC_ID,\n       E.ACC_NAME AS REC_ACC_NAME\n  FROM XC_GL_LD_PR_MODES B LEFT JOIN xc_gl_accounts D on B.PAY_ACC_ID = D.ACC_ID left JOIN xc_gl_accounts E on B.REC_ACC_ID = E.ACC_ID,\nXC_GL_PR_MODES C\n WHERE B.MODE_ID = C.MODE_ID\n   AND B.LEDGER_ID = {?ledger_id?}\n","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}