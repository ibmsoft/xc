{"inframe":false,"title":"根据账薄ID查询账薄具体信息","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT\n\tle.LEDGER_ID,\n    le.LEDGER_CODE,\n    le.LEDGER_NAME,\n    le.ACC_HRCY_ID,\n    hr.ACC_HRCY_NAME,\n    le.CURRENCY_CODE,\n    cu.CURRENCY_NAME,\n    le.START_PERIOD_CODE,\n    le.RETAINED_ACC_ID, \n    c1.CCID_NAME AS RETAINED_ACC_NAME,\n    c2.CCID_NAME AS EXCHANGE_ACC_NAME,\n    le.EXCHANGE_ACC_ID,\n    le.BG_HRCY_ID,\n    bghr.BG_HRCY_NAME,\n    le.EX_HRCY_ID,\n    exhr.EX_HRCY_NAME\nFROM\n    XC_GL_LEDGERS le\n\nLEFT JOIN XC_GL_CURRENCIES cu ON le.CURRENCY_CODE = cu.CURRENCY_CODE\nLEFT JOIN XC_GL_CCID c1 ON le.RETAINED_ACC_ID = c1.CCID\nLEFT JOIN XC_GL_CCID c2 ON le.EXCHANGE_ACC_ID = c2.CCID\nLEFT JOIN XC_GL_ACC_HRCY hr ON le.ACC_HRCY_ID = hr.ACC_HRCY_ID\nLEFT JOIN XC_BG_HRCY bghr ON le.BG_HRCY_ID = bghr.BG_HRCY_ID\nLEFT JOIN XC_EX_HRCY exhr ON le.EX_HRCY_ID = exhr.EX_HRCY_ID\n WHERE le.LEDGER_ID = {?ledger_id?}\n","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}