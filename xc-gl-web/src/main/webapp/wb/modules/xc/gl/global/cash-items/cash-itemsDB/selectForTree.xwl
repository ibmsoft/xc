{"inframe":false,"title":"生成树","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"oracle":"select CA_NAME \"text\",\n\t   CA_ID \"id\",\n\t   CA_CODE \"code\",\n\t   UP_ID   \"parent_id\",\n\t\tIS_LEAF as \"IS_LEAF\",\n\t\tCA_LEVEL as \"CA_LEVEL\",\n\t\tCA_DIRECTION as \"CA_DIRECTION\",\n\t\tASS_KEY as \"ASS_KEY\",\n\t\tCA_DESC as \"CA_DESC\",\n\t\tSTART_DATE as \"START_DATE\",\n\t\tEND_DATE as \"END_DATE\",\n\t\tORDERBY as \"ORDERBY\",\n\t\t(select a.CA_CODE from XC_GL_CASH_ITEMS a where a.CA_ID = t.UP_ID) \"UP_CODE\",\n\t\tcase\n         when (select count(*)\n                 from XC_GL_CASH_ITEMS t1\n                where t1.UP_ID = t.CA_ID) > 0 then\n          'false'\n         else\n          'true'\n \t\tend as \"leaf\"\n\tfrom XC_GL_CASH_ITEMS t\nwhere UP_ID ={?parentID?} or ( UP_ID = '-1' and UP_ID ='')\n","itemId":"sqlswitcher1","mysql":"select CA_NAME \"text\",\n\t   CA_ID \"id\",\n\t   CA_CODE \"code\",\n\t   UP_ID   \"parent_id\",\n\t\tIS_LEAF as \"IS_LEAF\",\n\t\tCA_LEVEL as \"CA_LEVEL\",\n\t\tCA_DIRECTION as \"CA_DIRECTION\",\n\t\tASS_KEY as \"ASS_KEY\",\n\t\tCA_DESC as \"CA_DESC\",\n\t\tSTART_DATE as \"START_DATE\",\n\t\tEND_DATE as \"END_DATE\",\n\t\tORDERBY as \"ORDERBY\",\n\t\t(select a.CA_CODE from XC_GL_CASH_ITEMS a where a.CA_ID = t.UP_ID) \"UP_CODE\",\n\t\tcase\n         when (select count(*)\n                 from XC_GL_CASH_ITEMS t1\n                where t1.UP_ID = t.CA_ID) > 0 then\n          'false'\n         else\n          'true'\n \t\tend as \"leaf\"\n\tfrom XC_GL_CASH_ITEMS t\nwhere UP_ID ={?parentID?} or ( UP_ID = '-1' and UP_ID ='')\norder by CA_CODE\n"},"children":[],"expanded":false,"type":"sqlswitcher"},{"configs":{"sql":"{#sqlswitcher1#}","itemId":"dataprovider1","type":"tree"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}