{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"itemId":"sqlswitcher1","oracle":"SELECT T.PERIOD_CODE, T.V_SERIAL_NUM, T.SUMMARY, T.T_DR, T.T_CR, T.CREATED_NAME,T.CREATION_DATE, t.V_ATTCH_TOTAL ,\n\t\t\tT.VERIFIER, T.SIGNATORY, T.BOOKKEEPER, T.V_STATUS, T.V_SRC_CODE, T.V_HEAD_ID, T.LEDGER_ID, \n\t\t\tT.V_CATEGORY_ID, T.IS_SIGNED, T.IS_WRITE_OFF, T.WRITE_OFF_NUM, T.V_TEMPLATE_TYPE\n  FROM XC_GL_V_HEADS T\nwhere t.LEDGER_ID ={?ledger?}\n  and to_date(t.creation_date,'yyyy-mm-dd') between to_date({?startDate?},'yyyy-mm-dd') and date_format({?to_date?},'yyyy-mm-dd')\n  and t.v_category_id = decode({?vCategoryId?},'ALL',t.v_category_id,{?vCategoryId?})\n  and t.v_template_type = decode({?templateType?},'ALL',t.v_template_type,{?templateType?})\n  and t.v_serial_num like '%{#vSerialNO#}%'\n  and exists (select 1 from xc_gl_v_lines xl,xc_gl_ccid xcc\n              where xl.CCID = xcc.CCID\n                and xl.V_HEAD_ID = t.V_HEAD_ID\n                and (xcc.ACC_CODE like '%{#account#}%' or xcc.ACC_NAME like '%{#account#}%')\n             )\n  and {#statusCondition#}\n  and t.t_dr >= decode({?startAmount?},'',t.t_dr,{?startAmount?}) \n  and t.t_dr <= decode({?endAmount?},'',t.t_dr,{?endAmount?})\n  and t.CREATED_NAME like '%{#createdByName#}%'\n  and t.SUMMARY like '%{#summary#}%'\n  and (case when '{#op#}' = 'record' then\n\t\t\t1=1\n       when '{#op#}' = 'check' then\n       \t\t(exists (select 1 from XC_GL_V_HEADS a \n                        where t.V_HEAD_ID = a.v_head_id \n                          and a.V_STATUS = '2' and a.BOOKKEEPER is null ))\n       when '{#op#}' = 'sign' then\n       \t\t(exists (select 1 from XC_GL_V_HEADS a \n                        where t.V_HEAD_ID = a.v_head_id \n                          and a.V_STATUS = '2' and a.BOOKKEEPER_ID is null\n                          and a.IS_SIGNED = 'Y'))\n       when '{#op#}' = 'account' then\n       \t\t(exists (select 1 from XC_GL_V_HEADS a \n                     \twhere t.V_HEAD_ID = a.v_head_id \n                          and a.VERIFIER_ID is not null AND NVL(IS_WRITE_OFF,'N') <> 'Y'\n                          and (case a.IS_SIGNED when 'Y' then a.SIGNATORY_ID is not null else a.SIGNATORY_ID is NULL end) \n                    ))\n       else\n       \t\t1=1\n       end)\n","mysql":"SELECT T.PERIOD_CODE, T.V_SERIAL_NUM, T.SUMMARY, T.T_DR, T.T_CR, T.CREATED_NAME,T.CREATION_DATE, t.V_ATTCH_TOTAL ,\n\t\t\tT.VERIFIER, T.SIGNATORY, T.BOOKKEEPER, T.V_STATUS, T.V_SRC_CODE, T.V_HEAD_ID, T.LEDGER_ID, \n\t\t\tT.V_CATEGORY_ID, T.IS_SIGNED, T.IS_WRITE_OFF, T.WRITE_OFF_NUM, T.V_TEMPLATE_TYPE\n  FROM XC_GL_V_HEADS T\nwhere t.LEDGER_ID ={?ledger?}\n  and date_format(t.creation_date,'%Y-%m-%d') \n\t\t\tbetween date_format(if({?startDate?}<>'',{?startDate?},'1900-01-01'),'%Y-%m-%d') \n\t\t\tand date_format(if({?endDate?}<>'',{?endDate?},'9999-01-01'),'%Y-%m-%d')\n  and t.v_category_id = if({?vCategoryId?}<>'ALL',{?vCategoryId?},t.v_category_id)\n  and t.v_template_type = if({?templateType?}<>'ALL',{?templateType?},t.v_template_type)\n  and IFNULL(t.v_serial_num,'%') like '%{#vSerialNO#}%'\n  and exists (select 1 from xc_gl_v_lines xl,xc_gl_ccid xcc\n              where xl.CCID = xcc.CCID \n                and xl.V_HEAD_ID = t.V_HEAD_ID\n                and (xcc.ACC_CODE like '%{#account#}%' or xcc.ACC_NAME like '%{#account#}%'))\n  and {#statusCondition#}\n  and t.t_dr >= if({?startAmount?}<>'',{?startAmount?},t.t_dr) \n  and t.t_dr <= if({?endAmount?}<>'',{?endAmount?},t.t_dr)\n  and IFNULL(t.CREATED_NAME,'%') like '%{#createdByName#}%'\n  and IFNULL(t.SUMMARY,'%') like '%{#summary#}%'\n  and (case when '{#op#}' = 'record' then\n\t\t\t1=1\n       when '{#op#}' = 'check' then\n       \t\t(exists (select 1 from XC_GL_V_HEADS a \n                        where t.V_HEAD_ID = a.v_head_id \n                          and a.V_STATUS = '2' and a.BOOKKEEPER is null ))\n       when '{#op#}' = 'sign' then\n       \t\t(exists (select 1 from XC_GL_V_HEADS a \n                        where t.V_HEAD_ID = a.v_head_id \n                          and a.V_STATUS = '2' and a.BOOKKEEPER_ID is null\n                          and a.IS_SIGNED = 'Y'))\n       when '{#op#}' = 'account' then\n       \t\t(exists (select 1 from XC_GL_V_HEADS a \n                     \twhere  t.V_HEAD_ID = a.v_head_id \n                          and a.VERIFIER_ID is not null AND IFNULL(IS_WRITE_OFF,'N') <> 'Y'\n                           and  (case a.IS_SIGNED when 'Y' then a.SIGNATORY_ID is not null else a.SIGNATORY_ID is NULL end) \n                    ))\n       else\n       \t\t1=1\n       end)"},"children":[],"expanded":false,"type":"sqlswitcher"},{"configs":{"sql":"{#sqlswitcher1#}","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":"seek_icon"}