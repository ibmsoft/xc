{"inframe":false,"title":"预算项表查询","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT\n\tbi.BG_ITEM_ID,\n\tbi.BG_ITEM_CODE,\n\tbi.BG_ITEM_NAME,\n\tbli.BG_LD_ITEM_ID\nFROM\n\txc_bg_ld_items bli,\n\txc_bg_items bi\nWHERE\n\tbli.BG_ITEM_ID = bi.BG_ITEM_ID\nAND bli.LEDGER_ID = {?LEDGER_ID?}\nAND (bi.BG_ITEM_NAME LIKE '%{#item_name#}%' OR bi.BG_ITEM_CODE LIKE '%{#item_name#}%')\nAND ({?NOWDATE?} BETWEEN bi.START_DATE AND bi.END_DATE)\nAND bi.BG_ITEM_UP_ID <> '-1'\nAND bli.IS_ENABLED = 'Y'","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}