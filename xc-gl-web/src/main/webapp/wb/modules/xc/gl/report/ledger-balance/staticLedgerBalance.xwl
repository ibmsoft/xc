     map.add('startPeriodCode',app.startPeriodCode.getValue());\n  }\n  if(Ext.isEmpty(app.endPeriodCode.getValue())){\n     map.add('endPeriodCode','0');\n  }else{\n     map.add('endPeriodCode',app.endPeriodCode.getValue());\n  }\n  //科目组编码\n  if(Ext.isEmpty(app.accGroup.getValue())){\n    map.add('accGroupId','-1');\n  }else{\n    map.add('accGroupCode',app.accGroup.getValue());\n  }\n  //科目起始编码\n  if(Ext.isEmpty(app.startAccCode.getValue())){\n     map.add('startAccCode','0');\n  }else{\n     map.add('startAccCode',app.startAccCode.getValue());\n  }\n  if(Ext.isEmpty(app.endAccCode.getValue())){\n     map.add('endAccCode','0');\n  }else{\n     map.add('endAccCode',app.endAccCode.getValue());\n  } \n  //汇总层级 \n  map.add('summaryLevel',app.summaryLevel.getValue());\n  //币种--默认为账簿本位币币种\n  map.add('currencyCode',app.ledgerCurrency.getValue());\n  map.add('currencyName',app.ledgerCurrency.getRawValue());\n  //组织名称\n  map.add('orgName',app.orgName.getValue());\n  map.add('defaultCurrency',app.oriCurrencyCode.getValue());\n//金额式\n  var targetUrl = '{#contextPath#}/ReportServer?reportlet=xc_balances/ledger_balance_cash.cpt&__bypagesize__=false';\n  if('1'==app.accountCheck.getValue()){\n    targetUrl = '{#contextPath#}/ReportServer?reportlet=xc_balances/ledger_balance_cash_t.cpt&__bypagesize__=false';\n  }\n//外币金额式\n  if('1'==app.ledgerFormat.getValue()){\n    targetUrl = '{#contextPath#}/ReportServer?reportlet=xc_balances/ledger_balance_foreign.cpt&__bypagesize__=false';\n    if('1'==app.accountCheck.getValue()){\n      targetUrl = '{#contextPath#}/ReportServer?reportlet=xc_balances/ledger_balance_foreign_t.cpt&__bypagesize__=false';\n  }\n  }\n//数量金额式\n  if('2'==app.ledgerFormat.getValue()){\n    targetUrl = '{#contextPath#}/ReportServer?reportlet=xc_balances/ledger_balance_amount.cpt&__bypagesize__=false';\n    if('1'==app.accountCheck.getValue()){\n      targetUrl = '{#contextPath#}/ReportServer?reportlet=xc_balances/ledger_balance_amount_t.cpt&__bypagesize__=false';