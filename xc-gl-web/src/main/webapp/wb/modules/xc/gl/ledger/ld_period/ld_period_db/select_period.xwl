{"inframe":false,"title":"查询账薄中不包含的全局会计期","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT PERIOD_CODE,YEAR,QUARTER,START_DATE,END_DATE\n   FROM XC_GL_PERIOD  \nWHERE END_DATE>= (select END_DATE from XC_GL_PERIOD where PERIOD_CODE={?period_code?})\n  AND START_DATE>= (select START_DATE from XC_GL_PERIOD where PERIOD_CODE={?period_code?})\n  AND PERIOD_CODE LIKE '%{#query_period_code#}%'\n  AND PERIOD_CODE NOT IN(\n    SELECT PERIOD_CODE FROM xc_gl_ld_period \n    WHERE LEDGER_ID = {?ledger_id?}\n  )\n AND (IS_ENABLED = 1 OR IS_ENABLED = '1')\n ORDER BY START_DATE\n","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":"seek_icon"}