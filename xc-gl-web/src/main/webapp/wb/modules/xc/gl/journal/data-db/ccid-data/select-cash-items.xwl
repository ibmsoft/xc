{"inframe":false,"title":"查询现金流量项目信息","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"oracle":"select t.CA_ID,t.CA_CODE,\n\t(t.CA_NAME||(case when t.CA_DIRECTION = 1 then\n     ' | 流入' \n\t when t.CA_DIRECTION = 1 then\n     \tt.CA_NAME||' | 流出' \n     else\n        ''\n     end)) CA_NAME,\n\tt.CA_CODE||t.CA_NAME as CA_DISPLAY_NAME \nfrom xc_gl_cash_items t order by t.CA_CODE","itemId":"sqlswitcher1","mysql":"select \n\tt.CA_ID,\n\tt.CA_CODE,\n\tCONCAT(t.CA_NAME,IF(t.CA_DIRECTION='1',' | 流入',IF(t.CA_DIRECTION='-1',' | 流出',''))) as CA_NAME,\n\tCONCAT(t.CA_CODE,t.CA_NAME) as CA_DISPLAY_NAME \nfrom xc_gl_cash_items t \nwhere t.IS_LEAF = 'Y'\norder by t.CA_CODE"},"children":[],"expanded":false,"type":"sqlswitcher"},{"configs":{"sql":"{#sqlswitcher1#}","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":"seek_icon"}