{"inframe":false,"title":"获取账簿库内部往来信息","hidden":true,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"itemId":"sqlswitcher","oracle":"SELECT\n\ta.ORG_CODE AS \"CODE\",\n    a.ORG_CODE || '|' || a.ORG_NAME as \"NAME\"\nFROM\n\txip_pub_orgs a,\n\txc_gl_ld_orgs b\nWHERE\n\ta.ORG_ID = b.ORG_ID\nAND a.ENABLE_FLAG = 'Y'\nAND b.IS_ENABLED = 1\nAND NOT EXISTS (select 1 from xip_pub_orgs t where t.UP_ORG_ID = a.ORG_ID)\nAND b.LEDGER_ID = {?ledgerId?}","mysql":"SELECT\n\ta.ORG_CODE AS \"CODE\",\n  CONCAT(a.ORG_CODE,'|',a.ORG_NAME) as \"NAME\"\nFROM\n\txip_pub_orgs a,\n\txc_gl_ld_orgs b\nWHERE\n\ta.ORG_ID = b.ORG_ID\nAND a.ENABLE_FLAG = 'Y'\nAND b.IS_ENABLED = 1\nAND NOT EXISTS (select 1 from xip_pub_orgs t where t.UP_ORG_ID = a.ORG_ID)\nAND b.LEDGER_ID = {?ledgerId?}"},"children":[],"expanded":false,"type":"sqlswitcher"},{"configs":{"sql":"{#sqlswitcher#}","itemId":"getLedgerOrg"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}