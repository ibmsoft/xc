{"inframe":false,"title":"查询账簿会计期","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT\n\tlp.LEDGER_ID,\n\tlp.PERIOD_CODE,\n\tlp.LD_PERIOD_STATUS,\n\tdtl.VAL_NAME AS LD_PERIOD_STATUS_NAME,\n\tp.YEAR,\n\tp.QUARTER,\n\tp.START_DATE,\n\tp.END_DATE,\n\tp.IS_ADJ_PERIOD\nFROM xc_gl_period p\nLEFT JOIN xc_gl_ld_period lp ON p.PERIOD_CODE = lp.PERIOD_CODE\nLEFT JOIN XIP_PUB_VAL_SET_DTL dtl ON lp.LD_PERIOD_STATUS = dtl.VAL_CODE AND VAL_SET_CODE = 'XC_GL_PERIOD_STATUS'\nWHERE lp.LEDGER_ID = {?ledger_id?}\n  AND IFNULL(p.YEAR,'%') like '%{#year#}%'\n  AND lp.PERIOD_CODE not in (select b.PERIOD_CODE from xc_fa_ld_periods b where b.LEDGER_ID = {?ledger_id?})\n  AND lp.PERIOD_CODE >= (select substring(p.PARAMS, 14, 7) from xc_fa_ld_parms p where p.LEDGER_ID = lp.LEDGER_ID)\nORDER BY p.START_DATE desc","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}