{"inframe":false,"title":"查询项目","hidden":false,"roles":{"default":1},"children":[{"configs":{"itemId":"module"},"children":[{"configs":{"sql":"select p.PROJECT_ID,\n       p.PROJECT_CODE,\n       p.PROJECT_NAME\n  from xc_pm_projects p \n where p.start_date <= CURRENT_TIMESTAMP()\n   and p.end_date >= CURRENT_TIMESTAMP()\n   and (p.PROJECT_CODE like '%{#textStr#}%' or p.PROJECT_NAME like '%{#textStr#}%')\n   and p.LEDGER_ID = {?LEDGER_ID?}\n order by p.PROJECT_CODE asc","itemId":"dataprovider"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}