{"inframe":false,"title":"查询会计平台头","hidden":false,"roles":{"default":1},"children":[{"configs":{"itemId":"module"},"children":[{"configs":{"sql":"select * \n  from (select h.*,\n               (select l.LEDGER_NAME from xc_gl_ledgers l where l.LEDGER_ID = h.LEDGER_ID) LEDGER_NAME,\n               vh.V_SERIAL_NUM,\n               case when vh.V_STATUS = 1 then 'qc'\n                    when vh.V_STATUS = 2 and vh.VERIFIER_ID is not null and (vh.BOOKKEEPER_ID is null or vh.BOOKKEEPER_ID = '')  then 'ysh'\n                    when vh.V_STATUS = 2 and vh.VERIFIER_ID is not null and vh.BOOKKEEPER_ID is null then 'yjz' end V_STATUS\n          from xc_gl_v_interface_heads h\n          left join xc_gl_v_heads vh on h.V_HEAD_ID = vh.V_HEAD_ID\n        LEFT JOIN xc_gl_v_interface gvi ON h.I_HEAD_ID = gvi.I_HEAD_ID\n         where h.LEDGER_ID = {?ledger?}\n           and h.PERIOD_CODE like '%{#period#}%'\n        \t AND gvi.TRANS_CODE {#TRANS_CODE#}) s\n where s.V_STATUS like '%{#status#}%'","itemId":"dataprovider"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}