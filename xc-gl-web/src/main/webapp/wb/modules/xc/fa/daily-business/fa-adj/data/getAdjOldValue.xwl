{"inframe":false,"title":"查询调整前值","hidden":false,"roles":{"default":1},"children":[{"configs":{"itemId":"module"},"children":[{"configs":{"sql":"SELECT CASE WHEN {?adjType?} = 'ZJFF' THEN a.ZJFF\n            WHEN {?adjType?} = 'ZCYT' THEN a.ZCYT\n            WHEN {?adjType?} = 'CAT_CODE' THEN a.CAT_CODE\n            WHEN {?adjType?} = 'ZCKM' THEN a.ZCKM\n            WHEN {?adjType?} = 'YZ' THEN a.YZ\n            WHEN {?adjType?} = 'SYNX' THEN a.SYNX\n            WHEN {?adjType?} = 'SFZJ' THEN a.SFZJ\n            WHEN {?adjType?} = 'JZZBKM' THEN a.JZZBKM\n            WHEN {?adjType?} = 'LJZJTXKM' THEN a.LJZJTXKM\n            WHEN {?adjType?} = 'CZL' THEN a.CZL\n            WHEN {?adjType?} = 'LJZJ_LJTX' THEN a.LJZJ_LJTX\n            WHEN {?adjType?} = 'CBZX' THEN a.CBZX\n            WHEN {?adjType?} = 'JZZB' THEN a.JZZB END OLD_VALUE,\n       CASE WHEN {?adjType?} = 'ZJFF' THEN (SELECT m.METHOD_NAME FROM xc_fa_depr_methods m WHERE m.METHOD_CODE = a.ZJFF)\n            WHEN {?adjType?} = 'ZCYT' THEN (SELECT u.USE_NAME FROM xc_fa_uses u WHERE u.USE_CODE = a.ZCYT)\n            WHEN {?adjType?} = 'CAT_CODE' THEN (SELECT c.CAT_NAME FROM xc_fa_cats c WHERE c.CAT_CODE = a.CAT_CODE)\n            WHEN {?adjType?} = 'ZCKM' THEN (SELECT acc.ACC_NAME \n                                              FROM xc_gl_accounts acc \n                                             WHERE acc.ACC_CODE = a.ZCKM \n                                               AND acc.ACC_HRCY_ID = (SELECT l.ACC_HRCY_ID \n                                                                        FROM xc_gl_ledgers l \n                                                                       WHERE l.ACC_HRCY_ID = acc.ACC_HRCY_ID \n                                                                         AND l.LEDGER_ID = a.LEDGER_ID))\n            WHEN {?adjType?} = 'YZ' THEN a.YZ\n            WHEN {?adjType?} = 'SYNX' THEN a.SYNX\n            WHEN {?adjType?} = 'SFZJ' THEN (CASE WHEN a.SFZJ = 'Y' THEN '是' ELSE '否' END)\n            WHEN {?adjType?} = 'JZZBKM' THEN (SELECT acc.ACC_NAME \n                                                FROM xc_gl_accounts acc \n                                               WHERE acc.ACC_CODE = a.JZZBKM \n                                                 AND acc.ACC_HRCY_ID = (SELECT l.ACC_HRCY_ID \n                                                                          FROM xc_gl_ledgers l \n                                                                         WHERE l.ACC_HRCY_ID = acc.ACC_HRCY_ID \n                                                                           AND l.LEDGER_ID = a.LEDGER_ID))\n            WHEN {?adjType?} = 'LJZJTXKM' THEN (SELECT acc.ACC_NAME \n                                               FROM xc_gl_accounts acc \n                                              WHERE acc.ACC_CODE = a.LJZJTXKM \n                                                AND acc.ACC_HRCY_ID = (SELECT l.ACC_HRCY_ID \n\t\t                                                         FROM xc_gl_ledgers l \n\t\t                                                        WHERE l.ACC_HRCY_ID = acc.ACC_HRCY_ID \n\t\t                                                          AND l.LEDGER_ID = a.LEDGER_ID))\n            WHEN {?adjType?} = 'CZL' THEN a.CZL\n            WHEN {?adjType?} = 'LJZJ_LJTX' THEN a.LJZJ_LJTX\n            WHEN {?adjType?} = 'CBZX' THEN (SELECT d.DEPT_NAME FROM xip_pub_depts d WHERE d.DEPT_ID = a.CBZX)\n            WHEN {?adjType?} = 'JZZB' THEN a.JZZB END OLD_VALUE_VIEW\n  FROM xc_fa_additions a\n WHERE a.ASSET_ID = {?asSetId?}","type":"object","itemId":"dataprovider"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}