{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"itemId":"module"},"children":[{"configs":{"sql":"select t.LEDGER_ID ,\n       t.LEDGER_NAME ,\n       t.CURRENCY_CODE ,\n       (select p.CURRENCY_NAME from xc_gl_currencies p WHERE p.CURRENCY_CODE =t.CURRENCY_CODE) as \"CURRENCY_NAME\",\n       (select p.ORG_NAME from  xip_pub_orgs p where p.ORG_ID = t.ORG_ID) AS \"ORG_NAME\",\n       t.ORG_ID\nfrom xc_gl_ledgers t \nwhere EXISTS (SELECT 1\n          FROM XC_GL_LD_SEC A\n         WHERE t.LEDGER_ID = A.LEDGER_ID\n           AND A.USER_ID = {?XIP.userId?}\n           AND A.ROLE_ID = {?XIP.roleId?})\n    AND t.IS_VALID = 'Y'","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}