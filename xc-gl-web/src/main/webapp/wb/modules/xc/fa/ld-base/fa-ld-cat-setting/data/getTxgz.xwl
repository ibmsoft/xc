{"inframe":false,"title":"获取摊销规则","hidden":false,"roles":{"default":1},"children":[{"configs":{"initScript":"var lang = request.getSession().getAttribute(\"XzSessionVars\").get('language');\nrequest.setAttribute(\"language\",lang);","itemId":"module"},"children":[{"configs":{"sql":"SELECT l.VAL_CODE,\n       case when t.VAL_NAME is null or t.VAL_NAME = '' then l.VAL_NAME else t.VAL_NAME end VAL_NAME\n  FROM xip_pub_val_set_dtl l\n  LEFT JOIN XIP_PUB_VAL_SET_DTL_LANG t \n         ON l.VAL_SET_CODE = t.VAL_SET_CODE  \n        AND l.VAL_CODE = t.VAL_CODE \n        AND t.VAL_SET_LANG = '{#language#}'\n WHERE l.VAL_SET_CODE = 'XC_FA_AMORTIZATION_RULE'","itemId":"dataprovider"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}