{"inframe":false,"title":"查询账薄中对应的全局会计期","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT p.PERIOD_CODE,\n       lp.START_DATE,\n       lp.END_DATE,\n       date_sub(lp.START_DATE,interval 1 day) LAST_DATE,\n       CASE WHEN DATE_FORMAT(CURDATE(), '%Y-%m') = p.PERIOD_CODE THEN 1 ELSE 0 END IN_PERIOD\n  FROM xc_fa_ld_periods p\n  left join xc_gl_period lp on p.PERIOD_CODE = lp.PERIOD_CODE\n WHERE p.LEDGER_ID = {?LEDGER_ID?}\n   AND p.STATUS = '1'","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}