{"inframe":false,"title":"查询资产","hidden":false,"roles":{"default":1},"children":[{"configs":{"initScript":"var lang = request.getSession().getAttribute(\"XzSessionVars\").get('language');\nrequest.setAttribute(\"language\",lang);","itemId":"module"},"children":[{"configs":{"sql":"select s.*,\n       (SELECT d.DEPT_NAME FROM xip_pub_depts d WHERE d.DEPT_ID = s.CBZX) CBZX_NAME,\n       '' VOUCHER_FLAG,\n       (SELECT acc.ACC_NAME \n           FROM xc_gl_accounts acc \n          WHERE acc.ACC_CODE = s.ZCKM \n            AND acc.ACC_HRCY_ID = (SELECT l.ACC_HRCY_ID \n                                     FROM xc_gl_ledgers l \n                                    WHERE l.ACC_HRCY_ID = acc.ACC_HRCY_ID \n                                      AND l.LEDGER_ID = s.LEDGER_ID)) ZCKM_VIEW,\n        (SELECT acc.ACC_NAME \n           FROM xc_gl_accounts acc \n          WHERE acc.ACC_CODE = s.LJZJTXKM \n            AND acc.ACC_HRCY_ID = (SELECT l.ACC_HRCY_ID \n                             FROM xc_gl_ledgers l \n                            WHERE l.ACC_HRCY_ID = acc.ACC_HRCY_ID \n                              AND l.LEDGER_ID = s.LEDGER_ID)) LJZJTXKM_VIEW,                             \n        (SELECT acc.ACC_NAME \n           FROM xc_gl_accounts acc \n          WHERE acc.ACC_CODE = s.JZZBKM \n            AND acc.ACC_HRCY_ID = (SELECT l.ACC_HRCY_ID \n                                     FROM xc_gl_ledgers l \n                                    WHERE l.ACC_HRCY_ID = acc.ACC_HRCY_ID \n                                      AND l.LEDGER_ID = s.LEDGER_ID)) JZZBKM_VIEW,                             \n        (SELECT acc.ACC_NAME \n           FROM xc_gl_accounts acc \n          WHERE acc.ACC_CODE = s.FYKM \n            AND acc.ACC_HRCY_ID = (SELECT l.ACC_HRCY_ID \n                                     FROM xc_gl_ledgers l \n                                    WHERE l.ACC_HRCY_ID = acc.ACC_HRCY_ID \n                                      AND l.LEDGER_ID = s.LEDGER_ID)) FYKM_VIEW,\n       (SELECT m.METHOD_NAME FROM xc_fa_depr_methods m WHERE m.METHOD_CODE = s.ZJFF) METHOD_NAME,\n       (CASE WHEN s.SFZJ = 'Y' THEN '是' ELSE '否' END) SFZJ_VIEW,\n       (SELECT u.USE_NAME FROM xc_fa_uses u WHERE u.USE_CODE = s.ZCYT) USE_NAME,\n       (SELECT c1.CAT_NAME FROM xc_fa_cats c1 WHERE c1.CAT_CODE = s.CAT_CODE) CAT_NAME,\n       (SELECT l.LEDGER_NAME FROM xc_gl_ledgers l WHERE l.LEDGER_ID = s.LEDGER_ID) LEDGER_NAME,\n       (SELECT case when t.VAL_NAME is null or t.VAL_NAME = '' then l.VAL_NAME else t.VAL_NAME end VAL_NAME\n          FROM xip_pub_val_set_dtl l\n          LEFT JOIN XIP_PUB_VAL_SET_DTL_LANG t \n                 ON l.VAL_SET_CODE = t.VAL_SET_CODE  \n                AND l.VAL_CODE = t.VAL_CODE \n                AND t.VAL_SET_LANG = '{#language#}'\n         WHERE l.VAL_SET_CODE = 'XC_FA_PROPERTY'\n           AND l.VAL_CODE = s.FA_PROPERTY) FA_PROPERTY_VIEW\n  from xc_fa_additions s\n where {#whereSql#}","itemId":"dataprovider"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}