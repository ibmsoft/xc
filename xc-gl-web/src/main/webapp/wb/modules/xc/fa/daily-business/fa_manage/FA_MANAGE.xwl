                  app.FORM_XWL.SFZJ.setValue(false);\n                }\n              }else{\n                app.FORM_XWL.SFZJ.setValue(false);\n              }\n\n              if(app.FORM_XWL.SFZJ.getValue()==true){\n                app.FORM_XWL.enableZj();\n              }else{\n                app.FORM_XWL.disableZj();\n              }\n\n              if(!Ext.isEmpty(record.get('ZJFF'))){\n                app.FORM_XWL.ZJFF.getStore().load(function(){\n                  app.FORM_XWL.ZJFF.setValue(record.get('ZJFF'));\n                });\n              }else{\n                app.FORM_XWL.SFZJ.setValue('');\n                app.FORM_XWL.SFZJ.setRawValue('');\n              }\n\n              if(!Ext.isEmpty(record.get('TXGZ'))){\n                app.FORM_XWL.TXGZ.getStore().load(function(){\n                  app.FORM_XWL.TXGZ.setValue(record.get('TXGZ'));\n                });\n              }else{\n                app.FORM_XWL.TXGZ.setValue('');\n                app.FORM_XWL.TXGZ.setRawValue('');\n              }\n\n              if(!Ext.isEmpty(record.get('CZL'))){\n                app.FORM_XWL.CZL.setValue(record.get('CZL'));\n              }else{\n                app.FORM_XWL.CZL.setValue(0);\n              }\n\n              if(!Ext.isEmpty(record.get('SYNX'))){\n                var _allMonth = Number(record.get('SYNX'));\n                if(_allMonth < 12){\n                  app.FORM_XWL.SYNX_M.setValue(_allMonth);\n                }else{\n                  app.FORM_XWL.SYNX_Y.setValue(parseInt(_allMonth/12));\n                  app.FORM_XWL.SYNX_M.setValue(_allMonth%12);\n                }\n              }else{\n                app.FORM_XWL.SYNX_Y.setValue('');\n                app.FORM_XWL.SYNX_M.setValue('');\n              }\n\n              app.FORM_XWL.ZCKM_XWL.QUERY_LEDGER_ID.setValue(record.get('LEDGER_ID'));\n              if(!Ext.isEmpty(record.get('YZKM')) && !Ext.isEmpty(record.get('YZKM_VIEW'))){\n                app.FORM_XWL.ZCKM_XWL.ACC_C