ableZj();\n    }\n  });\n\n  app.FORM_XWL.ZCKM_XWL.QUERY_LEDGER_ID.setValue(app.fa_ld.getValue());\n  app.FORM_XWL.LJZJTXKM_XWL.QUERY_LEDGER_ID.setValue(app.fa_ld.getValue());\n  app.FORM_XWL.JZZBKM_XWL.QUERY_LEDGER_ID.setValue(app.fa_ld.getValue());\n  app.FORM_XWL.FYKM_XWL.QUERY_LEDGER_ID.setValue(app.fa_ld.getValue());\n}\n"},"children":[],"expanded":false,"type":"button"},{"configs":{"text":"{#edit#}","itemId":"editBtn","iconCls":"file_edit_icon"},"events":{"click":"var rec = app.grid1.getSelection()[0];\nif (!rec) {\n  Wb.toast('{#selRecord#}');\n  return;\n}\nWb.reset(app.FORM_XWL.faPanel);\nopType = 'update';\nvar win = app.win1; //app._win指向配置对象而非实例\nwin.show();\nwin.setIconCls('record_edit_icon');\nWb.setTitle(win, '{#edit#}');\n\nvar _flag = true;\nExt.Msg.wait('');\nExt.Ajax.request({\n  url : 'm?xwl=xc/fa/common/FA_INFO_PANEL/data/checkJtzj',\n  method: 'POST',\n  async:false,\n  params:{assetId: rec.data.ASSET_ID},\n  success: function(response,options){\n    Ext.Msg.hide();\n    var resp = Ext.JSON.decode(response.responseText);\n\n    //是否计提 看计提期间，没有值为未计提折旧，有值的话 是否和当前打开期间是否一致 一样的话 就是已经计提\n    if(Ext.isEmpty(resp.JTQJ) || resp.JTQJ=='0000-00' || resp.JTQJ!=resp.CURRENT_PERIOD){\n      _flag = false;\n    }\n  },\n  failure: function(response,options){\n    Ext.Msg.hide();\n    Wb.error('');\n  }\n});\n\nExt.Msg.wait('');\nExt.Ajax.request({\n  url : 'm?xwl=xc/fa/common/FA_INFO_PANEL/data/checkIsSplit',\n  method: 'POST',\n  async:false,\n  params:{assetId: rec.data.ASSET_ID},\n  success: function(response,options){\n    Ext.Msg.hide();\n    var resp = Ext.JSON.decode(response.responseText);\n\n    //是否拆分\n    if(resp.CNT > 0){\n      _flag = true;\n    }\n  },\n  failure: function(response,options){\n    Ext.Msg.hide();\n    Wb.error('');\n  }\n});\n\napp.FORM_XWL.IS_JTZJ.setValue(_flag);\n\nif(_flag==true){\n  app.FORM_XWL.operateJt(true);\n}else{\n  app.FORM_XWL.operateJt(false);\n}\n\n\napp.FORM_XWL.LEDGER.setReadOnly(true);\napp.FORM_XWL.PERIODS.se