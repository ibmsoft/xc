{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"initScript":"var lang = request.getSession().getAttribute(\"XzSessionVars\").get('language');\nrequest.setAttribute(\"language\",lang);","itemId":"module"},"children":[{"configs":{"sql":"select a.*,\n       (select d.DEPT_NAME from xip_pub_depts d where d.DEPT_ID = a.CBZX)CBZX,\n       (select e.EMP_NAME from xip_pub_emps e where e.EMP_ID = (select u.EMP_ID from xip_pub_users u where u.USER_ID = a.BGR)) BGR,\n       (SELECT u.USE_NAME FROM xc_fa_uses u WHERE u.USE_CODE = a.ZCYT) ZCYT,\n       (select DIM_NAME from xc_gl_dimensions d where d.DIM_CODE = a.DW) DW,\n       (SELECT VAL_NAME FROM XIP_PUB_VAL_SET_DTL WHERE VAL_SET_CODE = 'YN' AND VAL_CODE = a.SFZJ)SFZJ,\n       (SELECT METHOD_NAME FROM xc_fa_depr_methods WHERE  METHOD_CODE = a.ZJFF)ZJFF,\n       (SELECT case when t.VAL_NAME is null or t.VAL_NAME = '' then l.VAL_NAME else t.VAL_NAME end VAL_NAME\n          FROM xip_pub_val_set_dtl l\n          LEFT JOIN XIP_PUB_VAL_SET_DTL_LANG t \n            ON l.VAL_SET_CODE = t.VAL_SET_CODE  \n           AND l.VAL_CODE = t.VAL_CODE \n           AND t.VAL_SET_LANG = '{#language#}'\n         WHERE l.VAL_SET_CODE = 'XC_FA_AMORTIZATION_RULE'\n           AND l.VAL_CODE = TXGZ) TXGZ\n  from xc_fa_additions a","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}