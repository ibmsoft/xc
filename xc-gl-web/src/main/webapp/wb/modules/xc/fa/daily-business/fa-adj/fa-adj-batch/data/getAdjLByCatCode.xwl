{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"itemId":"module"},"children":[{"configs":{"sql":"SELECT j.*,\n       '' VOUCHER_FLAG,\n       xfa.ZCBM,\n       xfa.ZCMC,\n       {#oldValue#}\n       CASE WHEN {?adjType?} = 'ZJFF' THEN (SELECT m.METHOD_NAME FROM xc_fa_depr_methods m WHERE m.METHOD_CODE = xfa.ZJFF)\n            WHEN {?adjType?} = 'ZCYT' THEN (SELECT u.USE_NAME FROM xc_fa_uses u WHERE u.USE_CODE = xfa.ZCYT)\n            WHEN {?adjType?} = 'CAT_CODE' THEN (SELECT c.CAT_NAME FROM xc_fa_cats c WHERE c.CAT_CODE = xfa.CAT_CODE)\n            WHEN {?adjType?} = 'ZCKM' THEN (SELECT acc.ACC_NAME \n                                              FROM xc_gl_accounts acc \n                                             WHERE acc.ACC_CODE = xfa.ZCKM \n                                               AND acc.ACC_HRCY_ID = (SELECT l.ACC_HRCY_ID \n                                                                        FROM xc_gl_ledgers l \n                                                                       WHERE l.ACC_HRCY_ID = acc.ACC_HRCY_ID \n                                                                         AND l.LEDGER_ID = xfa.LEDGER_ID))\n            WHEN {?adjType?} = 'YZ' THEN xfa.YZ\n            WHEN {?adjType?} = 'SYNX' THEN xfa.SYNX\n            WHEN {?adjType?} = 'SFZJ' THEN (CASE WHEN xfa.SFZJ = 'Y' THEN '是' ELSE '否' END)\n            WHEN {?adjType?} = 'JZZBKM' THEN (SELECT acc.ACC_NAME \n                                                FROM xc_gl_accounts acc \n                                               WHERE acc.ACC_CODE = xfa.JZZBKM \n                                                 AND acc.ACC_HRCY_ID = (SELECT l.ACC_HRCY_ID \n                                                                          FROM xc_gl_ledgers l \n                                                                         WHERE l.ACC_HRCY_ID = acc.ACC_HRCY_ID \n                                                                           AND l.LEDGER_ID = xfa.LEDGER_ID))\n            WHEN {?adjType?} = 'LJZJTXKM' THEN (SELECT acc.ACC_NAME \n                                               FROM xc_gl_accounts acc \n                                              WHERE acc.ACC_CODE = xfa.LJZJTXKM \n                                                AND acc.ACC_HRCY_ID = (SELECT l.ACC_HRCY_ID \n\t\t                                                         FROM xc_gl_ledgers l \n\t\t                                                        WHERE l.ACC_HRCY_ID = acc.ACC_HRCY_ID \n\t\t                                                          AND l.LEDGER_ID = xfa.LEDGER_ID))\n            WHEN {?adjType?} = 'CZL' THEN xfa.CZL\n            WHEN {?adjType?} = 'LJZJ_LJTX' THEN xfa.LJZJ_LJTX\n            WHEN {?adjType?} = 'JZZB' THEN xfa.JZZB END OLD_VAL_VIEW,\n       '' NEW_VAL_VIEW,\n       {?adjType?} ADJ_TYPE,\n       (SELECT t.TRANS_DESC \n          FROM xc_fa_trans_type t\n         WHERE t.TRANS_CODE = {?adjType?}) ADJ_TYPE_VIEW,\n       '111' DFKM_VIEW\n  FROM xc_fa_additions xfa\n  LEFT JOIN  xc_fa_adj_l j ON xfa.ASSET_ID = j.ASSET_ID\n WHERE xfa.ASSET_ID IN (SELECT xfas.ASSET_ID FROM xc_fa_additions xfas WHERE xfas.CAT_CODE = {?catCode?})","itemId":"dataprovider"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}