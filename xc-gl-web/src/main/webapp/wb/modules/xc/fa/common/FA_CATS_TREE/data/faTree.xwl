{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"SELECT a.CAT_NAME AS \"text\",\n\t   a.CAT_ID,       \n       a.CAT_CODE,\n       a.CAT_NAME,\n       a.UP_CODE,\n       ua.CAT_NAME UP_NAME,\n       a.IS_LEAF,\n       a.FA_PROPERTY,\n       (SELECT l.VAL_NAME \n          FROM XIP_PUB_VAL_SET_DTL l\n         WHERE l.VAL_SET_CODE = 'XC_FA_PROPERTY' \n           AND l.VAL_CODE = a.FA_PROPERTY) FA_PRO_NAME,\n       CASE WHEN (SELECT COUNT(*) \n                    FROM XC_FA_CATS b \n                   WHERE b.UP_CODE = a.CAT_CODE)>0 \n                      OR a.IS_LEAF = '0'\n            THEN 'false' ELSE 'true' \n             END AS \"leaf\"\n   FROM XC_FA_CATS a\n   LEFT JOIN XC_FA_CATS ua ON ua.CAT_CODE = a.UP_CODE\n  WHERE a.UP_CODE = {?UP_CODE?} \n    AND (EXISTS (SELECT 1 \n                  FROM xc_fa_ld_cat_settings s\n                 WHERE s.CAT_CODE = a.CAT_CODE\n                   AND s.LEDGER_ID = {?LEDGER_ID?}) OR \n         (SELECT COUNT(*) \n            FROM xc_fa_ld_cat_settings s1, XC_FA_CATS a1\n           WHERE s1.CAT_CODE = a1.CAT_CODE\n             AND s1.LEDGER_ID = {?LEDGER_ID?}\n             AND a1.UP_CODE = a.CAT_CODE) > 0 OR\n         (SELECT COUNT(*)\n            FROM xc_fa_ld_cat_settings s1, XC_FA_CATS a1\n           WHERE s1.CAT_CODE = a1.CAT_CODE\n             AND s1.LEDGER_ID = {?LEDGER_ID?}\n             AND a1.UP_CODE IN (SELECT a2.CAT_CODE \n                                 FROM XC_FA_CATS a2\n                                WHERE a2.UP_CODE = a.CAT_CODE)) > 0)\n {#whereSql#}","type":"tree","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}