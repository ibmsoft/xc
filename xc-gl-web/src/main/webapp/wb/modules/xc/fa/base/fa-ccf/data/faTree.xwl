{"inframe":false,"title":"","hidden":false,"roles":{"default":1},"children":[{"configs":{"loginRequired":"false","itemId":"module"},"children":[{"configs":{"sql":"select CAT_NAME as \"text\",\n       CAT_ID,\n       CAT_CODE,\n       CAT_NAME,\n       UP_CODE,\n       UP_NAME,\n       IS_LEAF,\n       FA_PROPERTY,\n       (SELECT VAL_NAME \n          FROM XIP_PUB_VAL_SET_DTL \n         WHERE VAL_SET_CODE = 'XC_FA_PROPERTY' \n           AND VAL_CODE = a.FA_PROPERTY) FA_PRO_NAME,\n       case when (select count(*) \n                    from XC_FA_CATS b \n                   where b.UP_CODE=a.CAT_CODE)>0 \n                      or a.IS_LEAF='0'\n            then 'false' else 'true' \n            end as \"leaf\"\n  from XC_FA_CATS a \n  left join (SELECT CAT_NAME UP_NAME,CAT_CODE CODE FROM XC_FA_CATS) U ON U.CODE = a.UP_CODE\n where UP_CODE = {?UP_CODE?} \n   and CAT_CODE LIKE '%{#fa_pro#}%' \n   AND (a.CAT_NAME like '%{#codeName#}%' OR \n         (SELECT COUNT(*) \n            FROM XC_FA_CATS a1\n           WHERE a1.UP_CODE = a.CAT_CODE\n             AND a1.CAT_NAME like '%{#codeName#}%') > 0 OR\n         (SELECT COUNT(*)\n            FROM XC_FA_CATS a1\n           WHERE a1.CAT_NAME like '%{#codeName#}%'\n             AND a1.UP_CODE IN (SELECT a2.CAT_CODE \n                                 FROM XC_FA_CATS a2\n                                WHERE a2.UP_CODE = a.CAT_CODE)) > 0)\n   {#whereSql#}\n\n","type":"tree","itemId":"dataprovider1"},"children":[],"expanded":false,"type":"dataprovider"}],"expanded":true,"type":"module"}],"pageLink":"","iconCls":""}